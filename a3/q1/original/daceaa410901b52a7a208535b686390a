$('.listing .comments').each(function(){
  var count = $(this).find('span').text();
  var span = $(this).find('span');
  var link =  $(this).find('a');
  $(this).html('')
  link.text('');
  span.text('');
  link.append(span);
  var comment = $('<div class="comment">')
  comment.html('Comentários <span>' + count + '</span>');
  span.append(comment);
  $(this).append(link);
})
</script>


