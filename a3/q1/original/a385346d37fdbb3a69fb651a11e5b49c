<script type="text/javascript">
<!--// 



function fsStore(maxNum){
	var fsStoreCont = document.getElementById("fs-page-sports-store-container"); // get a handle tot he store container
	var fsStoreUl = fsStoreCont.getElementsByTagName("ul");	// get a handle to the store list
	var fsStoreLi = fsStoreUl[0].getElementsByTagName("li"); // get a handle to the list items
	var rNumHolder = []; // holder exemptions
	for(var i=0; i<maxNum; i++){ // run items to show
		var randomNumber = randomizeMe(this, {maxNumber: fsStoreLi.length, exempt: rNumHolder.toString()}); // select a random number from the number of items minus the ones already selected
		fsStoreLi[randomNumber].className += " on orig"; // turn the item on
		rNumHolder[i] = randomNumber; // store the random number
	}		
	fSort = function(a,b){ // descending sort
		return b-a; // return the new sort order
	}
	lastItem = rNumHolder.sort(fSort); // sort the holder (put the largest item first
	fsStoreLi[lastItem[0]].className += " last"; // set the largest item index to "last" for the last item.
}


FsStore ={};

FsStore = new function () {
	mThis = this; 

	mThis.init = function(maxNum) {
		var doc = document;
		var fsStoreLi = FsUtils.tableToArray(doc.getElementById("fs-store-table"));
		var fsStoryDisplay = new Array();
		var rNumHolder = new Array();
		var randomNumber = 0;
		var isStory = false; 
		var template; 
		var storyTemplate = '<li class="on"><a href="{itemURL}" title="{itemDescription}" target="_blank"><img src="{imageSrc}" alt="{itemDescription}"></a><span class="price-container"><a href="{itemURL}" title="{itemDescription}" target="_blank" class="description">{itemDescription}</a><span class="sub-price-module"><span class="label">OUR PRICE</span><span class="price"><sup>$</sup>{price}</span></span><a href="{itemURL}" title="{itemDescription}" target="_blank" class="buy-now">BUY NOW</a></span></li>';
		var defaultTemplate = '<li class="on"><a href="{itemURL}" title="{itemDescription}" target="_blank"><img src="{imageSrc}" alt="{itemDescription}"></a><p class="price-container"><a href="{itemURL}" title="{itemDescription}" target="_blank" class="description">{itemDescription}</a><span class="label">OUR PRICE</span><span class="price"><sup>$</sup>{price}</span><a href="{itemURL}" title="{itemDescription}" target="_blank" class="buy-now">BUY NOW</a></p></li>';
       	var i = 0;
		while ( i < maxNum){ // run items to show // while < 3 
		var randomNumber = Math.floor(Math.random() * (fsStoreLi.length)); // select a random number from the number of items minus the ones already selected
			 //console.log (rNumHolder);  	
			if (!rNumHolder.toString().match(randomNumber) ) // number has been selected
			{
				rNumHolder.push(randomNumber);
				fsStoryDisplay.push(fsStoreLi[randomNumber]);
				i++;
			} else if ( fsStoreLi.length < maxNum) {
				i++; 
			}
			                      
		}	
	
	if (location.href.match('/story/') != null ) 
	{
		template = storyTemplate; 
		isStory = true; 
	}
	else 
	{ 
		template = defaultTemplate; 
	}
	
	 var isShopRightRail = (typeof fsLondonOlympics != 'undefined') ? true : false ;

	 if(isShopRightRail)
     {
	     template = storyTemplate;
	     $(".fs-sports-store .sprite span").html("SHOP");
     } 
		
	if ( isStory || isShopRightRail)
		fsStoryDisplay[0].imageSrc = fsStoryDisplay[0].imageSrc.replace('w=100','w=220'); // make the first image larger
	
	
	mThis.parseTemplate = function (template, data, limit){
		var checME = /\{\w*\}/g; //setup reg expression	to find fields in template... {...}		
		var replaceMe = template.match(checME); // find all of the fields in teh template						
		var holder=''; // holder for returning data
		if(data){ // if there's data
			var limit = (!limit)?data.length :limit; // set the limit on how many to run
			for(var i=0; i<limit; i++){ // run thrugh the limit
				if(data[i]){ // if there's data in the set
					if(replaceMe){ // if there's items to place
						var newPTemp = template; // set a new template to append changes too
						for(var r=0, rLen = replaceMe.length; r<rLen; r++){	// go through the list of items to replace 
							newPTemp = newPTemp.replace(replaceMe[r],data[i][replaceMe[r].replace("}","").replace("{","")]).replace("undefined", "0"); // Replace the fields with data
							
						}	
						holder +=newPTemp; // append the good data to the holder
					}
				}
			}
		}
		return holder; // return the parsed template
	}
	
	var storeDataToAppend = mThis.parseTemplate(template, fsStoryDisplay, maxNum);
	if ( isStory || isShopRightRail)
		storeDataToAppend = storeDataToAppend.replace('"on"', '"on featured"'); // set the first item as featured. 
	
	doc.getElementById('fs-store-data').innerHTML = storeDataToAppend;
	doc.getElementById("fs-store-data").childNodes[maxNum - 1].className += " last";
 } 
}


if (document.getElementById("fs-store-table") == null) {
	fsStore(3);	
} else {
	
	FsStore.init(3); 
}

//-->
</script><!--  -->


