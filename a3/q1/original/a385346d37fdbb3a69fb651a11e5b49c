<script language="javascript1.2" type="text/javascript">
	if (imgArray.length > 1){ //get length of image array
		var num = randomizeMe(this, {maxNumber: imgArray.length}); //get random number for displaying div

		for (var i=0; i<imgArray.length; i++){ // loop through all img array elements to hide them
			if (i != num){ // find the array element that matches the array index of the random number
				var element = document.getElementById(imgArray[i]); // get element from array element
				if (element) // marketing promo as per buisness system only support native size, null if image dont have any native size
					element.style.display = 'none'; // set display to none for image element
			}
		}
	}
</script>


