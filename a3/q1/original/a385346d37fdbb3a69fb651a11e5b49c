      function checkUserCookie(cookieName, length) {
                   var allcookies = document.cookie;
                   var cookiePos = allcookies.indexOf(cookieName + "=");
               if (cookiePos != -1) {
                    var start = cookiePos + length;
                    var end = allcookies.indexOf(";", start);
                    if (end == -1) end = allcookies.length;
                    var value = allcookies.substring(start, end);
                    value = unescape(value);
                    return value;
                  }
                    return "";
      }

      function fsLoadUserCookie (){
			try{
				var euid = '';
				var lmd = '';
				var efsuid = '';
				euid = checkUserCookie("pUID", 5);
				lmd = checkUserCookie("pLMD", 5);
				efsuid  = checkUserCookie("pEFSUID", 8);
			}catch(e){/*ignore*/}
      }
