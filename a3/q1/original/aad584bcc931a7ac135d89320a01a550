                $jq(".sbutton.large_green, .sbutton.almost_large_green").click(function(){//Click Here For Tickets Clicked
                    s.linkTrackVars     = "products,events";
                    s.linkTrackEvents   = "event11";
                    s.events            = "event11";
                    s.tl(this,"o","Click Here For Tickets Clicked");
                });

                $jq(document).delegate("#add-to-cart, .add-to-cart, .book-hotel", "click", function(){//Add to Cart Clicked
                    //Set event cookie on click
                    document.cookie='s_addCart=true; path=/';
                    vars                = s_gi(s_account);
                    s.linkTrackVars     = "products,events";
                    s.linkTrackEvents   = "scAdd,scOpen";
                    s.events            = "scAdd,scOpen";
                    s.tl(this,"o","Add to Cart Clicked");
                });
                $jq(document).delegate(".addthis_toolbox a", "click", function(){//Social Bookmark Selected - Main
                    bookmark = null;
                    if ($jq(this).attr("title") && $jq(this).attr("title").length > 1){
                        bookmark = $jq(this).attr("title");
                    }
                    if (bookmark != null){
                        vars                = s_gi(s_account);
                        s.linkTrackVars     = "events,products,eVar21";
                        s.linkTrackEvents   = "event3";
                        s.events            = "event3";
                        s.eVar21            = bookmark;
                        s.tl(this,"o","Social Media Shared Product / Article");
                    }
                    return 0;
                });
                $jq(document).delegate("a.at_item", "click", function(){//Social Bookmark Selected - Sub
                    bName               = $jq(this).attr("id").replace("atic_", "");
                    vars                = s_gi(s_account);
                    s.linkTrackVars     = "events,products,eVar21";
                    s.linkTrackEvents   = "event3";
                    s.events            = "event3";
                    s.eVar21            = bName;
                    s.tl(this,"o","Social Media Shared Product / Article")
                });
                $jq(document).delegate(".ui-tabs-panel a, .hotel-sort a", "click", function(){//Tab Views - Main
                    sHeader             = $jq(this).html();
                    vars                = s_gi(s_account);
                    s.linkTrackVars     = "events,eVar30";
                    s.linkTrackEvents   = "event5";
                    s.events            = "event5";
                    s.eVar30            =  s.pageName +":"+ sHeader;
                    s.tl(this,"o","Tab View");
                });
                $jq(".hotel-product-info ul.ui-tabs-nav li a, .product-info ul.ui-tabs-nav li a, .tabs ul li a").bind("click", function() {//Tab Views - Product Info
                    sHeader             = $jq(this).html();
                    vars                = s_gi(s_account);
                    s.linkTrackVars     = "events,eVar30";
                    s.linkTrackEvents   = "event5";
                    s.events            = "event5";
                    s.eVar30            =  s.pageName +":"+ sHeader;
                    s.tl(this,"o","Tab View");
                });
                $jq(document).delegate("#newsletter-subscribe", "click", function(){//Newsletter Subscription Selected
                    s.linkTrackVars     = "events";
                    s.linkTrackEvents   = "event8";
                    s.events            = "event8";
                    s.tl(this,"o","Newsletter Subscription")
                });
            });
            //set event
            if (document.cookie.indexOf("s_buyTickets=true") > -1 && document.cookie.indexOf("s_addCart=true") == -1){
                s.linkTrackVars     = "products,events";
                s.linkTrackEvents   = "event10";
                s.events            = "event10";
                s.tl(this,"o","Buy Tickets/Buy Now Clicked")
                //cleanup
                document.cookie='s_buyTickets=true; expires=Fri, 27 Jul 2001 02:47:11 UTC; path=/';
                document.cookie='s_addCart=true; expires=Fri, 27 Jul 2001 02:47:11 UTC; path=/';
                delete s.linkTrackVars;
                delete s.linkTrackEvents;
                delete s.events;
                delete s.products;
            }

            //Capture Page
            s.t()
        }
        //]]></script>
