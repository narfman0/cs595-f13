                        $fontLoader.append($loader);
                        loaders[fonts[i]] = { fallbackWidth: $loader.width(), $el: $loader, loaded: false };
                    }
                    // loader
                    $('head').append($loadFontStyles);
                    var total = fonts.length-1, checker = -1, failsafe = -1;
                    // failsafe
                    failsafe = setTimeout(function() {
                        clearInterval(checker);
                        for( var k in loaders ) {
                            delete loaders[k].$el;
                        }
                        Etsy.Logger.info("font-loader-timeout", loaders);
                    }, 10000);
                    checker = setInterval(function() {
                        for( var k in loaders ) {
                            if ( !loaders[k].loaded && loaders[k].$el.width() !== loaders[k].fallbackWidth ) {
                                loaders[k].loaded = true;
                                total--;
                                if ( total === 0 ) {
                                    clearInterval(checker);
                                    clearTimeout(failsafe);
                                    $('head').append($fontStyles);
                                }
                            }
                        }
                    }, 50);
                    // Set cookie
                    document.cookie = "perf=wf:1; path=/";
                });
