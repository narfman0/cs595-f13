        var _gaq = _gaq || [];

        var googleAnalyticsPremium = {

          sendTransactions: function(transactions) {
              try {
                var transactionList, transaction, itemList, item, i, j;

                if (transactions instanceof Array) {
                    transactionList = transactions;
                } else {
                    transactionList = [transactions];
                }

                for (i = 0; i < transactionList.length; i++) {
                    transaction = transactionList[i];
                    _gaq.push(["premium._addTrans",
                               transaction.orderId, this.replaceNull(transaction.affiliation), transaction.total,
                               this.replaceNull(transaction.tax), this.replaceNull(transaction.shipping), this.replaceNull(transaction.city),
                               this.replaceNull(transaction.state), this.replaceNull(transaction.country)]);

                    if (transaction.items !== undefined) {
                        if (transaction.items instanceof Array) {
                            itemList = transaction.items;
                        } else {
                            itemList = [transaction.items];
                        }

                        for (j = 0; j < itemList.length; j++) {
                            item = itemList[j];
                            _gaq.push(["premium._addItem", transaction.orderId, item.sku, item.name,
                                       this.replaceNull(item.category), item.price, item.quantity]);
                        }
                    }

                    _gaq.push(["premium._trackTrans"]);
                }
              } catch (e) {
                  if (console && console.warn) {
                      console.warn("google analytics script is unavailable: " + e);
                  }
              }
          },

          sendEvents: function(events) {
              try {
                var eventList, event, i;

                if (events instanceof Array) {
                    eventList = events;
                } else {
                    eventList = [events];
                }

                for (i = 0; i < eventList.length; i++) {
                    event = eventList[i];
                    _gaq.push(["premium._trackEvent",
                               event.category, event.action,
                               this.replaceNull(event.label), this.replaceNull(event.value), this.replaceNull(event.nonInteraction)]);
                }
              } catch (e) {
                  if (console && console.warn) {
                      console.warn("google analytics script is unavailable: " + e);
                  }
              }
          },

          replaceNull: function (value) {
              return value === null ? undefined : value;
          }

        };

