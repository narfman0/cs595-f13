    <a class="prev" href="#page${$data.PagerCurrent}" onclick="ModComment.ChangePage(${$data.PagerCurrent - 1}); return false;"
        {{if $data.PagerCurrent == 0}}style="visibility:hidden;"{{/if}}>&laquo; предыдушие</a>

    <a class="next" href="#page${$data.PagerCurrent + 2}" onclick="ModComment.ChangePage(${$data.PagerCurrent + 1}); return false;"
        {{if $data.PagerCurrent >= $data.PagerLast}}style="visibility:hidden;"{{/if}}>следующие &raquo;</a>

    <div class="pages">
        {{if $data.PagerCurrent == 0}}
            <a class="active" href="#page1" onclick="return false;">1</a>
        {{else}}
            <a class="" href="#page1" onclick="ModComment.ChangePage(0); return false;">1</a>
        {{/if}}
        {{if $data.PagerCurrent > 2 && $data.PagerLast > 4}}
            <a class="" href="#" onclick="return false;">...</a>
        {{/if}}

        {{each $data.PagerPages}}
            {{if $data.PagerCurrent == $value}}
                <a class="active" href="#page${$value + 1}" onclick="return false;">${$value + 1}</a>
            {{else}}
                <a class="" href="#page${$value + 1}" onclick="ModComment.ChangePage(${$value}); return false;">${$value + 1}</a>
            {{/if}}
        {{/each}}

        {{if $data.PagerCurrent < ($data.PagerLast - 2) &&  $data.PagerLast > 4}}
            <a class="" href="" onclick="return false;">...</a>
        {{/if}}
        {{if $data.PagerCurrent == $data.PagerLast}}
            <a class="active" href="#page${$data.PagerLast + 1}" onclick="return false;">${$data.PagerLast + 1}</a>
        {{else}}
            <a class="" href="#page${$data.PagerLast + 1}" onclick="ModComment.ChangePage(${$data.PagerLast}); return false;">${$data.PagerLast + 1}</a>
        {{/if}}
    </div>
</div>
