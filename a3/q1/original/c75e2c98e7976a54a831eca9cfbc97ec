        <div class="article-extra-left-1 article-extra">
          <div class="panel-pane pane-views-panes pane-rsn-articles-panel-pane-6" >
  
      
  
  <div class="pane-content">
    <div class="view view-rsn-articles view-id-rsn_articles view-display-id-panel_pane_6 view-dom-id-19a03f1b3e7b71bd1606b96dec255f64">
        
  
  
      <div class="view-content">
        <div class="views-row views-row-1 views-row-odd views-row-first views-row-last">
      
  <div class="views-field views-field-field-person-picture">        <div class="field-content"><a href="http://twitter.com/gonzoCSN"><img typeof="foaf:Image" src="http://www.csnphilly.com/sites/csnphilly/files/gonz_byline13.png" width="275" height="85" /></a></div>  </div>  </div>
