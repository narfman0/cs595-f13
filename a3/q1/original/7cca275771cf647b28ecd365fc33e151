                  <g:plusone size="tall"></g:plusone>
                </div>
                <div style="float:left; padding-left:10px;">
                  <iframe scrolling="no" id="f4d21b727706c2" name="f23d8006901db52" style="border: medium none; overflow: hidden; height: 59px; width: 61px;" class="fb_ltr " src="http://www.facebook.com/plugins/share_button.php?api_key=321829741185643&amp;channel=http%3A%2F%2Fstatic.ak.facebook.com%2Fconnect%2Fxd_arbiter.php%3Fversion%3D9%23cb%3Df1b1d53a023268e%26origin%3Dhttp%3A%2F%2Fwww.siamsport.co.th%252Ffd0a417d11b3cc%26domain%3Dwww.siamsport.co.th%26relation%3Dparent.parent&amp;href=http%3A%2F%2Fwww.siamsport.co.th%2FSport_Football%2F130924_292.html&amp;locale=en_US&amp;sdk=joey&amp;type=box_count"></iframe>
                </div>
