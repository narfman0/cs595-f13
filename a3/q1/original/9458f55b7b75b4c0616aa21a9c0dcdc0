<script type="text/javascript">
    //build slideshow
	$('#gallery_items_18114155').cycle({
		fx: 'scrollHorz', /*fade*/
		speed: 800,
        next: '#gallery_next_18114155',
        prev: '#gallery_prev_18114155',
		timeout: 8000,
        pager: '#gallery_bullets_18114155',
        pagerAnchorBuilder: function paginate(idx, el){
            return '<a class="item" id="item' + idx + '" href="#"  name="lpos=widget[C_5_gallery]&lid=[dot]">&bull;</a>';
        }
        //before: onBefore
	});

    function onBefore(curr,next,opts, fwd){
        //set the slide height
        var $ht = $(this).height();
        $(this).parent().animate({"height": $ht});
    }

</script>
