  <script type="text/javascript">
  //<![CDATA[
  var map;
  var latlng;

  

  function load() {
      var zm;
      
          latlng = new google.maps.LatLng(30.375321, 69.345116);
          zm = 5;
      
      var myOptions = { zoom: zm, center: latlng, mapTypeId: google.maps.MapTypeId.HYBRID };
      map = new google.maps.Map(document.getElementById("map"),
                                myOptions);

    // set the marker icon
var videoIcon = new google.maps.MarkerImage('/images/marker_video.png');
var slideshowIcon = new google.maps.MarkerImage('/images/marker_slideshow.png');
var myIconShadow = new google.maps.MarkerImage('/images/marker_shadow.png');
myIconShadow.anchor = new google.maps.Point(9, 36);
    var pos = new google.maps.LatLng(30.375321,69.345116)
    
    var markerOptions = {  icon:videoIcon, shadow:myIconShadow, position: pos, map:map };


    var marker = new google.maps.Marker(markerOptions);

    var homeControlDiv = document.createElement('div');
    var homeControl = new HomeControl(homeControlDiv, map);
      homeControlDiv.index = 1;
      map.controls[google.maps.ControlPosition.TOP_RIGHT].push(homeControlDiv);
  }

        function HomeControl(controlDiv, map) {
          controlDiv.style.paddingTop = '6px';

          // Set CSS for the control border
          var controlUI = document.createElement('div');
          controlUI.style.backgroundColor = 'white';
          controlUI.style.borderStyle = 'solid';
          controlUI.style.borderWidth = '2px';
          controlUI.style.cursor = 'pointer';
          controlUI.style.textAlign = 'center';
          controlUI.title = 'Click to go to the default world view';
          controlDiv.appendChild(controlUI);

          // Set CSS for the control interior
          var controlText = document.createElement('div');
          controlText.style.fontFamily = 'Arial,sans-serif';
          controlText.style.fontSize = '12px';
          controlText.style.paddingLeft = '4px';
          controlText.style.paddingRight = '4px';
          controlText.innerHTML = 'Reset view';
          controlUI.appendChild(controlText);

          google.maps.event.addDomListener(controlUI, 'click', function() {
              map.setCenter(new google.maps.LatLng(30.375321, 69.345116)); map.setZoom(5);
              });
        }

  function toggle_content() {
    d = document.getElementById('t_button');
    if(d.checked){
      document.getElementById('search_box').style.display='block';
      document.getElementById('browse1_box').style.display='none';
    } else {
      document.getElementById('search_box').style.display='none';
      document.getElementById('browse1_box').style.display='block';
    }
  }


  addLoadEvent(function(){toggle_content();});
  //]]>
  </script>

