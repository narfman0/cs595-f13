    function showState() {
      var box=document.getElementById('state_id');
      var box2=document.getElementById('country_id');
      if(box[box.selectedIndex].value!='') {
        showAddress(box2.options[box2.selectedIndex].text  +','+ box.options[box.selectedIndex].text,zls[box2.options[box2.selectedIndex].value]+2);
      }
      return false;
    }
