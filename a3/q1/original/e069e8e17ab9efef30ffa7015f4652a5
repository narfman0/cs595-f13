				<script type="text/javascript">
			
					function ValidateLogin() {					    
					    var username = $("input[id$='UserName']").val(); var password = $("input[id$='Password']").val(); var regExpression = /^\s*[\w\-\+_]+(\.[\w\-\+_]+)*\@[\w\-\+_]+\.[\w\-\+_]+(\.[\w\-\+_]+)*\s*$/; var errorMsgSelector = "span[id$='LoginErrorMsg']"; var regex = new RegExp(regExpression);
						if (username.length == 0)
						{ $(errorMsgSelector).html(" <img src=\"http://www1.ibdcd.com/images/icons/errorIco.gif\" alt=\"error\" /> Email is a required field.").show();$("input[id$='UserName']").focus();return false; }						
						if (password.length == 0)
						{ $(errorMsgSelector).html(" <img src=\"http://www1.ibdcd.com/images/icons/errorIco.gif\" alt=\"error\" /> Password is a required field.").show();$("input[id$='Password']").focus();return false; }						
						if (!regex.test(username))
						{ $(errorMsgSelector).html(" <img src=\"http://www1.ibdcd.com/images/icons/errorIco.gif\" alt=\"error\" /> Invalid email address format: name@domain.com.").show();$("input[id$='UserName']").focus();return false; }
						$(errorMsgSelector).hide();$("a[id$='LoginButton']").hide();$("#divSignInLoading").show();
						Won.ICom.Code.Services.SiteAjaxService.MemberSingIn(username, password, $("#RememberMe").is(':checked'), onLoginSignInComplete, onLoginSignInError);
						return false;
					}

					function onLoginSignInComplete(result) {
					    if (result == "AUTOGEN") {
					        var domain = Won.ICom.Code.Services.SiteAjaxService.GetSecureURL(onAutoGenComplete);
					        return;
					    }
					    else if (result == 'SOK') {
					        var url = window.location.href.toString();
						    if (url.toLowerCase().indexOf("myibd/forgotpassword.aspx") > -1) {
						        window.location = "/default.aspx";
						    }
						    else if (url.toLowerCase().indexOf("default.htm") > -1) {
						        window.location = "/default.aspx";
                            }
					        else {
					            window.location.reload(true);
					        }
						}
						else {
							var errorMsgSelector = "span[id$='LoginErrorMsg']";
							$(errorMsgSelector).html(" <img src=\"http://www1.ibdcd.com/images/icons/errorIco.gif\" alt=\"error\" /> Email address or password is incorrect.").show();
							$("#divSignInLoading").hide();$("a[id$='LoginButton']").show();
						}
					}
					
					function onAutoGenComplete(result) {
					    var url = result + "Secure/MyIBD/UpdatePassword.aspx?src=AUTOGEN";
					    window.location = url;
					}
					
					function onLoginSignInError(result) {
						var errorMsgSelector = "span[id$='LoginErrorMsg']";
						$(errorMsgSelector).html(" <img src=\"http://www1.ibdcd.com/images/icons/errorIco.gif\" alt=\"error\" /> Email address or password is incorrect.").show();
						$("#divSignInLoading").hide();$("a[id$='LoginButton']").show();
					}
				</script>
			</fieldset>
			
		
