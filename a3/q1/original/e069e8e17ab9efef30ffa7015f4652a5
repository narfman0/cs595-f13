    //script for articles
    function MPCheckboxClick(chkBox, changeId, classChg1, classChg2) {
        var chkState = chkBox.checked;
        $("#" + changeId + " input[type='checkbox']").attr("checked", "");
        if (chkState) {
            chkBox.checked = true;
            $("#" + changeId + " li").hide();
            $("#" + changeId + " li." + classChg1).show();
            if (typeof (classChg2) != 'undefined') {
                $("#" + changeId + " li." + classChg2).show();
            }
        }
        else {
            chkBox.checked = true;
        }
    }
</script>

