<script type="text/javascript">
function ShowRegister() { $('#register-overlay').jqm({ modal: false }).jqmShow(); }
function HideRegister() { $('#register-overlay').jqm().jqmHide(); }
function RegisterLink(button) {
	if (button.id == 'register1'){$("#registerlnk-disabled").hide();$("#registerlnk-lightbox").hide();$("#registerlnk-activate").show();}
	else if (button.id == 'register2'){$("#registerlnk-disabled").hide();$("#registerlnk-activate").hide();$("#registerlnk-lightbox").show();}
}
var forceShowLogin = false;
if (forceShowLogin || document.cookie.indexOf('ibdPV') > -1) {
    $("div[id$='signInBlock']").show();
    $("div[id$='regBenefitsBlock']").hide();
}
else {
    $("div[id$='signInBlock']").hide();
    $("div[id$='regBenefitsBlock']").show();
    AddBenefitsCarousel();
}
</script>

