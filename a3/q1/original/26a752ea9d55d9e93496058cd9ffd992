<script type="text/javascript">
	$(document).ready(function(){		
		$("#showNormas").colorbox({width:"750px", height:"600px", inline:true, href:"#normas"});
		$("#showNormas").click(function(){
			$.get("/getNormas.html", function (data) {
				$("#normas").html(data);
			});
		});
		
		$("#showProteccion").colorbox({width:"750px", height:"600px", inline:true, href:"#proteccion"});
		$("#showProteccion").hover(function(){
			if($(this).hasClass("getProteccion")){
				$.get("/getProteccion.html", function (data) {
					$("#proteccion").html(data);
				});
			}
			$(this).removeClass("getProteccion");
		});
		
	});
</script>

