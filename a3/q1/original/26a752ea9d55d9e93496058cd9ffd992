<script>
	var d_text = "Buscar...";
	jQuery("#search_text").val(d_text);
	jQuery("#search_text").focus(function () { if(jQuery(this).val() == d_text) jQuery(this).val(""); });
	jQuery("#search_text").blur(function () { if(jQuery(this).val() == "") jQuery(this).val(d_text); });
	function submitSearch(){
		if(jQuery("#search_text").val() == d_text){
			window.open('http://buscador.clarin.com?filter=content_section:Espectáculos'); 
			return false;					
		}
		else{
			window.open('http://buscador.clarin.com/' + encodeURIComponent(document.forms['form_header_search'].text.value) + "?filter=content_section:Espectáculos"); 
			return false;
		}
	}

		headerDate = function (jObj, url) {
			jQuery.get(url, function (_response) {
				var response = BBT.fixJson(_response);
				if (response["error"] == 0) {
					jObj.html(response["data"]);
				}
			});
		};
</script>

