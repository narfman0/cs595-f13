<script>
/*global jQuery, ndm */
/*jslint white: true, browser: true */
(function ($, NCA) {
    "use strict";
    NCA.page = NCA.page || {};
    NCA.page.story = function () {
        var $storyheader = $('#story div.story-header'),
            $storytools = $('#story div.story-header .story-header-tools'),
            $storyintro = $('#story div.story-intro');
        if ($('#story').length) {
            // add breadcrumbs to story header
            $storyheader.breadcrumbs();
            // add story summary bullets and comments link before story tools in header
            $storytools.before($('#story div.story-summary-list'));
            $storytools.before($('#story .story-comments-link'));
            // add social buttons within story header tools
            $storytools.prepend($('#story-social'));
            // add story intro after story promo and story sidebar
            $storyintro.before($('#story div.story-promo-middle'));
            $storyintro.before($('#story div.story-sidebar'));
            // wrap story headline, story info, story summary and story comments in container for styling
            $('#story .story-headline, #story .story-info, #story .story-summary-list, #story .story-comments-link').wrapAll('.story-header-container');
            $storytools.before($('.story-header-container'));
        }
    };
    if (ndm && ndm.newscomau && ndm.newscomau.page && ndm.newscomau.page.story) {
        ndm.newscomau.page.story();
        var $sidebarpoll = $('#story div.story-sidebar .poll-module');
        // add poll class to sidebar module
        if ($('body.entertainment').length) {
            $sidebarpoll.addClass('ent-text-poll-basic');
        } else {
            $sidebarpoll.addClass('news-text-poll-basic');
        }
    }
}(ndm.jQuery, ndm.newscomau));
</script>
