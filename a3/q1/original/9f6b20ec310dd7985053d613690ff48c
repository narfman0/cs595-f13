<script type="text/javascript">

    function initializeFacebookAPI() {
        FB.init({
            appId      : 519028218124180,
            channelUrl : '//' + window.location.hostname + '/services/facebook/channel.php',
            xfbml      : false,
            status     : true,
            cookie     : true
        });

        // custom function calls after initialization
        if (typeof parseXFBMLTags != 'undefined') {
            parseXFBMLTags();
        }
        if (typeof initializeSocialReader != 'undefined') {
            initializeSocialReader();
        }
    }

    if ($("#fb-root").length == 0) {
        $("body").prepend('<div id="fb-root"></div>');
    }

    loadScriptsAsync(['//connect.facebook.net/en_US/all.js'], initializeFacebookAPI);

</script>
