					  $("#ajaxfeedentries" + catID).load('/feed/show_menu_feed/' + catID, function(response, status, xhr) {
						  $("#feedentries" + catID).html('');
						  if (status == "error") {
							  var msg = "Sorry but there was an error: ";
							  $("#feedentries" + catID).html(msg + xhr.status + " " + xhr.statusText);
						  }
					  });
				      },  
				      function () {
