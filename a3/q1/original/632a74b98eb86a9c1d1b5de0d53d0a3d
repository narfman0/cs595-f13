        <script type="text/javascript">
        jQuery(document).ready(function(){
                  FB.init({appId: '438686849535173', status: true,
                           cookie: true, xfbml: true, oauth: true});
              });
                    
        function insertParam(key, value)
        {
            key = escape(key); value = escape(value);
            var kvp = document.location.search.substr(1).split('&');

            var i=kvp.length; var x; while(i--) 
            {
                x = kvp[i].split('=');

                if (x[0]==key)
                {
                        x[1] = value;
                        kvp[i] = x.join('=');
                        break;
                }
            }

            if(i<0) {kvp[kvp.length] = [key,value].join('=');}

            return kvp.join('&'); 
        }

        function fb_start_login()
        {

            FB.login(function(response) {
				if (response.authResponse) {
                   
                    var date = new Date();
                    date.setTime(date.getTime()+(response.authResponse.expiresIn*1000));
                    var expires = "; expires="+date.toGMTString();
                    document.cookie = "wp_fbs="+response.authResponse.accessToken+expires+"; path=/";
                    
                    var param = insertParam('fb_login', '1');
                    
                    if( jQuery('#modal-login input[name="redirect_to"]') != undefined 
                        && jQuery('#modal-login input[name="redirect_to"]').attr("value") != undefined    
                        && jQuery('#modal-login input[name="redirect_to"]').attr("value").length > 0){
                    	param = param + insertParam('redirect_to',  jQuery('#modal-login input[name="redirect_to"]').attr("value"));
                    }
                    	
                    document.location.search = param;
				} 
             }, {scope: ''});
            
        }
          
        </script>
         
