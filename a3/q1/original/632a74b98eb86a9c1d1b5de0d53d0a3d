<script>
function ice_search_submit()
{
            var rule='http://concreteloop.com/search/%s';
    var ws='%20';
        var v = document.getElementById('search_cond').value.replace(/ /g, ws);
    if(!v) return false;
    var url = rule.replace('%s', v);
    window.location = url;
    return false;
}
</script>

