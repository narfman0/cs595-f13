(function(){
 var links = document.getElementsByTagName('a');
 for (var i = 0; i < links.length; i++) {
	if (links[i].hostname == location.hostname) { continue; }
	links[i].onclick = (function(domain, href, onclick){
		return function(){
		if (typeof onclick == 'function') { onclick.apply(this); }
		_gaq.push(['_trackEvent', 'External Links', domain, href]);
		};
	})(links[i].hostname, links[i].href, links[i].onclick);
 }
})();
</script>

