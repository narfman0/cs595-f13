<script type="text/javascript">
(function(){

    var i, len, url, match,
        allScripts = YUI.presentation.lazyScriptList,
        comboRoot = 'http://l.yimg.com/zz/combo?',
        isCombo =  allScripts[0].indexOf(comboRoot) === 0,
        yuiMods = [],
        lazyScripts = [];

    if (isCombo) {
        for (i = 0, len = allScripts.length; i < len; ++i) {
            url = allScripts[i].replace(comboRoot, '');
            allScripts = allScripts.concat(url.split('&'));
        }
        allScripts = allScripts.slice(len);
    }

    for (i = 0, len = allScripts.length; i < len; ++i) {
        url = allScripts[i];
        match = url.match(/\/yui\/([^._]*)/);
        if (match && match.length > 1) {
            yuiMods.push(match[1]);
        } else {
            lazyScripts.push(url);
        }
    }

    if (isCombo) {
        lazyScripts = [comboRoot + lazyScripts.join('&')];
    }

    if (YMedia.ModulePlatform) {
        YMedia.ModulePlatform.reset();
    }

    YMedia.use(yuiMods, function (Y) {
        Y.dali.config.debug = false;
        Y.Get.script(lazyScripts, { onSuccess: function(){
