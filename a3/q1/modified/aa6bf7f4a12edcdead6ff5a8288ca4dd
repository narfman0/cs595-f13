		$.fn.showFeatureText = function() {
		  return this.each(function(){
		    var box = $(this);
		    var text = $('span.expander',this);
			text.css('height', $('.new').height());
			text.css('display', 'block');
		    text.hide();

		    box.hover(function(){
				clearTimeout($.data(this, 'timer'));
				$('span.expander', this).stop(true, true).slideDown(200);
		    },function(){
		      $.data(this, 'timer', setTimeout($.proxy(function() {
					$('span.expander', this).stop(true, true).slideUp(200);
				}, this), 200));
		    });

		  });
		}
