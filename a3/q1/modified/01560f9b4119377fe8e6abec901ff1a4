      var msgs = {'loadMore': 'Load more...', 'loading': 'Loading...', 'loaded': 'No more!', 'addComment': 'Add comment', 'reply': 'Reply', 'delete': 'Delete'};
      var config = {'blogId': '853639062370024653', 'postId': '7592408720728258960', 'feed': 'http://www.thisisamericanrugby.com/feeds/7592408720728258960/comments/default', 'authorName': 'Grant Cole', 'authorUrl': 'http://www.blogger.com/profile/06836649533720575156', 'baseUri': 'http://www.blogger.com', 'maxThreadDepth': 2};

