function syncRoadBlock(id, content, type, clickthrough, width, height) {
if (type == 'StaticResource') {
document.getElementById(id).innerHTML = "<a target='_blank' href='" + clickthrough + "'><img src='" + content + "' width='" + width + "' height='" + height + "'></a>";
$("#"+id).show();
} else if (type == "HTMLResource") {
document.getElementById(id).innerHTML = content;
$("#"+id).show();
} else if (type == "IFrameResource"){
document.getElementById(id).innerHTML = "<iframe scrolling='no' frameborder='0' style='border:none;overflow:hidden;' width='"+width+"' height='"+height+"' src='"+content+"'></iframe>";
$("#"+id).show();
}
}

