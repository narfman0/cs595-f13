$(function() {
	$('input, textarea').each(function() {
		var field = $(this);
		var default_value = field.val();

		field.focus(function() {
			if (field.val() == default_value) {
				field.val('');
			}
		});

		field.blur(function() {
			if (field.val() == '') {
				field.val(default_value);
			}
		});
	});
});
</script>
