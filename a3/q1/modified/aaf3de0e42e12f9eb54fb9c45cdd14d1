  <script>
   $(function() {
    if (document.documentElement.clientWidth < 600) {
     $("div.nomobilead").remove();
    }
    
    
    $("#three-col-promo, .promo-box, .topic-content, #three-over-two").append('<span class="expand-button"></span><div class="clearfix"></div>');
    $(".expand-button").click(function() {
     if ($(this).css("background-position") == "100% -80px"){
      $(this).css("background-position","100% 0px");
     } else {
      $(this).css("background-position","100% -80px");
     };
    });
    
    $("#three-col-promo .expand-button").click(function() {
     $(".tcp-row .col2 .tcp-item + .tcp-item, .tcp-row .col3").toggle();
    });
    $(".promo-box .expand-button").click(function() {
     $("ul li + li", $(this).parent()).toggle();
     if ($(".promo-box ul li + li.view-none, .promo-box ul li + li.view-gallery, .promo-box ul li + li.view-video").css("display") == "list-item") {
      $(".promo-box ul li + li.view-none, .promo-box ul li + li.view-gallery, .promo-box ul li + li.view-video").css("display","block");	
     }
    });
    $(".topic-content .expand-button").click(function() {
     $(".topic-side, .topic-side-2", $(this).parent()).toggle();
    });
    
    $("#three-over-two .expand-button").click(function() {
     $("#three-over-two .row2, #three-over-two .row1 .col2 .tot-item + .tot-item").toggle();
    });
    
    if ($('#three-over-two .row1').length == 0) {
	$('#three-over-two .row2').show();
	$("#three-over-two .expand-button").hide();
    }
    
    if (document.documentElement.clientWidth >= 600) {
     $("img.lazy").lazyload({});
     $("div.nofullad").remove();
    }
   });
  </script>


