						'wmode', "opaque",
						"bgcolor", "#000000",
						"name", "otvPlayer",						
						"allowScriptAccess", "always",
						"allowFullScreen", "true",
						"type", "application/x-shockwave-flash",
						"pluginspage", "http://www.adobe.com/go/getflashplayer"
				);
			}else{
				
				$(document).ready(function(e){		
								
					//do some browser detection first
					if(navigator.userAgent.match(/iPhone|iPod|iPad/i)) {
						//fallback to html5 video version for iphone, ipod, and ipad
						//all are html5 compatible mobile devices
						fallbackToHTML5();
					}else{			
						//we only do any scripting if the browser actually supports <video>
						var videoTag = document.createElement('video');
						if (!!videoTag.play) {			
							//video tag is supported, but can it be played?
							var mimeType = 'video/mp4; codecs="avc1.42E01E, mp4a.40.2"'
							if(videoTag.canPlayType && videoTag.canPlayType(mimeType)) {
								//video tag can be played using codec, switch to html5 version
								fallbackToHTML5();
							}else{
								//video tag can not be play using codec, request flash to be installed
								requestFlashInstall();
							}
						}else{
							//video tag not supported in browser, request flash to be installed
							requestFlashInstall();
						}
					}
				});
			}
		}
		
		function requestFlashInstall(){
			var player = $('#videoplayer');
			player.html('<div class="defaultPlayer"><a href="http://get.adobe.com/flashplayer/"><img src="http://cdn.abclocal.go.com/static/flash/getflash_600x338.png" alt="Get LATEST ADOBE® FLASH® PLAYER" /></a></div>');
		}
		
		function fallbackToHTML5(){
			var player = $('#videoplayer');
			player.html('<div class="defaultPlayer"><a href="http://abclocal.go.com/wls/html5/video?id=9260337&pid=9260330&section=news/local/mathie"><img src="http://cdn.abclocal.go.com/images/wls/cms_exf_2007/automation/vod/9260335_600x338.jpg" alt="Switch To HMTL5 Version" /><span id="poster_play" class= "poster_play_button" ></span></a></div>');
		}
	})(jQuery);
