<script type="text/javascript" >

	function getStatsForThemeMiniStats() {
		// Call ajax request only if data is not here.
		if ($("#upv_2320251").text() == "Loading..." ) {
			jQuery.getJSON("/stats/theme/2320251?view=json", function(json){
				jQuery("#upv_2320251").empty();
				if (json.stats && json.stats.uniqueVisitors >= 0) {
					jQuery("#upv_2320251").append(json.stats.uniqueVisitors);				
				} else {
					jQuery("#upv_2320251").append("Not available");
				}
			});
		}
	}
</script>
		
