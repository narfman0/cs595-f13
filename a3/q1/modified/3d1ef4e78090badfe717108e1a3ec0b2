<script type="text/javascript"><!--
	
	jQuery(document).ready(function(){
		var mail = getCookie("scoopemail");
        
		jQuery("#inputemail").val(mail);
		
						
							var thePostToPrintHasBeenClosed=false;
			jQuery("body").click(function(e) {
								if(thePostToPrintHasBeenClosed){
										return;
				}
			
								var target = jQuery(e.target);
								if (target.parents(".related-scoops").length===0 && target.parents(".feedbackBannerContent").length===0 && target.parents("#headbanner").length===0 && target.parents("#moreInfoBlock").length === 0 && target.parents("#thePostToPrint").length === 0 && target.parents(".simplemodal-data").length === 0 && !target.hasClass("simplemodal-overlay") && !target.hasClass("simplemodal-data") && !target.hasClass("simplemodal-close")) {
					closePostAndViewTopic(4008176135, "/t/technology-business-by-paulo-gervasio", 2320251, "http://www.scoop.it/t/technology-business-by-paulo-gervasio/p/4008176135/three-technologies-that-want-to-improve-on-javascript-zdnet", '/t/technology-business-by-paulo-gervasio',  null , 'false', 'Technology Business | Scoop.it');
					thePostToPrintHasBeenClosed = true;
				}
			});
			jQuery("#thePostToPrint .htmlfragment").show();
				
				
			});

	function resizeOverlays() {
		jQuery(".onePost").each(function() {
			jQuery(".overlayForCuttingPost", $(this)).css({
				width : $(this).outerWidth(),
				height : $(this).outerHeight()
			});
		});
	}
	
	function getCookie(c_name)
	{
		if (document.cookie.length>0) {
			var c_start=document.cookie.indexOf(c_name + "=");
			if (c_start!=-1) {
			    c_start=c_start + c_name.length+1;
			    c_end=document.cookie.indexOf(";",c_start);
			    if (c_end==-1) c_end=document.cookie.length;
				var val = unescape(document.cookie.substring(c_start,c_end));
				return val.replace(/['"]/g , '');
			}
		}
		return "";
	}
	
--></script>

			                
	
						<script type="text/javascript">
			jQuery(document).ready(function() {
				if(navigator.userAgent.indexOf('Chrome') != -1) {
					jQuery('.post-htmlfragment script[src*="http://player.espn.com"]').not('#thePostToPrint .post-htmlfragment script[src*="http://player.espn.com"]').parent().css("visibility", "hidden");
				}
