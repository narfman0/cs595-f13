<script type="text/javascript">


function updateGCPCBalance(result)
{
  var balance_container = document.getElementById('gc_promo_balance_slot');
  var balance_container_success = document.getElementById('gc_promo_success_slot');
  var balance_container_error = document.getElementById('gc_promo_balance_slot_error');
  result = result.replace(/^\s+|\s+$/g,"");

  if (balance_container)
  {
    balance_container.innerHTML = result;
  }

  if (balance_container_error)
  {
    balance_container_error.innerHTML = result;
  }

  if (balance_container_success)
  {
    balance_container_success.innerHTML = result;
  }
}

var g_GCPCPopupShownOnce = false;

function showGCPCPopup(dialogName) {
	hideGCPCPopup();

	var dialog;
	var button = document.getElementById('gcpc_button');

	if (dialogName) {
		dialog = document.getElementById(dialogName);
	} else {
		dialog = document.getElementById('gcpc_info_container');
	}

	document.getElementById('gcpc_offset_container').style.zIndex = 20000;

	var buttonOffsets = new Array();

	if (button.offsetWidth == 0) {
		buttonOffsets[0] = 160;
	} else {
		buttonOffsets[0] = button.offsetWidth;
	}

	if (button.offsetHeight == 0) {
		buttonOffsets[1] = 30;
	} else {
		buttonOffsets[1] = button.offsetHeight;
	}

	dialog.style.display = "block";
	dialog.style.left = (buttonOffsets[0] / 2) - 420;
	dialog.style.top = (buttonOffsets[1] / 2);

	if (typeof toggleOnGiftPromoPopup == 'function') { toggleOnGiftPromoPopup(true); }
}

function hideGCPCPopup() {
	var dialog = document.getElementById('gcpc_info_container');
	dialog.style.display = "none";

	if (document.getElementById('gcpc_success_container')) {
		document.getElementById('gcpc_success_container').style.display = "none";
	} 

	if (document.getElementById('gcpc_failure_container')) {
		document.getElementById('gcpc_failure_container').style.display = "none";
	}
	document.getElementById('gcpc_offset_container').style.zIndex = 0;

	if (typeof toggleOnGiftPromoPopup == 'function') { toggleOnGiftPromoPopup(false); }
}

function submitGCPCPopup(codeValueId) {
	var code;
	
	if (codeValueId) {
		code = document.getElementById(codeValueId);
	} else {
		code = document.getElementById('GCPCCodeValue');
	}

	var formCode = document.getElementById('frmProcessGCPCCode_Value');
	formCode.value = code.value;

	var GCPCPopupForm = document.getElementById('frmProcessGCPCCode');
	GCPCPopupForm.submit();
}

function checkEnterKey(evt) {
	if (evt.keyCode == 13) { return false; }
	return true;
}

function popupGCPCTOC()
{
  popupwindow = window.open('/gp/product/features/dv-gcpc-toc.html', 'toc', 'height=500,width=850,resizable=1,menubar=0,scrollbars=1');
  if (window.focus) { popupwindow.focus(); }
  return false;
}


