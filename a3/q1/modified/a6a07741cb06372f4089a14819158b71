	$('select#kejuaraan').change(function(){
				base_url = 'http://sport.detik.com/sepakbola/read';
		redirect_url = base_url;		
		if($(this).val() != '')
		{
			redirect_url = base_url + '/' + $(this).val() + '/';
		}
		window.location.href = redirect_url;
	});
	$('select#kejuaraan').val('');
	$('select#tim_terkait').change(function(){
		redirect_url = $(this).val();
		window.location.href = redirect_url;
	});


</script>

