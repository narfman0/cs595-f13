<script type="text/javascript">
	var setImageId=2; var countImage = ;
	function GetImage(UrlDetail, status, urlTrack) { 

	  $('div#idmagebox div').css('display','none');

	  if(UrlDetail =='nourlprev'){
		setImageId--;
		var getSrc = document.getElementById('idimageconten_'+setImageId).getAttribute('src');
		var urlKirim = getSrc;	
	  }else if(UrlDetail =='nourlnext'){
		setImageId++;
		var getSrc = document.getElementById('idimageconten_'+setImageId).getAttribute('src');
		var urlKirim = getSrc;	
	  }else{
		var urlKirim = UrlDetail;
		setImageId = status;
	  }

	  var urlTrackstr = urlTrack+'/'+(setImageId-1);
		  
        _gaq.push(
		    ['_setAccount', 'UA-891770-7'],
		    ['_trackPageview', urlTrackstr],
		    ['b._setAccount', 'UA-891770-59'],
		    ['b._trackPageview'],
		    ['c._setAccount', 'UA-891770-118'],
		    ['c._trackPageview']
      	);
        urchinTracker();

	  $('#idtextconten_'+setImageId).css('display','block');

		$("#idnourlprev").show();
		if(setImageId ==2){
			$("#idnourlprev").hide();
		}
		$("#idnourlnext").show();
		if(setImageId == countImage){
			$("#idnourlnext").hide();
		}

	
	  document.getElementById("idmultifoto_big").innerHTML = "";
	  document.getElementById("idmultifoto_big").innerHTML = "<span><img height='345' width='455' src='http://sport.detik.com/sepakbola/image/loading.gif'></span>";
	  $.get("http://sport.detik.com/sepakbola/index.php?fa=detik.getmultifoto&urlFoto="+urlKirim , function(data){   
		  document.getElementById("idmultifoto_big").innerHTML = "";
		  $('#idmultifoto_big').append(data);
	  });
		   
	}
</script>
