        function doit(){
                jQuery.post("/wp-content/plugins/imagenes/ajax-getbanner.php",{ngggal:"21",img:"-1"},function(data){
                    data = eval('('+data+')');
                    var img = jQuery('<img>');
                    img.attr('src','/'+data.path+'/'+data.filename)
                    if(data.description.match(/^http:\/\//) || data.description.match(/^https:\/\//)){
                       var a = jQuery('<a>');
                       if(data.description.indexOf('http://www.nicolasmaduro.org.ve')!=-1){
                           a.attr('target','')
                       }else{
                           a.attr('target','blank');
                       }
                       a.attr('href',data.description);
                       a.attr('rel','nofollow')
                       a.append(img);
                       img = a;
                    }
                    jQuery('#imagen2').append(img);
                    jQuery('#imagen2').css('position','relative');
                    jQuery('#imagen2').css('margin-bottom','20px');
                });
        }

        doit();
