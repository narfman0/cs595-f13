	<script language="javascript">
		function footerSearch() {
			var q = document.getElementById("f").value.replace(/ /g,"+");
			if (q.length == 0) {
				alert("Please enter your search criteria");
				return;
			}
			var dname = document.domain; 
			if ( dname=="driving.ca") {
				document.location.href="http://www.driving.ca/vancouversun/search/search.html?q=" + escape(q);
				}
			else {
				document.location.href="/search/search.html?q=" + escape(q);
			}
		}
	</script>
	<script language="javascript">
		document.getElementById("f").value = document.getElementById("q").value;
	</script>

