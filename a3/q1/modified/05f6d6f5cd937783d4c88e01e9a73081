			<script type="text/javascript">
				function viewPhoto(current, count, akamai_url)
				{
					// change main pic's properties
					document.getElementById("story_photo").alt = arrAlt[current];
					document.getElementById("story_photo").title = arrCaption[current];
					document.getElementById("storyphotocaption").innerHTML  = arrCaption[current];
					// akamaize image URL if passed in
					if (akamai_url != "")
						document.getElementById("story_photo").src = akamai_url + "/" + arrPhoto[current];
					else
						document.getElementById("story_photo").src = arrPhoto[current];
					// credit/distributor
					if(arrCredit[current] != '')
					{
						if(arrDistributor[current] != '')
							document.getElementById("storyphotocredit").innerHTML  = "<b>Photograph by: </b>" + arrCredit[current] + ", " + arrDistributor[current];
						else
							document.getElementById("storyphotocredit").innerHTML  = "<b>Photograph by: </b>" + arrCredit[current];
					}
					else
						document.getElementById("storyphotocredit").innerHTML  = "";

					// highlight current thumbnail, blur others
					for(x = 1; x <= count; x++)
					{
						if(x == current)
						{
							document.getElementById("photo" + x).style["opacity"] = "0.9";
							document.getElementById("photo" + x).style["filter"] = "alpha(opacity = 90)";
						}
						else
						{
							document.getElementById("photo" + x).style["opacity"] = "0.5";
							document.getElementById("photo" + x).style["filter"] = "alpha(opacity = 50)";
						}
					}

					// calculate next and previous links
					var prev = current - 1;
					var next = current + 1;
					if (current == count)
					{
						next = 1;
					}
					else if (current == 1)
					{
						prev = count;
					}
					document.getElementById("previousphoto").href = "javascript:photoClick(" + prev + "," + count + ",'" + akamai_url + "',' - Photo Click Previous' )";
					document.getElementById("nextphoto").href = "javascript:photoClick(" + next + "," + count + ",'" + akamai_url + "',' - Photo Click Next')";
				}

				function photoClick(pos, count, url, eventText)
				{
					//show photo
					viewPhoto(pos,count,url);

					//refresh ads
					adSync.TriggerAdSynch();

					//omniture event
					statsEvent(eventText)
				}
			</script>
		
