<script type="text/javascript">

function mycarousel_itemLoadCallback(carousel, state)
{
	// Since we get all URLs in one file, we simply add all items
	// at once and set the size accordingly.
	if (state != 'init')
		return;

	var data = '<div onClick="changeSlideshow(0);" class="thumb_image"><img main_w="465" main_h="310" src="http://cdn-corporate.canada.travel/sites/default/files/imagecache/fullstory_thumb/content/ctc_news/GoMedia/gomedia_awards_2013.jpg" alt="" /><span></span></div>|';
	data = data.substring(0, data.length-1);
	
	mycarousel_itemAddCallback(carousel, carousel.first, carousel.last, data);

//    jQuery.get('dynamic_ajax.txt', function(data) {
//        mycarousel_itemAddCallback(carousel, carousel.first, carousel.last, data);
//    });
};

function mycarousel_itemAddCallback(carousel, first, last, data)
{
    // Simply add all items at once and set the size accordingly.
    var items = data.split('|');

    for (i = 0; i < items.length; i++) {
        carousel.add(i+1, mycarousel_getItemHTML(items[i]));
    }

    carousel.size(items.length);
};

/**
 * Item html creation helper.
 */
function mycarousel_getItemHTML(url)
{
	return url;
//    return '<img src="' + url + '" width="75" height="75" alt="" />';
};


// JS for Changing the Main Slideshow window
var number_of_items = 1;

var items = [ { "type": "image", "original": { "fid": "11728", "uid": "729", "filename": "gomedia_awards_2013.jpg", "filepath": "sites/default/files/content/ctc_news/GoMedia/gomedia_awards_2013.jpg", "filemime": "image/jpeg", "filesize": "66543", "status": "1", "timestamp": "1379003319", "list": "1", "data": { "credit": { "body": "Golden touch: 2013 GoMedia Awards winners. ", "format": 0, "style": "textarea" }, "alt": "", "title": "" }, "nid": "9709", "view": "\x3cimg  class=\"imagefield imagefield-field_imce_image\" width=\"465\" height=\"310\" alt=\"\" src=\"http://cdn-corporate.canada.travel/sites/default/files/content/ctc_news/GoMedia/gomedia_awards_2013.jpg?1379003319\" /\x3e" }, "thumb_url": "http://cdn-corporate.canada.travel/sites/default/files/imagecache/fullstory_thumb/content/ctc_news/GoMedia/gomedia_awards_2013.jpg", "description": "Golden touch: 2013 GoMedia Awards winners.\n", "main_url": "http://cdn-corporate.canada.travel/sites/default/files/imagecache/fullstory_image_465w/content/ctc_news/GoMedia/gomedia_awards_2013.jpg", "main_w": 465, "main_h": 310 } ];  

function changeSlideshow(index) {
	if (items[index].type == "image") {
		//alert("Is an Image!");
		loadImage(index);
	}
	else if (items[index].type == "video") {
		//alert("Is a Video!");
		loadVideo(index);
	}
	else {
		alert("unknown type");
	}
}

function loadImage(index) {
	// Is it currently a video (iframe)?
	if ($("#slideshow iframe").length > 0){
		$("#slideshow iframe").remove();
		$("#slideshow").append('<img src="" alt="" />');
	}

  // fade out current image
  $("#slideshow img").fadeTo('fast', 0.2);

  // set margin for portait
  var main_w = $('.thumbs li:eq('+ index +") img").attr("main_w");
  //IE6 
  if ($.browser.msie && $.browser.version < 7) {  
    main_w = Math.ceil(main_w);
  }

  if (main_w < 465) {
    // portait
    var margin_left = (465 - main_w) / 2;
    $('#slideshow img').css("margin-left", margin_left);
  }
  else {
    // landscape
    $('#slideshow img').css("margin-left", 0);
  }
   
  // fade in new image after it's positioned
  $("#slideshow img").attr('src', items[index].main_url).fadeTo(1000, 1);
  $("#slideshow img").attr('title', items[index].description);

  $("#blog-gallery-caption .desc").text(items[index].description);
}

function loadVideo(index) {
	//is it currently an image?
	if ($("#slideshow img").length > 0){
		$("#slideshow img").remove();
		$("#slideshow").append('<iframe width="465" height="310" src="" frameborder="0" allowfullscreen></iframe>');
	}

	// fade in new image after it's positioned
  $("#slideshow iframe").attr('src', items[index].main_url);

  $("#blog-gallery-caption .desc").text('');
}


jQuery(document).ready(function() {
	jQuery('#mycarousel').jcarousel({
		itemLoadCallback: mycarousel_itemLoadCallback,
		vertical: true,
		scroll: 3,
		visible: 3
	});
});

</script>
