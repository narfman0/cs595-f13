      <script type="text/javascript">var addthis_config = {services_compact: "facebook, twitter, email, bitly, google_plusone_share, stumbleupon, favorites, gmail, blogger, tumblr, linkedin, more"}</script>      <a class="addthis_button" addthis:url="http://en-corporate.canada.travel/content/ctc_news/gomedia-canada-marketplace-2013-awards" addthis:title="GoMedia Awards illuminate CTC’s premier annual media event." href="http://www.addthis.com/bookmark.php?v=250&amp;username=xa-4c72bd5f03ea9dc1"><img src="http://s7.addthis.com/static/btn/v2/lg-share-en.gif" width="125" height="16" alt="Bookmark and Share" style="border:0"/></a><script type="text/javascript" src="http://s7.addthis.com/js/250/addthis_widget.js#username=xa-4c72bd5f03ea9dc1"></script>
