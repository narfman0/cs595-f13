		<script type="text/javascript" language="JavaScript">
  
			function getchannel() {

				var breadcrumb = $("div.breadcrumb").text();

				var chead = breadcrumb.split(" › ")[1];

				if (!chead) {

					chead = "Home";

				}

				switch (chead) {

					case 'Markets':

						break;

					case 'Research':

						break;

					case 'Resources':

						chead = 'Resources for Industry';

						break;

					case 'Media Centre':

            chead = 'Mediacentre';

						break;

          case 'Centre des médias':

            break;

					case 'About the CTC':

						chead = 'About CTC';

						break;

					case 'Marchés':

						break;

					case 'Recherche':

						break;

					case 'Ressources':

						chead = 'Ressources pour l’industrie';

						break;

					case 'Salle de presse':

						chead= 'Salle des Nouvelles';

						break;

					case 'La CCT':

						chead='À propos de la CCT';

						break;

				  case 'News releases':

						break;

					case 'CTC News':

						break;

				  case 'CTC Subscriptions':

				    break;

				  case 'Out offices':

				    break;

				  case 'Nouvelles de la CCT':

				    break;

				  case 'Communiqués':

				    break;

				  case 'S\'abonner aux nouvelles de la CCT':

				    break;

				  case 'Nos bureaux':

				    break;

					case 'GoMedia Awards illuminate CTC’s premier annual media event.':

						if (chead != 'Resources for Industry' 
							&& chead != 'Ressources pour l’industrie' 
							&& chead != 'À propos de la CCT') {

							chead = 'Orphan Pages';

						}

						break;

				}

				return chead;
			}

		</script>
