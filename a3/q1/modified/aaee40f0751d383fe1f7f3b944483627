erAgent||navigator.vendor||window.opera);

	function browserCheck(){
		var userAgent = navigator.userAgent.toLowerCase();
		$.browser.chrome = /chrome/.test(navigator.userAgent.toLowerCase());

		if($.browser.msie){
			userAgent = $.browser.version;
			version = userAgent.substring(0,userAgent.indexOf('.'));
			if (version < 8) return false;
		} else if($.browser.chrome){
			userAgent = userAgent.substring(userAgent.indexOf('chrome/') +7);
			version = userAgent.substring(0,userAgent.indexOf('.'));
			if (version < 23) return false;
		} else if($.browser.safari){
			userAgent = userAgent.substring(userAgent.indexOf('version/') +8);
			version = userAgent.substring(0,3);
			if (version < 5.1) return false;
		} else if($.browser.mozilla && navigator.userAgent.toLowerCase().indexOf('firefox') != -1){
			userAgent = userAgent.substring(userAgent.indexOf('firefox/') +8);
			version = userAgent.substring(0,userAgent.indexOf('.'));
			if (version < 16) return false;
		}
		return true;
	}

	if((readCookie('browser_check') === null) && (! jQuery.browser.mobile)) {
		if(!browserCheck()) {
			alert("To better enjoy E! Online, please consider updating your browser. We like IE 8.0, Safari 5.1, Chrome 23.0, Firefox 16.0 (or greater). Thank you!\n\nFor more info\nhttp://www.eonline.com/news/333611/faq-mdash-using-the-new-e-online");
		}
		createCookie('browser_check','1',0);
	}

	if(eol.uattr('cmpid') === 'rd-xfinity-comcast-eonline') {
		//if cmpid attribute then create cookie that expires after 1 day
		if( navigator.userAgent.match(/iPad/i) !== null) {
			//persistent cookie for 1 hour
			createCookie('rd-xfinity-comcast-eonline-iPad', '1', 0.4);
		} else {
			createCookie('rd-xfinity-comcast-eonline', '1', 0);
		}
	}
	if(readCookie('rd-xfinity-comcast-eonline') === '1' || readCookie('rd-xfinity-comcast-eonline-iPad') === '1') {
		//inject before first script
		var firstScript = document.getElementsByTagName("script")[0];
		var newScript = document.createElement("script");
		newScript.type = "text/javascript";
		newScript.src = "http://xfinity.comcast.net/js-api/compressed/xpbar.js?version=3&pagename=eonline";
		firstScript.parentNode.insertBefore(newScript, firstScript);
	} 
})(jQuery.noConflict());
</script>

