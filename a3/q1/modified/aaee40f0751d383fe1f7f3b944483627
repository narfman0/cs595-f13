<script type="text/javascript">

// These 2 functions have a scoping issue, with jQuery. So they can't be included into the standard set of functions below.

function isSearchValid(inputfield, inputform) {
	var searchStr = jQuery(inputfield).val();
	if (searchStr === '') {
		jQuery(inputfield).focus();
		return false;
	}else{
		trackLink2(jQuery(inputfield)[0], searchStr, {"events": "event1"});
		return true;
	}
}

function submitSearch(searchStr) {
	jQuery('#Header_Search_Input').val(searchStr);
	jQuery('#Header_Search_Form').submit();
}

// End Search Functions.

jQuery.noConflict();
jQuery(document).ready(function(){

	if (isAppleDevice) { 
		document.title = "E! Online";
	}

	var debugIPad = false; // Turn this on/off as needed, to debug the collapsed nav on the iPad.
	var mainMenu = ".Header_E_Layout";
	var mainMenuCont="#Header_Layout_content";
	var secNav = "#Header_SecondaryNav";
	var searchbox = "#Header_SearchBlock";
	var navblock = "#Header_NavBlock";
	var logoblock = "#Header_LogoBlock";
	var topshowstab = "#Header_NavItem_EShows";
	var headerCallOut = ".Header_Callout";
	var eShowsCallOut= "#Header_NavItem_EShows .Header_Callout";
	var mainContentLayout = ".lp_layout";
	var nonLiquidLayout = ".lp_wrap, #content_interior";
	var emptyHeader = ".Header_Empty_Layout";
	var showsSubMenu= "#Header_ShowsSubMenu";
	var menulistlinkheight= ".Header_NavItem a";
	var menulistheight = "#Header_NavBlock ul li";
	var menuOverrideheight = "#Header_NavBlock .Nav_Fontsize_Override .Header_NavItem";
	var callOut = ".Header_Callout #Header_CalloutLine";
	var callOutCaret = ".Header_Callout #Header_Callout_Caret";

	var showstab = ".Header_E_Layout #Header_NavItem_EShows, .Header_E_Layout #Header_ShowsSubMenu, #Header_NavItem_EShows .Header_Callout";
	var submenuarrow = "#Header_ShowsSubMenu .Header_nav_locator img";
	var showsItemLeft ="";
	var subMenuLeft = "";
	var subMenuCenter = "";
	var arrowLeft = "";
	var navAnimate = true;
	var isCollapsed = false;
	var animationTimer = 100;
	var lastScrollTop = -1;
	var scrollTop = 0;

	// Backup Safety Check
	
	//header spot caret styling per browser
	if (!isLocalhost) { debugIPad = false; } // This will force the debugging switch to false on non-Localhost servers, if you forget to reset it above before checking the code in.

	if(jQuery.browser.mozilla && navigator.userAgent.toLowerCase().indexOf('firefox') != -1){
			jQuery(callOutCaret).css({"margin-left":"-8px"});
		}
	if(jQuery.browser.webkit && navigator.userAgent.toLowerCase().indexOf('safari') != -1){
			jQuery(callOutCaret).css({"margin-left":"-3px"});
	}
	if(jQuery.browser.msie && navigator.userAgent.toLowerCase().indexOf('msie') != -1){
		jQuery(callOutCaret).css({"margin-left":"-6px"});
	}
	var jumpline = jQuery(mainMenu).offset().top + jQuery(mainMenu).height() - 14; // The 14 px modifier takes the jumpline up to the bottom of the E! logo. Once a user passes that point & before they hit the bottom of the global nav, they would simply see a solid black bar... instead of a collapsed menu.
	jQuery(window).load(function () {
		setTimeout(updateArrowUp, 100);
		if(jQuery(document).scrollTop() > jumpline) {
			buildCollapsedNav();
		}	
	});
	var smalllogopath = "/resources/images/header_footer/E_logo_trans_62x50.png";
	var largelogopath = "/resources/images/header_footer/E_Nav_logo_100x120.png";

	jQuery(mainMenu).css({"margin-left":"0"});

	//Tracking vars
	var trackingElogo = "#Header_Layout_content a#e-logo";
	var trackingAll = "#Header_NavItem_All a";
	var trackingNews = "#Header_NavItem_News a";
	var trackingPhotos = "#Header_NavItem_Photos a";
	var trackingVideos = "#Header_NavItem_Videos a";
	var trackingTv = "#Header_NavItem_TVScoop a";
	var trackingTrends = "#Header_NavItem_Trends a";
	var trackingEShows = "#Header_NavItem_EShows a#header-shows-link";
	var trackingRedCarpet = "#Header_NavItem_RedCarpet a";
	var trackingmoreshows = "#Header_ShowsSubMenu .more-shows a";
	var trackingfullschedule = "#Header_ShowsSubMenu .full-schedule a";
	var trackingfeaturedShowThumb = "#Header_ShowsSubMenu table#HeaderShowsSubnavTable td.featured-show-thumb a";
	var trackingTileShowThumb1 = "#Header_ShowsSubMenu table#HeaderShowsSubnavTable td.recommended-shows-thumbnails table td.thumb1 a";
	var trackingTileShowThumb2 = "#Header_ShowsSubMenu table#HeaderShowsSubnavTable td.recommended-shows-thumbnails table td.thumb2 a";
	var trackingTileShowThumb3 = "#Header_ShowsSubMenu table#HeaderShowsSubnavTable td.recommended-shows-thumbnails table td.thumb3 a";
	var trackingTileShowThumb4 = "#Header_ShowsSubMenu table#HeaderShowsSubnavTable td.recommended-shows-thumbnails table td.thumb4 a";

	if (navigator.appVersion.indexOf("Win")!=-1){
		if(jQuery.browser.mozilla && navigator.userAgent.toLowerCase().indexOf('firefox') != -1){
			jQuery(menulistlinkheight).css({"display":"block","height":"29px"});
			jQuery(menulistheight).css({"display":"block","float":"left","padding-right":"12px"});
			jQuery(menuOverrideheight).css({"padding-left":"7px","padding-right":"12px"});
			jQuery(callOut).css({"top":"14px"});
		}
	}

	// *****************
	// *** MAIN LINE ***
	// *****************

	jQuery(window).scroll(function () {
		
			
			
				isScrolling = true;
			
			
		

		var iPadTrace = '';
		scrollTop = jQuery(document).scrollTop();
		/* Welcome to Leaderboard Math! 
		The /about/ section (Privacy Policy, Terms of Usage, etc...) has a different leaderboard setup than the rest of the site does. It's older & has a 93px leaderboard height + a 0px new leaderboard height + a 5px top margin = 98px. The newer global leaderboard has a 90px high table + a 5px top margin = 95px.
		*/
		var leaderBoardHeight = (window.location.href.indexOf('/about/') > -1) ? jQuery('.leaderboard-ad').height() : jQuery('#Header_BannerBlock').height();

		// Since leaderboards are dynamically scalable, we need to take that into account for the collapsed nav.
		var newScrollTop = (scrollTop - 125 - leaderBoardHeight); // Change this to affect the Collapsed Nav's snap-up height, when the iPad's keyboard is displayed on-screen.

		// Now check it if needed.
		if (debugIPad) { 
			iPadTrace += 'jumpline: ' + jumpline + '\n';
			iPadTrace += 'scrollTop: ' + scrollTop + '\n';
			iPadTrace += 'scrollTop >= jumpline: ' + (scrollTop >= jumpline) + '\n';
			iPadTrace += 'leaderBoardHeight: ' + leaderBoardHeight + '\n';
			iPadTrace += 'newScrollTop: ' + newScrollTop + '\n';
			iPadTrace += 'trace route: ';
		}

		if(scrollTop >= jumpline){
			if (debugIPad) { iPadTrace += '1'; }
			if(navAnimate && (lastScrollTop < 1) && !isCollapsed){
				if (debugIPad) { iPadTrace += ' -> 2'; }
				/**************************************************************************/
				/* Collapsed Nav functionalities are moved to buildCollapsedNav function  */
				/**************************************************************************/
				buildCollapsedNav();
				if (isAppleDevice) {
					if (debugIPad) { iPadTrace += ' -> 3'; }
					jQuery(mainMenu).css({"position":"relative"}); // This one is required, so that the collapsedNav jumps back to the top when the iPad's keyboard opens.
					moveNav(newScrollTop + 'px', 250);
				} else if (debugIPad) { iPadTrace += ' -> 4'; }
				// = "collapsed";
				// = "something";
				//console.log("testing"+);
			} else { // isCollapsed
				if (debugIPad) { iPadTrace += ' -> 5'; }
				// Allows the iPad's collapsed nav to snap back to the page top, whenever the keyboard is opened & the user begins to scroll.
				// Otherwise, it floats around on the page.
				if (isAppleDevice) {
					if (debugIPad) { iPadTrace += ' -> 6'; }
					// Move the Nav, by that many pixels.
					jQuery(mainMenu).css({"position":"relative"}); // This one is required, so that the collapsedNav jumps back to the top when the iPad's keyboard opens.
					moveNav(newScrollTop + 'px', 50);
				} else if (debugIPad) { iPadTrace += ' -> 7'; }
			}
			if (debugIPad) { iPadTrace += ' -> 8'; }
			// Capture the scrollTop value, for next time.
			lastScrollTop = scrollTop;
			isCollapsed = true;
		}else if(scrollTop < jumpline){
			if (debugIPad) { iPadTrace += '9'; }
			/*if (isAppleDevice && (lastScrollTop !== -1)) {
				if (debugIPad) { iPadTrace += ' -> 10'; }
				//jQuery(mainMenu).css({"position":"fixed","top":"0px"}); // Don't need this one. I think this is causing problems on the Mac Mini.
			} else {
				if (debugIPad) { iPadTrace += ' -> 11'; }
			}*/
			if(!navAnimate){
				
					
					
						isScrolling = false;
					
					
				
				/***********************************************************************/
				/* Expanded Nav fucntionalities are moved to buildExpandedNav function */
				/***********************************************************************/
				buildExpandedNav();
			}
			lastScrollTop = -1;
			isCollapsed = false;
		} else {
			if (debugIPad) { iPadTrace += ' -> 12'; }
		}
		
			
			
				isScrolling = false;
			
			
		

		if (debugIPad) {
			if (isAppleDevice) {
				alert(iPadTrace); // iPad Alert.
			} else {
				log(iPadTrace);
			}
		}
	});

	// *****************
	// *** FUNCTIONS ***
	// *****************

	function buildCollapsedNav(){
		centerNav();
		jQuery(headerCallOut).hide();
		jQuery(mainMenu).css({"margin-top":"-30px"});
		if(navigator.appVersion.indexOf("Mac")!=-1)
			animationTimer = 100;

		

		jQuery(mainMenu).animate({
			top: "+=30px"
		}, animationTimer , function() {
			jQuery(mainMenu).css({"margin-top":"0","top":"0"});
		});

		navAnimate= false;
		isCollapsed = true;

		jQuery(emptyHeader).css({"visibility":"hidden","display":"block"});
		jQuery(mainMenu).css({"height":"50px"});
		jQuery(mainMenu).css({"position":"fixed","top":"0px"});
		jQuery(secNav).hide();
		jQuery(mainMenuCont).css({"height":"50px"});

		jQuery(searchbox).css({"top":"6.5px"});
		jQuery(navblock).css({"top":"10px"});
		jQuery(logoblock).css({"height":"50px","width":"60px","margin-left":"25px"});
		jQuery(logoblock).css({"background-image":"url('"+smalllogopath+"')"});
		jQuery(submenuarrow).css({"margin-left":arrowLeft+"px"});
		jQuery(mainMenuCont).addClass("Header_Collapsed");
		updateTracking();
	}

	function buildExpandedNav(){
		jQuery(emptyHeader).css({"display":"none"});
		jQuery(headerCallOut).show();
		jQuery(mainMenuCont).removeClass("Header_Collapsed");
		jQuery(mainMenu).css({"height":"120px","position":"relative","margin-left":"0"});
		jQuery(mainMenuCont).css({"height":"120px"});
		jQuery(logoblock).css({"background-image":"url('"+largelogopath+"')"});
		jQuery(logoblock).css({"height":"120px","width":"100px","margin-left":"5px"});
		jQuery(searchbox).css({"top":"42px"});
		jQuery(navblock).css({"top":"47px"});
		jQuery(secNav).show();
		navAnimate = true;
		isCollapsed = false;
		updateTracking();
	}

	function centerNav(){
		nonLiquidWidth = jQuery(mainMenu).width();
		if(nonLiquidWidth > 980)
			mainContWidth = jQuery(mainContentLayout).width();
		else
			mainContWidth = jQuery(nonLiquidLayout).width();
		//console.log("maincontWidth"+mainContWidth);
		jQuery(mainMenu).css({"width":mainContWidth+"px"});
		navCenter(mainMenu);
	}

	function moveNav(pixels,timer) {
		if (!pixels || !isAppleDevice) return;
		if (pixels.indexOf('px') === -1) { pixels += 'px'; }
		if (pixels.indexOf(';') > -1) { pixels = pixels.replace(';',''); }
		if (!timer) { timer = 0; }
		setTimeout(function() {
			if (isAppleDevice) { // && isCollapsed) {
				// The iPad's Keyboard automatically centers the search box & global nav bar in the center of the screen. So it shifts the page down 358px. 
				// We have to move the global nav bar back to the top of the screen, whenever that happens.
				//jQuery('.Header_E_Layout').css("top",pixels);
				jQuery(mainMenu).css({"position":"fixed","top":"0px"});
			}
		},timer); // This timeout is necessary to give it a slight delay, so that the scroll finishes moving... before the global nav is shifted up. Don't remove it!
	}

	function navCenter(inputNav) {
		jQuery(inputNav).css({"margin-left": ( jQuery(window).width() - jQuery(inputNav).width() ) / 2+jQuery(window).scrollLeft() + "px"});
		jQuery(inputNav).css({"left":"0"});
	}

	function resetNav() { // Short-cut function.
		if (isAppleDevice && isCollapsed) {
			jQuery(mainMenu).css({"position":"fixed","top":"0px"});
			moveNav('0px');
		}
	}

	function updateArrowUp(){
		showsItemLeft = jQuery(topshowstab).offset().left;
		subMenuLeft = jQuery(showsSubMenu).offset().left;
		subMenuCenter = ((jQuery(topshowstab).width())-7) /2;
		arrowLeft = showsItemLeft - subMenuLeft - 27 +subMenuCenter;
		jQuery(submenuarrow).css({"margin-left":arrowLeft+"px"});
	}

	function updateTracking(){
		var trackingArray = [trackingElogo, trackingAll, trackingNews, trackingPhotos, trackingVideos, trackingTv, trackingTrends, trackingEShows, trackingRedCarpet, trackingmoreshows, trackingfullschedule, trackingfeaturedShowThumb, trackingTileShowThumb1, trackingTileShowThumb2,trackingTileShowThumb3,trackingTileShowThumb4];
		var dataOmnitureValues = [];
		var collapsedOmnitureValues = [];
		var expandedOmnitureValues = [];
		for(var i=0, j=trackingArray.length; i<j; i++){
			dataOmnitureValues[i] = jQuery(trackingArray[i]).attr('data-omniture');
			//console.log("top:"+dataOmnitureValues[i]);
		}

		if(isCollapsed){
			for(var i=0, j=dataOmnitureValues.length; i<j; i++){
				collapsedOmnitureValues[i] = (dataOmnitureValues[i]) ? dataOmnitureValues[i].replace(/top-navigation/g,"collapsed-navigation") : "";
			}
			for(var i=0, j=collapsedOmnitureValues.length; i<j; i++){
				jQuery(trackingArray[i]).attr('data-omniture',collapsedOmnitureValues[i]);
			}
		}else{
			for(var i=0, j=dataOmnitureValues.length; i<j; i++){
				expandedOmnitureValues[i] = (dataOmnitureValues[i]) ? dataOmnitureValues[i].replace(/collapsed-navigation/g,"top-navigation") : "";
			}
			for(var i=0, j=expandedOmnitureValues.length; i<j; i++){
				jQuery(trackingArray[i]).attr('data-omniture',expandedOmnitureValues[i]);
			}
		}
	}

	//credits for this method goes to stackoverflow
	var waitForFinalEvent = (function () {
		var timers = {};
		return function(callback, ms, uniqueId){
			if(!uniqueId){
				uniqueId = "Don't repeat the resize process until the final resize is done";
			}
			if(timers[uniqueId]){
				clearTimeout(timers[uniqueId]);
			}
			timers[uniqueId] = setTimeout(callback, ms);
		};
	})();

	// ***********************
	// *** EVENT LISTENERS ***
	// ***********************

	jQuery(window).resize(function() {
		waitForFinalEvent(function(){
			centerNav();
			if(!isCollapsed)
			jQuery(mainMenu).css({"margin-left":"0"});
			updateArrowUp();
		},300,"ending");
	});

	if(isAppleDevice){
		jQuery(showstab).on('touchstart',function(e) {
			jQuery(showsSubMenu).css({"visibility":"visible"});
			jQuery(submenuarrow).css({"visibility":"visible"});
			e.stopPropagation();
		});
		jQuery('html').on('touchstart',function(e) {
			jQuery(showsSubMenu).css({"visibility":"hidden"});
			jQuery(submenuarrow).css({"visibility":"hidden"});
		});
		// showstab is the <li> tag & not the <a> tag.
		jQuery('#header-shows-link').on('touchstart',function(e) {
			jQuery('#header-shows-link').attr('href','javascript:void(0);');
			e.stopPropagation();
		});

		//Begin Collapsed Nav iPad Keyboard Fix.
		/* jQuery('input').on('click',function(e) { // Not used, as the iPad Alert's 1 -> 2 -> 3 -> 8 or 1 -> 4 -> 5 -> 8 sequences will run, immediately after this onClick is run. Both of those have moveNav() functions, built into them.
			if (isCollapsed) {
				moveNav('-358px');
			}
		});*/
		jQuery('input').on('blur',function(e) {
			if (isCollapsed) {
				resetNav();
			}
		});
		// End Collapsed Nav iPad Keyboard Fix.
	}else{
		// log('no');
		jQuery(showstab).on({
			mouseenter: function() 
			{
				jQuery(showsSubMenu).css({"visibility":"visible"});
				jQuery(submenuarrow).css({"visibility":"visible"});
			},
			mouseleave: function()
			{
				jQuery(showsSubMenu).css({"visibility":"hidden"});
				jQuery(submenuarrow).css({"visibility":"hidden"});
			}
		});
	}
});
</script>


            











	            
