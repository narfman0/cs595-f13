    <script type="text/javascript">
        $(document).ready(function ($) {

            /* Watermarks */
            $('.home #ilEmailAddress').watermark('Email Address', { className: 'watermark' });
            $('.home #ilPassword').watermark('Password', { className: 'watermark' });

            $('.password #iPasswordEmailAddress').watermark('Email Address', { className: 'watermark' });

            function adaptToWindowSize() {
                $("body").toggleClass("fbIframe", $(window).width() < 820);
                $('html').toggleClass("fbIframe", $(window).width() < 820);
            };

            $(window).resize(adaptToWindowSize);
            $(adaptToWindowSize);

        });

       

        var isInIFrame = (window.location != window.parent.location);
        if (isInIFrame == true) {
            window.fbAsyncInit = function () {
                FB.init({ status: true, cookie: true, xfbml: true, oauth: true });
                window.setTimeout(function () {
                    FB.Canvas.setAutoGrow();
                }, 250);
            };

            (function () {
                var e = document.createElement('script');
                e.async = true;
                e.src = document.location.protocol + '//connect.facebook.net/en_US/all.js';
                document.getElementById('fb-root').appendChild(e);
            } ());
        }
        else {
            // redirect to full site
        }
    </script>

    <script type="text/javascript">

        $(document).ready(function () {
