<!-- handle enter clicks -->
<script type="text/javascript">
    $(document).ready(function () {

        $(".b-login-element").keypress(function (e) {
            if (e.keyCode == 13) {
                $("#btnSubmitLogin").click();
            }
        });

        $(".b-pin-element").keypress(function (e) {
            if (e.keyCode == 13) {
                $("#btnSubmitUpc").click();
            }
        });

        $(".b-password-element").keypress(function (e) {
            if (e.keyCode == 13) {
                $("#btnSubmitPassword").click();
            }
        });

        $(".b-friend-element").keypress(function (e) {
            if (e.keyCode == 13) {
                $("#btnSubmitFriend").click();
            }
        });

        $(".b-reminder-element").keypress(function (e) {
            if (e.keyCode == 13) {
                $("#btnSubmitReminder").click();
            }
        });
    });    
</script>

    <!-- modal code -->
    <script type="text/javascript">
        
        function GetRegisterData() {
            var arr = new Array();
            var formdata = new Object();

            $(".b-register-element").each(function (i) {
                formdata = new Object();
                formdata.Element = this.id;
                formdata.Content = $(this).val();

                if ($(this).is(':checkbox') == true) {
                    formdata.Content = $(this).is(':checked');
                }
                arr[i] = formdata;
            });
            return arr;
        }

//        function LogError(error, thrownError) {
//            $.ajax({
//                type: "POST",
//                url: '../JQuery.asmx/LogError',
//                data: "{strError: '" + error.responseText) + "', strThrownError: '" + thrownError + "'}",
//                contentType: "application/json; charset=utf-8",
//                dataType: "json",
//                success: function () {
//                }
//            });
//        }
    </script>
    <!-- end modal code -->

    <!-- modal window functions -->
    <script type="text/javascript">
        $(document).ready(function () {
            // submit your login credentials to attempt a login
            $("#btnSubmitLogin").click(function () {

                $("#btnSubmitLogin").hide();
                $("#imgLoading").show();

                var arr = new Array();
                var formdata = new Object();

                $(".b-login-element").each(function (i) {
                    formdata = new Object();
                    // formdata.Element = this.id.replace("ucModal_i", "");
                    formdata.Element = this.id;
                    formdata.Content = $(this).val();
                    arr[i] = formdata;
                });

                var data = new Object();
                data.fd = arr;

                $.ajax({
                    type: "POST",
                    url: "../JQuery.asmx/Login",
                    data: JSON.stringify(data),
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    error: function (xhr, ajaxOptions, thrownError) {
                        $("#btnSubmitLogin").show();
                        $("#imgLoading").hide();
                    },
                    success: function (data) {

                        var obj = $.parseJSON(data.d); // '{"name":"John"}'

                        // clear all errors
                        var theStatus = "success";
                        var humanId = null;
                        $(".intro-login-standard").addClass("display_code");
                        $(".error-login-password").addClass("display_code");
                        $(".error-login-standard").addClass("display_code");

                        for (var i = 0; i < obj.length; i++) {
                            if (obj[i].Element == "Status") {
                                theStatus = obj[i].Status;
                                if (obj[i].Status == "data") {
                                    $(".error-login-standard").removeClass("display_code");
                                }
                                else if (obj[i].Status == "password") {
                                    $(".error-login-password").removeClass("display_code");
                                    //window.location.href = "login-invalid.aspx";
                                }
                            }
                            else if (obj[i].Element == "Human") {
                                $("#iHumanId").val(obj[i].Content);
                            }
                            else {
                                // remove any previous errors
                                $("#il" + obj[i].Element).removeClass("error");
                                $("#ll" + obj[i].Element).removeClass("error");

                                if (obj[i].Status == "False") {
                                    $("#il" + obj[i].Element).addClass("error");
                                    $("#ll" + obj[i].Element).addClass("error");
                                }
                            }
                        }

                        if (theStatus == "success") {
                            window.location.href = "login-complete.aspx";
                        }

                        if (theStatus == "repeat") {
                            window.location.href = "login-complete.aspx";
                        }

                        if (theStatus == "not-registered") {

                            window.location.href = "register-invalid.aspx";
                        }

                        if (theStatus == "first-login") {
                            window.location.href = "register-confirm.aspx?h=" + $("#iHumanId").val();
                        }

                        $("#btnSubmitLogin").show();
                        $("#imgLoading").hide();
                    }
                });
            });


            // submit your registration information to attempt contest entry
            $("#ucRegister_btnSubmitRegister").click(function () {

                $("#ucRegister_btnSubmitRegister").hide();
                $("#imgLoading").show();

                var data = new Object();
                data.fd = GetRegisterData();

                $.ajax({
                    type: "POST",
                    url: "../JQuery.asmx/Register",
                    data: JSON.stringify(data),
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    error: function (xhr, ajaxOptions, thrownError) {
                        $("#ucRegister_btnSubmitRegister").show();
                        $("#imgLoading").hide();
                    },
                    success: function (data) {
                        var obj = $.parseJSON(data.d); // '{"name":"John"}'

                        // clear all errors
                        var theStatus = "success";
                        $(".intro-register-standard").addClass("display_code");
                        $(".error-register-system").addClass("display_code");
                        $(".error-register-standard").addClass("display_code");

                        for (var i = 0; i < obj.length; i++) {

                            if (obj[i].Element == "Status") {
                                theStatus = obj[i].Status;
                                if (obj[i].Status == "data") {
                                    $(".error-register-standard").removeClass("display_code");
                                }
                                else if (obj[i].Status == "system") {
                                    $(".error-register-system").removeClass("display_code");
                                }
                            }
                            else {
                                // remove any previous errors
                                $("#i" + obj[i].Element).removeClass("error");
                                $("#l" + obj[i].Element).removeClass("error");

                                if (obj[i].Status == "False") {
                                    $("#i" + obj[i].Element).addClass("error");
                                    $("#l" + obj[i].Element).addClass("error");
                                }
                            }
                        }

                        if (theStatus == "success") // User is registered successfully
                        {
                            window.location.href = "register-complete.aspx";
                        }

                        if (theStatus == "repeat") // User has already registered
                        {
                            window.location.href = "register-repeat.aspx";
                        }

                        $("#ucRegister_btnSubmitRegister").show();
                        $("#imgLoading").hide();
                        $(document).scrollTop($("#bform_wrapper_1").offset().top);
                    }
                });
            });


            // submit the password to the forgot password function
            $("#btnSubmitPassword").click(function () {

                $("#btnSubmitPassword").hide();
                $("#imgLoading").show();

                $.ajax({
                    type: "POST",
                    url: '../JQuery.asmx/ForgotPassword',
                    data: "{strEmailAddress: '" + $("#iPasswordEmailAddress").val() + "', strLanguage: '" + $("#iPasswordLanguage").val() + "'}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (msg) {
                        if (msg.d == "error") {
                            $(".intro-password-standard").addClass("display_code");
                            $(".error-password-standard").removeClass("display_code");
                            $("#iPasswordEmailAddress").addClass("error");
                        }
                        else if (msg.d == "not-registered") {
                            window.location.href = "register-invalid.aspx";
                        }
                        else {
                            window.location.href = "password-thanks.aspx";
                        }

                        $("#btnSubmitPassword").show();
                        $("#imgLoading").hide();

                    }
                });
            });

            // submit the password to the forgot password function
            $("#btnSubmitMoment").click(function () {

                $("#btnSubmitMoment").hide();
                $("#imgLoading").show();

                $.ajax({
                    type: "POST",
                    url: '../JQuery.asmx/AddMoment',
                    data: "{strMomentId: '" + $("#imMomentId").val() + "', strHumanId: '" + $("#hdHumanId").val() + "'}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (msg) {
                        if (msg.d == "error") {
                            $(".intro-moment-standard").addClass("display_code");
                            $(".error-moment-standard").removeClass("display_code");
                            $("#imMomentId").addClass("error");
                        }
                        else {
                            window.location.href = "moment-complete.aspx";
                        }

                        $("#btnSubmitMoment").show();
                        $("#imgLoading").hide();

                    }
                });
            });

            // submit the password to the forgot password function
            $("#btnSubmitVote").click(function () {

                $("#btnSubmitVote").hide();
                $("#imgVoteLoading").show();

                var pollAnswerId = '';

                $(".b-vote-element").each(function (i) {

                    if ($(this).is(':checked') == true) {
                        pollAnswerId = $(this).val()
                    }
                });

                if (pollAnswerId == '') {

                    $("#imgVoteLoading").hide();
                    $("#lblErrorMessage").removeClass("hide");
                    $("#btnSubmitVote").show();
                    return;
                }

                $.ajax({
                    type: "POST",
                    url: '../JQuery.asmx/AddVote',
                    data: "{strPollId: '" + $("#ucBonus_ucPoll_hdPollId").val() + "', strPollAnswerId: '" + pollAnswerId + "', strHumanId: '" + $("#ucBonus_ucPoll_hdHumanId").val() + "'}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (msg) {
                        $("#imgVoteLoading").hide();
                        $("#divVoteQuestion").hide();
                        $("#divVoteResults").show();

                    }
                });
            });

        });
    </script>
    <!-- end modal window functions -->

      
    
    <script type="text/javascript">
        $(document).ready(function () {
            $("#imgLoading").hide();
        });
    </script> 

    
 <script type="text/javascript">
     $(document).ready(function () {
         $("#imgVoteLoading").hide();
     });
    </script>
    

