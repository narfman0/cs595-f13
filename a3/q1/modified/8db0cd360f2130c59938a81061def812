<!-- code to execute customer FB share button if placed on page -->
<script type="text/javascript">
    function facebook_click() {
        _gaq.push(['_trackPageview', 'external/facebook-share']);
        u = 'http://pintyswingman.com/en/default.aspx'; // this is the url you want to share - you can hardcode this to whatever you want
        t = ''; // this is the title you want to display during the facebook share
        f = 'https://www.facebook.com/sharer.php?u=' + encodeURIComponent(u) + '&t=' + encodeURIComponent(t);
        window.open(f, 'sharer', 'toolbar=0,status=0,width=626,height=436');
        return false;
    }
</script>
<!-- Twitter -->
    <script src="https://platform.twitter.com/widgets.js" type="text/javascript"></script>
    <script type="text/javascript">
        function twitter_click() {
            _gaq.push(['_trackPageview', 'external/twitter-share']);
            u = 'http://pintyswingman.com'; // this is the url you want to share - you can hardcode this to whatever you want
            t = 'I just entered the @pintysfoods Epic Wingman Weekend Contest for a chance to win a trip to see NASCAR in Las Vegas'; // this is the title you want to display during the facebook share
            f = 'https://twitter.com/share?text=' + encodeURIComponent(t) + '&url=' + encodeURIComponent(u);
            window.open(f, 'twitsharer', 'toolbar=0,status=0,width=626,height=436');
            return false;
        }
    </script>
<!-- Google + -->
<script type="text/javascript">
    function google_click() {
        _gaq.push(['_trackPageview', 'external/google-share']);
        u = 'http://pintyswingman.com'; // this is the url you want to share - you can hardcode this to whatever you want
        f = 'https://plus.google.com/share?url=' + encodeURIComponent(u);
        window.open(f, '', 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=600,width=600');
        return false;
    }
</script>

    

