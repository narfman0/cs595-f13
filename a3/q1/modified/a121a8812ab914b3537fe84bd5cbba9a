<script type="text/javascript">

	(function($){  $.fn.scrollNongol = function() {  

		var element = this;
		var keadaan = 'ilang'; //state awal
				
			$(window).bind('scroll', function(){
			
				var posisi_sekarang = $(this).scrollTop();
				var posisi_trigger =  $(document).height()/4;
			
				if (posisi_sekarang > posisi_trigger && keadaan == 'ilang') { 
					element.animate({	right : '0',	});
					keadaan = 'nongol';
				}
				
				else if(posisi_sekarang < posisi_trigger && keadaan == 'nongol') {
					element.animate({	right : '-410px',});
					keadaan = 'ilang';
				}

			});
			
		$('.closeBtn' , this).bind('click' , function(){
			element.animate({	right : '-410px',	} , function(){
				element.hide();
			});
		});		
			
	  };
	})(jQuery);
	
	$(document).ready(function(){
		$('#articleFloat').scrollNongol();
	});
	
</script>
