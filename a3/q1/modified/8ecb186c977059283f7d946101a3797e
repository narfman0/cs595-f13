<script type="text/javascript">	
	window.onload = function () {	
		var scriptSrc = '/ams/page-ads.js?ad_category_prefix=holiday&ad_sub_category_prefix=halloween&ams_promo=&amz_browse_node_ids=10304191&amz_search_indexes=Miscellaneous&article_type_prefix=blog_entry_lite&browser_path=%2Fnews%2Ffashion-style%2Fcharlotte-olympia-halloween-accessories%3Fsrc%3Dspr_TWITTER%26spr_id%3D1448_24327573%266411572%3D1&cat_prefixes=&keywords=&position_list=ams_circ_elm_300x250_footer%2Cams_circ_elm_620x200%2Cams_circ_elm_arrival%2Cams_circ_elm_persistent_footer%2Cams_circ_elm_sub_nav_link%2Cams_digital_elm_300x250_footer%2Cams_digital_elm_foot_link%2Cams_digital_elm_nav_link%2Cams_elm_circ_takeover%2Cams_elm_house%2Cams_elm_like_gate%2Cams_elm_smartout&section_prefix=fashion-style&site_prefix=elle&sub_cat_prefixes=nav-promo%2Ccat-fashion-news-blog&subdomain=www&url_name=charlotte-olympia-halloween-accessories';

		if (origRefer === null) {
			origRefer = document.referrer;
			scriptSrc = scriptSrc + '&original_referrer=' + origRefer;
		}
		scriptSrc += '&screen_height=' + getHeight();
		scriptSrc += '&screen_width=' + getWidth();

		var pageAdsScript = {
			url: scriptSrc,
			jsonpCallback: 'HDM.ads.getPageAds',
			cache: true,
			dataType: 'jsonp'
		};
		HDM.DeferredScriptQueue.stack.unshift(pageAdsScript);
		HDM.DeferredScriptQueue.process();
	}
</script>
