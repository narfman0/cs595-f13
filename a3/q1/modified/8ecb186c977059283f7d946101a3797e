<script type="text/javascript">
$(window).load(function(){

	function buildScriptTag(src,success,fail){
		var script = document.createElement('script');
		var timeoutid;
		function runSuccess(obj){
			clearTimeout(timeoutid);
			try{
				success(this);
			} catch(e){}
		}
		function runFail(){
			fail(this);
		}
		// prelim check to see if script tag already exists...
		for (var i = 0,s = document.getElementsByTagName("script"); i < s.length; i++){
			if(s[i].src && s[i].src == src){
				runFail();
				console.warn(s[i],"detected - skipping CA call")
				return;
			}
		}
		script.src = src;
		if(script.addEventListener) {
			script.addEventListener("load",function(){runSuccess(this);},false);
		}else if(script.readyState) {
			script.onreadystatechange = function(){runSuccess(this);};
		}
		script.onerror = function(){
			clearTimeout(timeoutid);
			runFail();
		}

		timeoutid = setTimeout(runFail,5000);
		document.getElementsByTagName("HEAD")[0].appendChild(script);
	}


	// define runme
	var runme = function(){
		if (typeof gptLayer == "object"){

			var gptUnique  = [];
			// cull duplicates
			for (var j = 0;j < gptLayer.length; j++){
				console.log(gptLayer[j])
				var elementId = gptLayer[j].id;
				var found = false;
				for (var i = 0; i <gptUnique.length; i++){
					if (gptUnique[i].id == elementId){
						found = true;
						console.error("GPT duplicate entry found:",gptUnique[i].id)
					}
				}
				if (!found){
					gptUnique.push(gptLayer[j]);
				}
			}
			gptLayer = gptUnique;


			gptLayer.zone="/4328/hdm.elle/" + ad_zone; // Set zone for page
			googletag.cmd.push(function() {
				gptLayer.refreshArray = [];
				try{
					for (i=0;i<gptLayer.length;i++){ // Define every ad in gptLayer
						if (gptLayer[i].outofpage) {
							googletag.defineOutOfPageSlot(gptLayer.zone, gptLayer[i].id).addService(googletag.pubads());
						} else if (gptLayer[i].refresh) {
							var slot = googletag.defineSlot(gptUnique.zone, gptUnique[i].adslot,gptUnique[i].id);
							slot.setTargeting("pos", gptUnique[i].pos);
							slot.addService(googletag.pubads());
							gptUnique[i].slot=slot
							gptUnique.refreshArray.push(slot);
						} else {
							googletag.defineSlot(gptLayer.zone, gptLayer[i].adslot,gptLayer[i].id).setTargeting("pos", gptLayer[i].pos).addService(googletag.pubads());
						}
						console.log (gptLayer.zone+":"+gptLayer[i].adslot+":"+gptLayer[i].id);
					} 
				} catch(e){
					console.error(e);
				} finally {
					gptLayer.refresh=function(){googletag.pubads().refresh(gptLayer.refreshArray);}
					// Set Page Level Targeting Here
					googletag.pubads().setTargeting("site",["elle"]); 
					googletag.pubads().setTargeting("sect",["news"]);
					googletag.pubads().setTargeting("sub",["fashion-style"]); 
					googletag.pubads().setTargeting("subsub",["index"]); 
					googletag.pubads().setTargeting("page",["charlotte-olympia-halloween-accessories"]); 
					googletag.pubads().setTargeting("cat",["holiday"]); 
					googletag.pubads().setTargeting("subcat",["halloween"]); 
					googletag.pubads().setTargeting("tool",["blog_entry_lite"]); 
					googletag.pubads().setTargeting("artid",["15962386"]); 
					googletag.pubads().setTargeting("kw",[""]); 
					googletag.pubads().setTargeting("epic",[epicKV]); 
					googletag.pubads().setTargeting("refer",[ad_refer]); 
					googletag.pubads().setTargeting("cas",caSegID); 
					googletag.pubads().setTargeting("rsi",segQS); 
					googletag.pubads().set("page_url", window.location.protocol + "//" + window.location.hostname + window.location.pathname);
					// Call to DFP
					googletag.pubads().collapseEmptyDivs(true); // Collapses ad div if no ad is returned
					googletag.pubads().enableSingleRequest();
					googletag.enableServices();					
				}
			});
			// Push ad content to divs
			googletag.cmd.push(function() { 
				for (i=0;i<gptLayer.length;i++) {
				//	if (!gptLayer[i].outofpage) {
					googletag.display(gptLayer[i].id);
					console.log ("Push: "+ gptLayer[i].id);
				//	}
				}
			});
		} else {
			throw "gptLayer object not found!";
		}
	} // runme

