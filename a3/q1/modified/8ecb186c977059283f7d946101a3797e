	<!-- Deferred Scripts -->
	<script type="text/javascript">
		$( window ).on('load', function(){
			var Site = {
				articleID: 15962386,
				adRefreshInterval: 1,
				pageAdsParams: pageAdsParams,
				facebookAppID: 134662310036632
			};

			//glo module url.. needed in elle/global.js
			window.gloURl = "/wd-glofeed-msn/rss/general.xml";
			
			//initialize hdm-lib.. which kicks off registration			
			HDM.init( Site );

			HDM.scriptList = [
				//load the social tools and tracking
				'/cm/shared/social/social_tools.js',
				//load global.js
				'/cm/elle/scripts/global.js',
				{
					load: [
						'//admin.brightcove.com/js/BrightcoveExperiences.js',
						'//admin.brightcove.com/js/APIModules_all.js'
					],
					complete: function(){
						brightcove.createExperiences();
					}
				}
			];

			

			

			

			

			

			

			

			

			

			

			

			

			

			

			

			//load Google+ and Pinterest
			HDM.scriptList.push({
				load: 'https://apis.google.com/js/plusone.js',
				complete: function(){
					var script = document.createElement('script');

					$('img').live('hover',function(evt){
						var img = $(this);
						var pinitButton = $('a[class*=pin_it_button_floating]');
						if(pinitButton != null && pinitButton != undefined){
							pinitButton.hover(function(){
								var pinUrl = $(this).attr('href');//get current href of the pinit button
								var imgTitle = img.attr('title');//get the title of  the image
								if(imgTitle == undefined || imgTitle === ''){
									//remove the description of the pinit button if the images don't have title.
									var newUrl = pinUrl.substring(0,pinUrl.indexOf('description')) + "description=";
									$(this).attr({'href' : newUrl});
								}
							});	
						}
					});

					script.type = "text/javascript";
					script.src = "//assets.pinterest.com/js/pinit.js";
					script.setAttribute('data-pin-hover','true');

					document.getElementsByTagName('head')[0].appendChild( script );
				}
			});

			Modernizr.load( HDM.scriptList );
		});
	</script>
	
