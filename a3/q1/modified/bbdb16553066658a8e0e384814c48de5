<script type="text/javascript">
$(function() {
  // Font-size dinamico
  $("#bt_font_mas").click(function() { $("#noticia_texto span").ampliarFont(); });
  $("#bt_font_mns").click(function() { $("#noticia_texto span").reducirFont(); });

  // Especiales Acordión
  $("#especiales_acordion").accordion({ icons:false, fillSpace:true });

  // Lightbox
  $(".lightbox").lightbox();

  // Slide videos
  $(".bt_extra2").click(function() {
    var clave = this.rel;
    var video = $(this).attr("data-rel");
    lightbox_video(clave,video,"");
  });

  // Slide galeria
  $(".bt_extra1").click(function() {
    var clave = this.rel;
    var fotos = JSON.parse($(this).attr("data-rel"));
    lightbox_galeria(clave,fotos,"");
  });

  // Selección de codigo link
  $(".cnt_comp_tags>div:last-child input").click(function() { $(this).select(); });

  // Eventons del text nombre
  if($("#coment_nombre").val()!=="Escribe un nombre") { $("#coment_nombre").css("color","#333"); }
  else if($("#coment_nombre").val()=="") { $("#coment_nombre").val("Escribe un nombre").css("color","#CCC"); }

  if($("#coment_texto").val()!=="Escribe tu mensaje"){ $("#coment_texto").css("color","#333"); }
  else if($("#coment_texto").val()=="") { $("#coment_texto").val("Escribe tu mensaje").css("color","#CCC"); }

  $("#coment_nombre").bind("focus blur",function() {
    var v = $(this).val();
    if(v=="Escribe un nombre") { $(this).val("").css("color","#333"); }
    else if(v=="") { $(this).val("Escribe un nombre").css("color","#CCC"); }
  });

  $("#coment_texto").bind("focus blur",function() {
    var v = $(this).val();
    if(v=="Escribe tu mensaje") { $(this).val("").css("color","#333"); }
    else if(v=="") { $(this).val("Escribe tu mensaje").css("color","#CCC"); }
  });

  // Eventos de envío
  $("#coment_envio").click(function() {
    var clv = $("#coment_clave").val();
    var nmb = $("#coment_nombre").val();
    var txt = $("#coment_texto").val();

    if(nmb!=="" && nmb!==" " && nmb!==undefined && nmb!==null) {
      if(!(/^[a-z A-Z á é í ó ú ñ ü Á É Í Ó Ú Ñ Ü ]{3,}$/.test(nmb))) {
        alert("El campo nombre debe incluir sólo letras, mínimo 3 caracteres.");
        $("#coment_nombre").focus();
        return false;
      }
    }
    else if(nmb=="" && nmb==" ") {
      alert("El campo nombre está vacío...");
      $("#coment_nombre").focus();
      return false;
    }

    if(nmb=="Escribe un nombre") {
      alert("Debes poner tu nombre en el campo");
      $("#coment_nombre").focus();
      return false;
    }

    if(txt!=="" && txt!==" " && txt!==undefined && txt!==null) {
      if(!(/^[a-z A-Z 0-9 á é í ó ú ü ñ Á É Í Ó Ú Ñ Ü @ \. \, ; : _ \\ \= \- \' \" \/ \@ \¡ \! \¿ \?]{5,}$/.test(txt))) {
        alert("El campo comentario debe incluir sólo caracteres y números válidos, mínimo 5 caracteres.");
        $("#coment_texto").focus();
        return false;
      }
    }
    else if(txt=="" && txt==" ") {
      alert("El campo comentario está vacío...");
      $("#coment_texto").focus();
      return false;
    }

    if(txt=="Escribe tu mensaje") {
      alert("Debes de poner un comentario en el campo");
      $("#coment_texto").focus();
      return false;
    }

    $.ajax({ url:"../includes/servicios/comentarios_guardar_comentario.php", type: "POST", data:{ clv:clv, nmb:nmb, txt:txt, tip:1},
      success: function(d) {
        var msj = "";
        $("#coment_nombre").empty().val("Escribe un nombre").css("color","#CCC");
        $("#coment_texto").empty().val("Escribe tu mensaje").css("color","#CCC");

        if(d!==undefined && d!=="" && d!==null) msj=d;
        else {
          var nobj = $(".aux_coment_item").clone();
          $(".notas_comntarios").after(nobj.attr({"id":"aux_coment_item","class":"coment_item"}).removeClass("aux_coment_item"));
          $("#aux_coment_item .fecha_comentario").text("Por aprobar - "+$.datepicker.formatDate('yy-m-dd', new Date()));
          $("#aux_coment_item .nombre_comentario").text(nmb);
          $("#aux_coment_item .text_comentario").text(txt);
          $("#aux_coment_item").fadeIn(2000,function(){ $("#aux_coment_item").removeAttr("id"); });
          msj = "Tu comentario fue agregado correctamente, <br/><b>Gracias</b>.";
        }
        $("<div>"+msj+"</div>").dialog({ title: "Aviso" });
      }
    });
  });

  $("#coment_terminos").click(function() {
    $(".contenedores_terminos").dialog({ title:"Terminos de uso", width:500, resize:false, modal:true });
  });

  // Eventos botón coment
  $(".noticia_tcoment").click(function() {
    var y = $(".notas_comntarios").position();
    $('html, body').animate({ scrollTop: y.top }, 'slow');
  });
});
</script>
