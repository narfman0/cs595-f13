e.display = 'none';   }      document.onclick = onBlurSearchResults;     function makeSearchChoice(chosen,urlTitle) {    /* added catch to remove category name from search term */    if ( urlTitle.toLowerCase().search('<a>') != -1 ) {     var endPoint = urlTitle.toLowerCase().search('<a>');     var urlTitle = urlTitle.substr(0, endPoint-1);    }    /*var thisDiv = (evt) ? evt.target : window.event.srcElement; */    document.getElementById("popupsSearch").innerHTML = "";    /*var titleURL = urlTitle.replace(/\&amp;/g,'&');    var titleURL = titleURL.replace(/[^0-9a-zA-Z]/g,'-');    window.location="/page/local.headDetail/id/" + chosen + "/" + titleURL + ".html";*/    window.location="/page/local.searchBoxCatchAutocomplete/id/" + chosen + "/text/" + urlTitle;      document.getElementById("popupsSearch").style.display = "none";    /*document.getElementById('searchBox').value = thisDiv.innerHTML.replace(/\&amp;/g,'&'); */    /*document.getElementById("popupsSearch").innerHTML = ""; */   }     </script>     <div id="searchHeaderClass" class="cBdrHdr">     <div class="cBdrTLOn">&nbsp;</div>     <div class="cBdrTROn">&nbsp;</div>     <div class="cBdrTC"><h2 class="color_lBlue">EZToUse.com</h2></div>    </div>    <div class="cBdrMain">     <form id="frmEzSearch" action="http://altoona.eztouse.com/searchname.asp" method="get" enctype="application/x-www-form-urlencoded" name="NameSearch" target="_blank" class="odistyle28">      <h5>I am looking for:</h5>      <div><input name="name" id="ezSearchBox" value="" autocomplete="off" type="text" size="24" maxlength="128" /> <input name="btnEzSearch" type="submit" value="Search" class="fButton" /></div>     </form>     <div><a href="http://altoona.eztouse.com/" title="Visit EZToUse.com" target="_blank"><img src="/shared/ads/ez/EZToUse300x50.gif" border="0" /></a></div>    </div>    <div class="cBdrFdr">     <div class="cBdrBL"><img src="/images/spacer.gif" border="0" width="1" height="1" /></div>     <div class="cBdrBR"><img src="/im