   /* code for IE7+, Firefox, Chrome, Opera, Safari */      return new XMLHttpRequest();    }    if (window.ActiveXObject) {      /* code for IE6, IE5 */      return new ActiveXObject('Microsoft.XMLHTTP');    }    return null;   }     function setnotificationTitleArray() {    if (xmlhttpNotificationCenter.readyState == 4) {     if (xmlhttpNotificationCenter.status == 200) {      if (xmlhttpNotificationCenter.responseXML) {       var allNotifications = xmlhttpNotificationCenter.responseXML.getElementsByTagName("note");       for (var i=0; i<allNotifications.length; i++) {        var curSortType = allNotifications[i].getElementsByTagName("sortType")[0].firstChild.nodeValue;        if ( curSortType == 'Latest' ) {         var curID = allNotifications[i].getElementsByTagName("FK_noteID")[0].firstChild.nodeValue;         var curTitle = allNotifications[i].getElementsByTagName("title")[0].firstChild.nodeValue;         notificationTitleArray[i] = curTitle;         notificationURLArray[i] = '/page/notificationCenter.click?noteID='+curID;        } else if ( curSortType == 'Most Clicked' ) {         var curID = allNotifications[i].getElementsByTagName("FK_contentID")[0].firstChild.nodeValue;         var curTitle = allNotifications[i].getElementsByTagName("title")[0].firstChild.nodeValue;         var curCatID = allNotifications[i].getElementsByTagName("FK_catID")[0].firstChild.nodeValue;         notificationTitleArray[i] = curTitle;         notificationURLArray[i] = '/page/content.detail/id/'+curID+'/'+curTitle.replace(/[^0-9a-zA-Z]/g,'-')+'.html?nav='+curCatID;        } else if ( curSortType == 'Most Commented Article' ) {         var curID = allNotifications[i].getElementsByTagName("FK_contentID")[0].firstChild.nodeValue;         var curTitle = allNotifications[i].getElementsByTagName("title")[0].firstChild.nodeValue;         var curCatID = allNotifications[i].getElementsByTagName("FK_catID")[0].firstChild.nodeValue;         notificationTitleArray[i] = curTitle;         notificationURLArray[i] = '/page/content.detail/id/'+curID+'/'+curTitle.replace(/[^0-9a-zA-Z]/g,'-')+'.html?nav='+curCatID;        } else if ( curSortType == 'Most Commented Poll' ) {         var curID = allNotifications[i].getElementsByTagName("FK_pollID")[0].firstChild.nodeValue;         var curTitle = allNotifications[i].getElementsByTagName("title")[0].firstChild.nodeValue;         notificationTitleArray[i] = curTitle;         notificationURLArray[i] = '/page/polls.detail/id/'+curID+'/'+curTitle.replace(/[^0-9a-zA-Z]/g,'-')+'.html';        } else if ( curSortType == 'Most Commented Topic' ) {         var curID = allNotifications[i].getElementsByTagName("FK_topicID")[0].firstChild.nodeValue;         var curTitle = allNotifications[i].getElementsByTagName("title")[0].firstChild.nodeValue;         notificationTitleArray[i] = curTitle;         notificationURLArray[i] = '/page/mboard.detail/fNav/'+curID+'/'+curTitle.replace(/[^0-9a-zA-Z]/g,'-')+'.html';        } else if ( curSortType == 'Most Commented Blog' ) {         var curID = allNotifications[i].getElementsByTagName("FK_blogID")[0].firstChild.nodeValue;         var curTitle = allNotifications[i].getElementsByTagName("title")[0].firstChild.nodeValue;         notificationTitleArray[i] = curTitle;         notificationURLArray[i] = '/page/blogs.detail/display/'+curID+'/'+curTitle.replace(/[^0-9a-zA-Z]/g,'-')+'.html';        } else {         notificationTitleArray[i] = '';         notificationURLArray[i] = '';        }       }       setNotification(0, rotateNotifications);      }     }    }   }         function rotateNotifications() {    setTimeout('setNotification("'+nextNotification+'",rotateNotifications)', notificationCenterDelay);    nextNotification++;    if ( nextNotification > notificationTitleArray.length -1 ) {     nextNotification = 0;    }   }       function setNotification(chosen,functionName) {    document.getElementById('notificationNumber').innerHTML = (parseInt(chosen)+1)+') ';    document.getElementById('notificationTitle').innerHTML = notificationTitleArray[chosen];    document.getElementById('notificationURL').href = notificationURLArray[chosen];    document.getElementById('notificationURL').title = notificationTitleArray[chosen];    functionName();   }  </script>      </div>   <div class="clearR"></div>  </div> 
<script language="javascript">

	var showMobileAd = 0;
	
	/* check for apple product */
	var apple = {};
	apple.userAgent = navigator.userAgent;
	apple.device = false;
	apple.products = ["iPhone", "iPod", "iPad"];
	for (var d = 0; d < apple.products.length; d++) {
		var t = apple.products[d];
		apple[t] = !!apple.userAgent.match(new RegExp(t, "i"));
		apple.device = apple.device || apple[t];
	}
	
	
	
	/* cookie manipulation */
	function getCookie(cookieName) {
		var i,x,y,ARRcookies=document.cookie.split(";");
		for ( i=0; i<ARRcookies.length; i++ ) {
			x = ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
			y = ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
			x = x.replace(/^\s+|\s+$/g,"");
			if ( x == cookieName ) {
				return unescape(y);
			}
		}
	}
	 
	function setCookie(cookieName,value,exdays) {
		var cookieName = 'mobileIphoneAppAdClose';
		var expireDate = new Date();
		expireDate.setDate(expireDate.getDate() + exdays);
		/* expireDate.setTime(expireDate.getTime() + 60000); for testing - set to 1 minutes */
		var cookieValue=escape(value) + ((exdays==null) ? "" : "; expires="+expireDate.toUTCString());
		document.cookie = cookieName + "=" + cookieValue;
	}
	/* END cookie manipulation */
	 
 

	if ( apple.device ) {
		var checkForCookie = getCookie("mobileIphoneAppAdClose");
		if (checkForCookie!=null && checkForCookie!="") {
			showMobileAd = 0;
		} else {
			showMobileAd = 1;
		}
	}
	
	function closeAd(){
		setCookie("mobileIphoneAppAdClose",'adClosed',14);
	}


	/* create display ad */
	if ( showMobileAd ) {
		var msg = "\n Enjoy a better mobile experience by downloading the free \'The Altoona Mirror\' app today! Would you like to download the app now? \n\n";
		if (confirm(msg)) {
                                                  closeAd();
			location.replace("/iPhone");
		} else {
			closeAd();
		}
	}
</script>































<script language="javascript">

	var showMobileAndroidAd = 0;
	
	/* check for apple product */
	var android = {};
	android.userAgent = navigator.userAgent;
	android.device = false;
	android.products = ["android"];
	for (var d = 0; d < android.products.length; d++) {
		var t = android.products[d];
		android[t] = !!android.userAgent.match(new RegExp(t, "i"));
		android.device = android.device || android[t];
	}
	
	
	
	/* cookie manipulation */
	function getCookie(cookieName) {
		var i,x,y,ARRcookies=document.cookie.split(";");
		for ( i=0; i<ARRcookies.length; i++ ) {
			x = ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
			y = ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
			x = x.replace(/^\s+|\s+$/g,"");
			if ( x == cookieName ) {
				return unescape(y);
			}
		}
	}
	 
	function setCookie(cookieName,value,exdays) {
		var cookieName = 'mobileAndroidAppAdClose';
		var expireDate = new Date();
		expireDate.setDate(expireDate.getDate() + exdays);
		/* expireDate.setTime(expireDate.getTime() + 60000); for testing - set to 1 minutes */
		var cookieValue=escape(value) + ((exdays==null) ? "" : "; expires="+expireDate.toUTCString());
		document.cookie = cookieName + "=" + cookieValue;
	}
	/* END cookie manipulation */
	 
 

	if ( android.device ) {
		var checkForCookie = getCookie("mobileAndroidAppAdClose");
		if (checkForCookie!=null && checkForCookie!="") {
			showMobileAd = 0;
		} else {
			showMobileAd = 1;
		}
	}
	
	function closeAd(){
		setCookie("mobileAndroidAppAdClose",'adClosed',14);
	}


	/* create display ad */
	if ( showMobileAd ) {
		var msg = "\n Enjoy a better mobile experience by downloading the free \'The Altoona Mirror\' app today! Would you like to download the app now? \n\n";
		if (confirm(msg)) {
			closeAd();
			location.replace("/Android");
		} else {
			closeAd();
		}
	}
</script>

