$(document).ready(function(){
extendedForecastURL = 'http://www.manchestereveningnews.co.uk/weather/';
iconURL = 'http://s.manchestereveningnews.co.uk/skins/shared/gfx/widgets/weather/';
ajaxURL = 'http://www.manchestereveningnews.co.uk/weather/?service=ajax&item=weather';
$.ajax({
url: ajaxURL,
dataType: 'json',
success: function(forecast) {
var linkURL = 'http://www.manchestereveningnews.co.uk/weather/';
$("#weather-hdr-icon-link").attr("href", linkURL);
$("#weather-hdr-icon-image").attr("src", iconURL + forecast.conditions.weatherIcon + ".png");
$("#weather-hdr-temp a").attr("href", linkURL);
$("#weather-hdr-desc a").attr("href", linkURL);
$("#weather-hdr-5 a").attr("href", linkURL);
