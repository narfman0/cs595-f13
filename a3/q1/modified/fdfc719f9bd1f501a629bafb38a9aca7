    <script language="JavaScript">
pcnt = 94;

front_img = new Image();
inside_img = new Image();
back_img = new Image();

var oset = 0;
var orientation_style = '7" &#215; 5"';
var fb_link_def = 'http://www.greetingcarduniverse.com'+'/community/viewdetails.asp';

function img_change(img_name, img_label) {
    if (document.images) {
        $("image1").src = img_name;
        reset_label();
        $(img_label+"_label").className = 'join';
        if (img_label == "front_img") {
            $("image2").style.visibility = "visible";
        }
        else {
            $("image2").style.visibility = "hidden";
        }

    }
}

function reset_label() {
    $("front_img_label").className = '';
    $("inside_img_label").className = '';
    $("back_img_label").className = '';
}

function add_to_cart(cart_link, m) {
    xw = window_param(1);

    if (m == 'b') sy = window_param(4);
    else sy = 0;

    xx = (xw - 400)/2;
    yy = sy + 100;

    showMe(cart_link, 400, 230, xx, yy, 'Add to Cart');
}

function show_cards(div_id, pid, cid, cmd) {
    if (!div_id) return false;

    var vals = '';
    vals += 'cmd='+cmd;
    if (pid > 0) vals += '&pid=' + pid;
    if (cid > 0) vals += '&cid=' + cid;

    var $ref = '';
    var $art_id = '0';

    if ($ref) vals += '&ref='+escape($ref);
    if ($art_id) vals += '&aid=' + $art_id;

    if (cmd == 'nav') {
        vals += '&pcnt=94';
        vals += '&off=' + oset;
        vals += '&pa=';

        var $featured = 0;

        if ($featured) vals += '&f=1';
    }

    vals += '&ts='+ new Date().getTime();
    var do_tabs = true;

    new Ajax.Updater(
        'd_'+div_id,
        '/community/card_details.asp',
        {   method: 'get',
            parameters: vals,
            evalScripts: true,
            onComplete: function(req) {
                if (div_id == 'card_title')
                  d.title = d.getElementsByClassName('h1bd')[0].innerHTML;
                if (div_id == 'card_details') {
                    var uatw = getUA();
                    var uapi = getUA();

                    uatw.open('GET', '/js/twitter.js', true);
                    uatw.onreadystatechange = function() {
                        if (uatw.readyState == 4) {
                            try { eval(uatw.responseText); }
                            catch(e) {
                            }
                        }
                    };
                    uatw.send(null);

                }
                tabsInit();
            }
        }
    );

    return false;

}

function show_me(pid, cid, aid) {
    show_cards("card_details", pid, cid, "show");
    show_cards("card_title", pid, cid, "title");
    show_cards("card_nav", pid, cid, "nav");
    location.replace('#product='+pid);
    load_other_cards(pid, cid, aid, 0);
}

function show_nav(pid, cid, offset) {
    if (offset > 0) oset = oset + 6;
    else oset = oset - 6;

    if (oset < 0) oset = 0;

    b = oset;
    if (b + 6 > pcnt - 1) {
        e = oset + 6 - 1;
        if (e > pcnt - 1) {
            e = pcnt - 1;
            b = e - (6 - 1);
        }
    }
    if (oset > b) oset = b;
}

function show_zoom(card_link, card_w, card_h) {
    xw = window_param(1);
    sy = window_param(4);
    xx = (xw - card_w)/2;
    yy = sy + 50;
    showMe(card_link, card_w, card_h, xx, yy, 'Large View')
}

function get_tab_content(tab_num) {
    if ($('tab' + tab_num).innerHTML == '') {
        $('tab' + tab_num).innerHTML = '<p>Loading...</p>';

        var myAjax = new Ajax.Request(
            '/community/card_details_tabs.asp',
            {
                method: 'get',
                parameters: 'ajax=true&tab_num=' + tab_num + '&orientation='
                            + escape(orientation_style) +
                            '&ts=' + new Date().getTime(),
                onComplete: function(req) {
                    $('tab' + tab_num).innerHTML = req.responseText
                }
            });
    }
}

function hover_view(cardid, card_title, pid, card_width,
                    card_hook_target, card_hook_tip) {
    new Tip(cardid, {
        title: card_title,
        target: $(cardid),
        style: 'default',
        ajax: {
            url: '/shopping/card_div.asp?pid='+pid,
            options: {
                method: 'get',
                onComplete: function(transport) { }
            }
        }
        ,
        width: card_width,
        border: 3,
        radius: 3,
        borderColor: '#849BCD',
        hook: { target: card_hook_target, tip: card_hook_tip },
        offset: { x: 0, y: -3 }});
}

function check_for_card_load() {
    if (String(location).match(/#/)) {
        var anchor = String(location).split(/#/)[1];
        if (anchor.match(/product=\d+$/)) {
            var lprod = anchor.split(/=/)[1];
            if (product.pid != lprod) show_me(lprod, product.cid, product.aid);
        }
    }
}

register(window, 'load', check_for_card_load);

    </script>

