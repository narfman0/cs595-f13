    <script>
function load_other_cards(pid, cid, aid, oset) {
if(oset == "") { oset = 0; }

var myAjax = new Ajax.Request(
    '/community/card_details.asp',
    { method: 'get',
      parameters: 'ajax=true&cmd2=get_other_cards&aid='+aid+'&pid='+pid+'&cid='+cid+'&off='+oset+'&fromsearch=false&ts='+ new Date().getTime(),
      onComplete: function(req) {
          $('nav_html_content_ajax').innerHTML = req.responseText;
          imgsize3d = Number(document.getElementById('imgsize3dvar').innerHTML);
          if (imgsize3d == 8004 || imgsize3d == 9188) regen3d();
          else if (imgsize3d == 0) window.location = '/'+pid;
      }
     });
}
    </script>

