function regen3d() {
    regenIndicatorE = document.getElementById('regenIndicator');
    regenIndicatorE.style.display = 'block';
    window.im3d = d.getElementById('image1');
    var imgurl = '/disk2/websitesbd/code/html' + im3d.src.replace(/^.*\/images/, '/images/').replace(/\/+/g, '/').replace(/_3d\.jpg/, '_print.png').replace(/\?.*$/, '');
    var ua = getUA();
    if (!ua) return uaFail();
    ua.open('GET', '/p/regen3d?img='+imgurl+'&x='+new Date().getTime(), true);
    ua.onreadystatechange = function() {
        if (ua.readyState == 4) {
            var r = eval('('+ua.responseText+')');
            if (r.error) {
                regenIndicatorE.style.display = 'none';
                alert('regen failed: ' + r.error);
                return false;
            }
            if (non3d) setTimeout('location.reload()', 5000);
            else setTimeout(reload3d, 5000);
            return true;
        }
    };
    ua.send(null);
}
function reload3d() {
    // if (!doregen) return false;
    im3d.src = im3d.src + "?x=" + new Date().getTime();
    regenIndicatorE.style.display = "none";
}
      </script>
