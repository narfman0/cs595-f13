<script language="JavaScript">
function activateMenu(menu) {
    var m = d.getElementById('menu_'+menu);
    if (m) {
        var j = eval('(' + m.getAttribute('submenu') + ')');
        var div = d.createElement('div');
        div.className = 'submenu';
        var ul = d.createElement('ul');
        for (var i = 0; i < j.length; i++) {
            var l = j[i];
            var li = d.createElement('li');
            if (l.sep) {
                li.style.borderBottom = '1px solid white';
            }
            var a = d.createElement('a');
            a.href = l.url;
            li.appendChild(d.createTextNode(l.text));
            a.appendChild(li);
            ul.appendChild(a);
/* 
    Note the above is weird and not what you expect. We're making things like
    this:
        <ul>
          <a>
            <li></li>
          </a>
        </ul>

    I.e. ul > a > li

    This is weird and nonstandard. But it's not against the rules. But it's
      gonna be bound to annoy someone. But without doing this, only the text
      is clickable rather than the entire list element.
*/
        }
        div.appendChild(ul);
        m.appendChild(div);
    }
}


activateMenu('allcards');

activateMenu('announcements');

activateMenu('birthday');

activateMenu('invitations');

activateMenu('occasions');

activateMenu('thanks');

activateMenu('wedding');

activateMenu('holiday');

activateMenu('photo');


</script>
</div>



