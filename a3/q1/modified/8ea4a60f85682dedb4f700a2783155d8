	$(function(){
	var closed = false;
	var visible = false;
	var speed = 750;
	var position = $('.post-listing').position();
	$('#recommended-popup-close').click(function() {
	$('#recommended-popup').hide('slide', { direction: 'right' }, speed);
	$('#recommended-popup').css('right','0');
	$('#recommended-popup').css('bottom','0');
	closed = true;
});

$(window).scroll(function(){
if (!closed) {
var th = position.top - 25;
if ( $(window).scrollTop() > th ) {
if (!visible ) {
$('#recommended-popup').show('slide', { direction: 'right' }, speed);
$('#recommended-popup').css('right','0');
$('#recommended-popup').css('bottom','0');
visible = true;
}
} else {
if (visible) {
$('#recommended-popup').hide('slide', { direction: 'right' }, speed);
$('#recommended-popup').css('right','0');
$('#recommended-popup').css('bottom','0');
visible = false;
}
}
}
});

});</script>              <div class="post-listing">
