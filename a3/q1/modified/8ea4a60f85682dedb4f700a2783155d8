			'<h3>get the free Ecouterre newsletter</h3>'+
			'<div style="vertical-align: top; width: 100%; height: 35px" class="mc-field-group">'+
			  '<input type="text" style="margin: 2px 9px 0 7px; z-index: 999; float:left; width:150px" id="mce-EMAIL-page" class="required email" name="EMAIL" value="'+email+'">'+
			  '<div style="float:right; width: 100px; margin-right: 22px; margin-top: 2px;"><img src="http://www.ecouterre.com/wp-content/themes/ecouterre/img/subscribe.jpg" border="0" alt="Submit this form" name="sub_but" style="cursor:pointer;" onclick="signup();"/></div>'+
			'</div>';
			$("#column3_newsletter_signup").css("background-color",color);
		}
	}
	
	function normal()
	{
		email = document.getElementById("mce-EMAIL").value;
		fname = document.getElementById("mce-FNAME").value;
		lname = document.getElementById("mce-LNAME").value;
		$("#column3_newsletter_signup").fadeTo("fast",0);
		setTimeout('$("#column3_newsletter_signup").fadeTo("fast",1,setElementValue(2))',speed);
	}
	
	function formCheck()
	{
		email = document.getElementById("mce-EMAIL").value;
		fname = document.getElementById("mce-FNAME").value;
		lname = document.getElementById("mce-LNAME").value;
		
		var xmlhttp;
		
		var additionSize = 0;
		
		if(window.XMLHttpRequest)
			xmlhttp = new XMLHttpRequest();
		else
			xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
		
		xmlhttp.onreadystatechange=function()
		{
			if (xmlhttp.readyState==4 && xmlhttp.status==200)
			{
				var response = xmlhttp.responseText;
				has = eval("has="+response);

				document.getElementById("mce-error-response").innerHTML = "";
				document.getElementById("mce-success-response").innerHTML = "";
				document.getElementById("mce-error-response").style.display = "none";
				document.getElementById("mce-success-response").style.display = "none";
				
				document.getElementById("mce-EMAIL").value = has.email;
				document.getElementById("mce-FNAME").value = has.fname;
				document.getElementById("mce-LNAME").value = has.lname;
				
				additionSize = new Array(50,20,20,0);
				count = 0;
				
				if(has.validEmail == true && has.validFName == true && has.validLName == true)
				{
					document.getElementById("mce-success-response").style.display = "block";

					document.getElementById("mce-success-response").innerHTML = '<center><h3 style="margin:0; color:#000000;" >THANK YOU FOR SUBSCRIBING!</h3></center>';
					document.getElementById("mc-embedded-subscribe-form").submit();
					
					additionSize[3] = 60;
				}
				else
				{		
					document.getElementById("mce-error-response").style.display = "block";
					
					if(has.validEmail == false)
					{
						document.getElementById("mce-error-response").innerHTML += '<center><p style="margin:0; color:#8B0000;" >Please enter a valid email address</p></center>';
						additionSize[3] += additionSize[count++];
					}
					
					if(has.validFName == false)
					{
						document.getElementById("mce-error-response").innerHTML += '<center><p style="margin:0; color:#8B0000;" >Please enter a valid first name</p></center>';
						additionSize[3] += additionSize[count++];
					}
					
					if(has.validLName == false)
					{
						document.getElementById("mce-error-response").innerHTML += '<center><p style="margin:0; color:#8B0000;" >Please enter a valid last name</p></center>';
						additionSize[3] += additionSize[count++];
					}
				}
				
				document.getElementById("mc-embedded-subscribe-form").style.marginBottom = (size + additionSize[3]) + "px";
			}
		}
		xmlhttp.open("GET","http://www.ecouterre.com/wp-content/themes/ecouterre/formValidator.php?email="+email+"&fname="+fname+"&lname="+lname,true);
		xmlhttp.send();
	}
</script>

