  config: {
  },

  log: function(m){
    if (!m || typeof uet !== 'function' || typeof uex !== 'function' ) {
      return;
    }

    if(typeof ppdClientMetrics.config.overall_scope !== 'undefined'){
      if(m == 'ld'){
        amznJQ.onCompletion("amznJQ." + ppdClientMetrics.config.overall_scope + ".cf", function(){
            uex('ld', ppdClientMetrics.config.overall_scope, {wb:1});
        });
      } else {
          uet(m, ppdClientMetrics.config.overall_scope, {wb:1});
          amznJQ.completedStage("amznJQ." + ppdClientMetrics.config.overall_scope + "." + m);
      }
    }

    if(typeof ppdClientMetrics.config.wdg_scope !== 'undefined'){
      if(m == 'ld'){
        amznJQ.onCompletion("amznJQ." + ppdClientMetrics.config.wdg_scope + ".cf", function(){
          uex('ld', ppdClientMetrics.config.wdg_scope, {wb:1});
