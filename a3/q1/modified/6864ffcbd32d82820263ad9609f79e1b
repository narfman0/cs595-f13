<div class="str-lyt-col01">
<!-- #BeginEditable "contentsTitle" --><h2 class="lyt-hdg-02-04">謎の大流行ゲーム『Cookie Clickr(クッキークリッカー)』って？今さらだけど始める方法</h2>
<!-- #EndEditable -->
</div><!--/str-lyt-col01-->

<div class="clearfix lyt-doc-border">

<div class="lyt-namearea">
<!-- #BeginEditable "contentsPartner" --><a href="http://okguide.okwave.jp/" target="_blank"><img id="okguide" src="/images/publisher/okguide.png" border="0"></a><!-- #EndEditable -->
<!-- #BeginEditable "author" --><!-- #EndEditable -->　
<!-- #BeginEditable "contentsDate" -->[2013/09/18]<!-- #EndEditable --></div>

</div><!--/clearfix lyt-doc-border-->

<!-- #BeginEditable "ad-text-article" --><!--text-ad-->
<div class="clearfix" id="ad-index-text">
<ul class="lyt-list-pr">
<script type="text/javascript" language="JavaScript">
<!--
impAserver = "http://as.mv.impact-ad.jp";
impAtarget1 = "/AAMB1/SITE=MN.CAREER.KONETA/AREA=MN.HEADLINE1/AAMSZ=1X1/OENCJP=UTF8";
impAtarget2 = "/AAMB2/SITE=MN.CAREER.KONETA/AREA=MN.HEADLINE2/AAMSZ=1X1/OENCJP=UTF8";
impAtarget3 = "/AAMB3/SITE=MN.CAREER.KONETA/AREA=MN.HEADLINE3/AAMSZ=1X1/OENCJP=UTF8";
impAtarget4 = "/AAMB4/SITE=MN.CAREER.KONETA/AREA=MN.HEADLINE4/AAMSZ=1X1/OENCJP=UTF8";
impAtarget  = impAtarget1 + impAtarget2 + impAtarget3 + impAtarget4;

impArnd = Math.round(Math.random() * 100000000);
if (!impApid) var impApid = Math.round(Math.random() * 100000000);
document.write('<scr');
document.write('ipt src="' + impAserver + '/bservers/AAMALL/OENCJP=UTF8/acc_random=' + impArnd + '/pageid=' + impApid + impAtarget+'">');
document.write('</scr');
document.write('ipt>');
//-->
</script>


<script>
var impAnads = 4;
var impAbs = new Array(impAnads);
for ( i = 0 ; i < impAnads ; i++) {
	eval('impAbs['+i+'] = (typeof(AAMB' + (i+1) + ') != "undefined")?AAMB' + (i+1) + ':"";');
	impAbs[i] = (impAbs[i].indexOf('AEMV') == -1)?impAbs[i]:'';
}

impAads = impA_rotate(impAbs);

function impA_rotate(list) {
	var i = list.length;
	while (--i) {
		var j = Math.floor(Math.random() * (i + 1));
		if (i == j) continue;
		var k = list[i]; list[i] = list[j]; list[j] = k;
	}
	for ( i = 0; i <= list.length; i++){ if ( list[i] == "" ) list.splice(i,1);}
	return list;
}
</script>

<script>
if (typeof(impAads[0]) !='undefined') { if (impAads[0] !="") { document.write(impAads[0]); } }
</script>

<script>
if (typeof(impAads[1]) !='undefined') { if (impAads[1] !="") { document.write(impAads[1]); } }
</script>

<script>
if (typeof(impAads[2]) !='undefined') { if (impAads[2] !="") { document.write(impAads[2]); } }
</script>

<script>
if (typeof(impAads[3]) !='undefined') { if (impAads[3] !="") { document.write(impAads[3]); } }
</script>

</ul>
