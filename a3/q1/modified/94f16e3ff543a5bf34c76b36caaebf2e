        function checkBannerCookie()
        {
          var bg = getBannerCookie('BG');
          if(bg){
            document.getElementById("header_container").style.backgroundImage='url(' + bg + ')';
          }
          else
          {
            document.getElementById("header_container").style.backgroundImage='url(http://1.bp.blogspot.com/-f5BFm2WLkak/UasN4kgzpQI/AAAAAAABPtQ/lFujLMaUfWI/s1600/stvbanner2013-14.jpg)';
          }
        }
        function checkBackgroundCookie()
        {
          var bg = getBackgroundCookie('BC');
          if(bg){
            document.body.style.background=bg;
          }
          else
          {
            document.body.style.background='#000000';
          }
        }
