if ($entries_music) { $entries_music.hide();  $entries_tri_music.hide(); }
if ($entries_film) { $entries_film.hide(); $entries_tri_film.hide(); }
if ($entries_clubs) { $entries_clubs.hide(); $entries_tri_clubs.hide(); }
if ($entries_atw) { $entries_atw.hide(); $entries_tri_atw.hide(); }
