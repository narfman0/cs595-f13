function show_balloon(id)
{
$entries_tot = $('blog_recent_entries_tot');
$entries_music = $('blog_recent_entries_music');
$entries_film = $('blog_recent_entries_film');
$entries_clubs = $('blog_recent_entries_film');
$entries_atw = $('blog_recent_entries_atw');
$entries_tri_tot = $('blog_recent_entries_tri_tot');
$entries_tri_music = $('blog_recent_entries_tri_music');
$entries_tri_film = $('blog_recent_entries_tri_film');
$entries_tri_clubs = $('blog_recent_entries_tri_clubs');
$entries_tri_atw = $('blog_recent_entries_tri_atw');
if ($entries_tot) { $entries_tot.hide(); $entries_tri_tot.hide(); }
if ($entries_music) { $entries_music.hide();  $entries_tri_music.hide(); }
if ($entries_film) { $entries_film.hide(); $entries_tri_film.hide(); }
if ($entries_clubs) { $entries_clubs.hide(); $entries_tri_clubs.hide(); }
if ($entries_atw) { $entries_atw.hide(); $entries_tri_atw.hide(); }
$('blog_recent_entries_' + id).show();
$('blog_recent_entries_tri_' + id).show();
}
Event.observe(window, 'load', function()
{
$entries_tot = $('blog_recent_entries_tot');
$entries_music = $('blog_recent_entries_music');
