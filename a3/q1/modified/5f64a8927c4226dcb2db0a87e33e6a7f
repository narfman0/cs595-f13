$(function(){
    $.ajax({
        url : "/ajax_head.html",
        type : "post",
        async: true,
        //data : ({ "item[text_color_top]" : val}),
        success: function(request) {
            $("#read_head_area").html(request);
            $("#read_head_login").html( $("#read_head_lg").html() );
            //マウスオーバーの処理
            over();
        }
    });
    $.ajax({
        url : "/ajax_head/meg.html",
        type : "post",
        async: true,
        //data : ({ "item[text_color_top]" : val}),
        success: function(request) {
            $("#WrapMrg").html(request);
            //マウスオーバーの処理
            over();

            var $set;
            $set = false;
            $(".WrapMrg").on({
                //mouseoverイベント時
                "mouseover" : function(){
        //            $offset = $(".wrap3").offset();
                    $set = true;
                    if ($set) {
                        $(".WrapMrg").offset({top: 178});
                    }
                    $set = false;
                },
                "mouseout" : function(){
                    $set = true;
                    if ($set) {
                        $(".WrapMrg").offset({top: 180});
                    }
                    $set = false;

                }
            });

        }
    });
});
</script>
