<li class="shareGoogle">
    <!-- Render Google +1 button -->
    <g:plusone size="medium" callback='DW.levt("ria", "log", {"riaevent":"click","comp":"main","objnm":"googleplus_share","comptyp":"app","siteid":175,"mapp":"social_share_bar","objtyp":"button"})' href="http://www.cbssports.com/nfl/writer/pat-kirwan/23817099/nfl-power-rankings-impressive-bears-dolphins-chiefs-remain-on-rise"></g:plusone>
    </li>
