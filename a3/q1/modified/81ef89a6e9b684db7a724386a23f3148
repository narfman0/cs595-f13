    <script type="text/javascript">
    if (switchXFP != 'off') {
        googletag.cmd.push(function() {googletag.display("mpu_plus_top");});
        if (location.search.substring(1).indexOf('DCLK')>-1) $('<input type="text" value="mpu_plus_top" style="width:300px"><br>').insertBefore('#mpu_plus_top');
    }
    </script>
</div>
