<script type="text/javascript" src="http://ftpcontent.worldnow.com/wncustom/js/wxmanager-source_updated.js"></script>
<script type="text/javascript">
$wn(document).bind('csWxCol4Ready', function() {
  CSWxManager.setOption('defaultHandler', csSiteOptions.wxHandler);
  CSWxManager.setOption('iconSize', csSiteOptions.wxIconSize);
  CSWxManager.setOption('wsiID', csSiteOptions.wsiFeedKey);

  CSWxManager.loadConfig(csSiteOptions.wxStations);
  
  $wn('#csWxCol4 #csWxData .csWxRadar').html(csSiteOptions.wxRadar);
  $wn('#csWxCol4 #csWxLinks').html(csSiteOptions.wxLinks);

  CSWxManager.ready(function(wx) {
    if (!wx.hasData()) { return; }
  
    var cookieName = 'csWxCol4' + wng_pageInfo.affiliateName;
    var defaultStation = WNCookie.get(cookieName);
    
    // Populate the drop-down box
    $wn.each(wx.listNames(), function(i, name) {
      var $opt = $wn('<option />', {value: i, text: name});
      $wn('#csWxCol4 .csWxPicker').append($opt);
    });
    
    // Define hi's and lo's via weather feed
    var hi, lo;
    switch (csSiteOptions.wxHandler) {
      case 'myweather':
        hi = wx.fetch('forecastdays').first().find('forecast').attr('high');
        lo = wx.fetch('forecastdays').first().find('forecast').attr('low');
        break;
      case 'wsi':
        hi = wx.fetch('high');
        lo = wx.fetch('low');
        break;
    }

    // Wire up the drop-down 'change' event
    $wn('#csWxCol4 .csWxPicker').change(function() {
      wx.index($wn(this).val());
      $wn('#csWxCol4 .csWxTemp').html(wx.fetch('temperature'));
      $wn('#csWxCol4 .csHi').html('Hi <span>' + hi + '&deg;</span>');
      $wn('#csWxCol4 .csLo').html('Lo <span>' + lo + '&deg;</span>');
      $wn('#csWxCol4 .csFeels').html('Feels like: ' + wx.fetch('feelslike') + '&deg;');
      $wn('#csWxCol4 .csIcon').css('background-image', wx.fetch('iconcss'));
      $wn('#csWxCol4 .csSky').html(wx.fetch('sky'));
      $wn('#csWxCol4 .csWxRemember')[0].checked = (wx.index() === parseInt(defaultStation, 10));
    });
    
    // Handle remembering and forgetting the default city
    $wn('#csWxCol4 .csWxRemember').change(function() {
      if (this.checked) {
        // Expire one year from now
        var expDate = new Date();
        expDate.setFullYear(expDate.getFullYear() + 1);
        
        defaultStation = wx.index();
        WNCookie.set(cookieName, defaultStation, { expires: expDate });
      } else {
        defaultStation = null;
        WNCookie.set(cookieName, defaultStation, { expires: new Date(0) });
      }
    });
    
    // Load up the initial city
    if (defaultStation !== null) {
      $wn('#csWxCol4 .csWxPicker').val(defaultStation).change();
    } else {
      $wn('#csWxCol4 .csWxPicker').change();
    }
    
    $wn('#csWxData').show();
  });
});
</script>

