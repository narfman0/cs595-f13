<script type="text/javascript">
$wn(document).bind('csBreakingNewsLevel3Ready', function() {
  // Dynamic templates
  var leftTemplate = '' +
    '<div class="csImageWrap">' +
      '<a href="{$headlineHref}">' +
        '<img src="{$graphicSrc}">' +
        '{if $hasVideo}<span class="csHasVideo"></span>{/if}' +
      '</a>' +
    '</div>' +
    '<div class="csSpacer"></div>' +
    '<h3>Related Stories</h3>' +
    '<ul class="csLeftStories"></ul>';
  var rightTemplate = '' +
    '{if $customDate}<div class="csDate">Updated: {$customDate}</div>{/if}' +
    '<h4><a href="{$headlineHref}">{$headlineText}</a></h4>' +
    '<p>{$summaryHtml} <a class="csMore" href="{$headlineHref}">More&gt;&gt;</a></p>' +
    '<div class="csSpacer"></div>' +
    '<div id="csRightStories"></div>';
  var leftStoryTemplate  = '<li><a href="{$headlineHref}">{$headlineText}</a></li>';
  var rightStoryTemplate = '' +
    '<div class="csCell">' +
      '<div class="csImageWrap">' +
        '<a href="{$headlineHref}">' +
          '<img src="{$graphicSrc}">' +
          '{if $hasVideo}<span class="csHasVideo"></span>{/if}' +
        '</a>' +
      '</div>' +
      '<h4><a href="{$headlineHref}">{$headlineText}</a></h4>' +
    '</div>';

  WNHttpRequestManager.makeRequest(csSiteOptions.breaking3Feed, {
    onSuccess: function() {
      // Parse the XML feed
      var $data = $wn(this.response.responseXML);
      var storyData = $data.find('story, clip').wnAPIStoryExtract();
      
      // Extract individual display sizes
      var topStory   = (storyData['-20'] || [])[0];  //only need first one
      var topStoryHL =  storyData['-18'] || [];
      var fullHoriz  =  storyData['-7']  || [];
      
      // Fill the template with the top story's data
      topStory.customDate = CSHELPERS.timeAgo(topStory.dateText, '4 hours');
      topStory.hasVideo = (topStory.isClip || topStory.isVideo);
      $wn('#csBNContent .csBNLeft').csFormatterHtml(leftTemplate, topStory);
      $wn('#csBNContent .csBNRight').csFormatterHtml(rightTemplate, topStory);
      
      // Add headlines to the left
      $wn.each(topStoryHL, function(i) {
        if (i >= csSiteOptions.tsHeadlineCount) { return false; /* break */ }
        $wn('.csLeftStories').csFormatterAppend(leftStoryTemplate, this);
      });
      
      // Add thumbnails to the right
      $wn.each(fullHoriz, function(i) {
        if (i >= 3) { return false; /* break */ }
        this.hasVideo = (this.isClip || this.isVideo);
        $wn('#csRightStories').csFormatterAppend(rightStoryTemplate, this);
      });

      // Center the video icons over the images
      $wn('#csBreakingNewsLevel3').imagesLoaded(function() {
        $wn('#csBreakingNewsLevel3 .csImageWrap').each(function() {
          var $wrap = $wn(this);
          var $icon = $wrap.find('.csHasVideo');
          $icon.css({
            left: ($wrap.width()  / 2) - ($icon.width()  / 2),
            top:  ($wrap.height() / 2) - ($icon.height() / 2)
          }).show();
        });
      });
      
      // Hide this UB, move the BN before Col2/3 and Col4
      $wn('#csBreakingNewsLevel3').parent().hide();
      $wn('#csBreakingNewsLevel3').insertBefore('#WNCol23');
      $wn('#csBreakingNewsLevel3, #csBreakingNewsLevel3 .header').show();
    }
  });
  
  if (csSiteOptions.moreLink) {
    $wn('#csBreakingNewsLevel3 .csMoreLink').show();
    $wn('#csBreakingNewsLevel3 .csMoreLink').find('a').attr('href', csSiteOptions.moreLink);
  } 

});
</script>

<div id="csBreakingNewsLevel3" class="wnBlock displaySize displaySizeId-30">
  <ul class="wnGroup contentGroup odd last">
    <li class="wnItem header">
      <h3>
        <span class="text siteDefault">Breaking News</span>
        <div class="wnClear"></div>
      </h3>
    </li>
    <li id="csBNContent" class="wnItem feature">
      <div class="csBNLeft"></div>
      <div class="csBNRight"></div>
    </li>
    <li class="wnItem csMoreLink">
      <a href="" class="wnContent more enabled"><span class="text">More Breaking News>></span></a>
    </li>
  </ul>
  <div class="wnClear"></div>
</div>

<script type="text/javascript">
$wn(document).trigger('csBreakingNewsLevel3Ready');

  if (csSiteOptions.linksPlus) {

    setTimeout(function() {
      $wn('.csBNLeft').append('<h3>Additional Breaking News Links</h3><ul class="csLeftStories two"></ul>');

      // Links Plus assigned as Top Story Headline
      var $linksPlus = $wn('#WNDS-18 .wnDVHeadline');
      $wn.each($linksPlus, function() {
        var linkItem = $wn(this).wrapAll('<li/>');
        linkItem.appendTo('.csLeftStories.two');
      });
      $wn('#WNDS-18').hide();
    }, 500);

  }
</script></div><div class="wnDVUtilityBlock"><style type="text/css">
#csTsr {
	margin: 0;
	padding: 0;
	width: auto;
