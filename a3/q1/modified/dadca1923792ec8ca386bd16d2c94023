    <script type="text/javascript">
    $(document).ready(function () {
        function losmas(vls) {
            switch (vls) {
                case 1:
                    $("#lmhead").removeClass("masVistas");
                    $("#lmhead").removeClass("masComent");
                    $("#lmhead").addClass("loUltimo");

                    $("#lmascontent").html($("#lutimoC").html());
                    break;
                case 2:
                    $("#lmhead").removeClass("loUltimo");
                    $("#lmhead").removeClass("masComent");
                    $("#lmhead").addClass("masVistas");

                    $("#lmascontent").html($("#mVistasC").html());
                    break;
                case 3:
                    $("#lmhead").removeClass("loUltimo");
                    $("#lmhead").removeClass("masVistas");
                    $("#lmhead").addClass("masComent");

                    $("#lmascontent").html($("#mComentC").html());
                    break;
            }
            return false;
        }
    });

    </script>
    
