<script>
   var TopHeight = 0;
   var Toptvfm = false;

   function Pro_view(mlist, cc, nn)
     {
       //TopMenuh = $('#TopMenu2').css("height").replace("px", "");
 
             $.post('/main/'+mlist+'.asp', {}, function(data) {
                 $('#TopMenu1').html(data);
             });

             $('#TopMenu1').removeClass('tv').removeClass('fm');
             $('#TopMenu1').addClass(cc);

	      	
    if (cc == 'fm')
        {
		   if('News' != 'FM') location.href="/fm/fmPro_view.asp?Tabtv=1&oddtv=1&topmenu=ok";
		     else 
			   {
/*
			       $('#TopMenu2').slideDown('slow', function() { 
			            $('#TopMenu2').css("height",nn+"px");
			            $('#G1_ad02').css("top", parseInt(nn+60)+"px");
                        $('#tv_ad').css("top", parseInt(nn+60)+"px");
                        $('#fm_ad').css("top", parseInt(nn+60)+"px");
			        });
*/
	              // Toptvfm = false;


			   }
       } 
    else if (cc == 'tv')
           {
		   if('News' != 'TV') location.href="/tv/tvPro_view.asp?oddtv=1&topmenu=ok";
		     else 
			   {

/*			       $('#TopMenu2').slideDown('slow', function() { 
			            $('#TopMenu2').css("height",nn+"px");
			            $('#G1_ad02').css("top", parseInt(nn+60)+"px");
                        $('#tv_ad').css("top", parseInt(nn+60)+"px");
                        $('#fm_ad').css("top", parseInt(nn+60)+"px");
			        });
*/
	               //Toptvfm = false;
			   }
           }

 
	         if(!Toptvfm) 
			   {
			       $('#TopMenu2').slideDown('slow', function() { 
			            $('#TopMenu2').css("height",nn+"px");
			            $('#G1_ad02').css("top", parseInt(nn+60)+"px");
                        $('#tv_ad').css("top", parseInt(nn+60)+"px");
                        $('#fm_ad').css("top", parseInt(nn+60)+"px");
			        });
	               Toptvfm = true;
			   }
            else 
			   {
			   //alert('up');
			       $('#TopMenu2').slideUp('slow', function() { 
			            $('#TopMenu2').css("height",nn+"px");
			            $('#G1_ad02').css("top", "30px");
                        $('#tv_ad').css("top", "30px");
                        $('#fm_ad').css("top", "30px");
			        });
	               Toptvfm = false;
			   }

	
  
   
	}

   function Pro_out2(yy)
     {
        //if ('News' == 'MAIN' || 'News' == '')
		if ('News' != 'TV' && 'News' != 'FM')
          {
	          if( yy < 80) clout();
          }
        else Toptvfm = false;
	 }

   function Pro_out()
     {
	     //alert('Pro_out');
        //if ('News' == 'MAIN' || 'News' == '')
		//if ('News' != 'TV' && 'News' != 'FM')
          {
             clout();
          }
	 }

  function clout()
   {  
	    $('#TopMenu2').slideUp('slow', function() { 
            $('#TopMenu2').css("display", "none");	
            $('#G1_ad02').css("top","120px");
	   });
       Toptvfm = false;   
	   return false;
   } 

  $(document).ready(function() { 

/*
