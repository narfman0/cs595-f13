<script type="text/javascript" >

	function getStatsForThemeMiniStats() {
		// Call ajax request only if data is not here.
		if ($("#upv_904656").text() == "Loading..." ) {
			jQuery.getJSON("/stats/theme/904656?view=json", function(json){
				jQuery("#upv_904656").empty();
				if (json.stats && json.stats.uniqueVisitors >= 0) {
					jQuery("#upv_904656").append(json.stats.uniqueVisitors);				
				} else {
					jQuery("#upv_904656").append("Not available");
				}
			});
		}
	}
</script>
		
