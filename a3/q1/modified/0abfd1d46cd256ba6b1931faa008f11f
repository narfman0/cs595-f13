bim_reg.subscribeToChannel("regAuthNoUser", thresholdReg);




function thresholdReg() {
	var _optout = bim_reg.l.cookies.get("BIMREG_optout");

    if(!_optout) {
        if(bim_reg.m.page.getPageCount() > 5) {
            bim_reg.m.page.resetPageCount();
            setTimeout("$('#login-button a.signin').click()",500);
        }
    }
};

function initRedirect() {
			}
	
function createIMGCookie() {
	var zipcode = bim_reg.m.user.getZipcode();
	if(typeof(zipcode) == "undefined") zipcode = "";
	var firstname = bim_reg.m.user.getFirstName();
	if(typeof(firstname) == "undefined") firstname = "";
	bim_reg.l.cookies.set("IMG","|"+firstname+"|"+zipcode+"||",365);	
}
        
function userNameErrorCheck() {
	
        //if ( bim_reg.m.user.getEmail().replace(/\W/g,"") === bim_reg.m.user.getUsername().replace(/\W/g,"") ) {
	//	var failedName = bimreg.getuserkey("username");  
	//	var forcedUpdateName = failedName.replace(/@.*/,"").replace(/\W/g,"");
	//	var currentTime = new Date();
	//	var seconds = currentTime.getTime();
	//	var secString = seconds+'';
	//	var digits = secString.length + forcedUpdateName.length - 16;
	//	if (digits > 0) {
	//		seconds = secString.substring(digits,secString.length);
	//	}
	//	username = forcedUpdateName + seconds;
	//	//update user
	//	bimreg.updateprofile({username:username});
	//	
	//	if ( document.URL.indexOf('form=login') > -1 ) {
	//		//window.location.replace("/register/?form=user");
	//		$('#login-button a.signin').click();
	//	} else {
	//		//window.location = "/register/?form=user";
	//		$('#login-button a.signin').click();
	//	}
	//}
	
	//[bim_reg] Update -Begin-
	if(bim_reg.m.user.getEmail().replace(/\W/g,"") === bim_reg.m.user.getUsername().replace(/\W/g,"")){
		var failedName = bim_reg.m.user.getUsername();  
		var forcedUpdateName = failedName.replace(/@.*/,"").replace(/\W/g,"");
		var currentTime = new Date();
		var seconds = currentTime.getTime();
		var secString = seconds+'';
		var digits = secString.length + forcedUpdateName.length - 16;
		if (digits > 0) {
			seconds = secString.substring(digits,secString.length);
		}
		username = forcedUpdateName + seconds;
		//update user
		bim_reg.m.user.setUsername(username);
		bim_reg.m.user.save();
		
		if ( document.URL.indexOf('form=login') > -1 ) {
			//window.location.replace("/register/?form=user");
			$('#login-button a.signin').click();
		} else {
			//window.location = "/register/?form=user";
			$('#login-button a.signin').click();
		}
	}
	//[bim_reg] Update -End-
}
        
function checkLoginReferrer() {
	if(referrer) {
		bim_reg.l.cookies.remove("BIMREG_signup_referrer");
		window.location = referrer;
	} else {
		try {
			if (document.referrer) {
				window.location = document.referrer+window.location.hash; 
							} else if (window.location.hash !== "") { 
								window.history.go(-1);
			} else {
				window.location = "/";
			}
		} catch(e) {
			window.location = "/";
		}  
	} 	
}
    
