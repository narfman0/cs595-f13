<script>
$(document).ready(function() {
	$("#newsLetterWidget form .signup").click(function(){
		if(bim_reg.m.user.isLoggedIn()){
			//bimreg.listener.addListener("updateProfile",newsletterUpdate);
			nl=$('.datranNewsletterName').val();
			bim_reg.m.user.subscribeToNewsletter(nl);
			bim_reg.m.user.save();
			$(".newsletter-description").html("Thank you for subscribing to this newsletter.");
			$(".newsletter-email").css("display","none");
			$("button.signup").css("display","none");
		}else{
			$("#login-button a").click();
		}
	});
});

function newsletterUpdate(){
	bimreg.listener.removeListener("updateProfile",newsletterUpdate);
	//window.location = "/register?form=welcome-back-int&docReferrer=%2F&newsletter=newsletter_wvec_break";
	location.reload();
	return;
}

function loadprofile(){
	var nogo=false;
	var newsletters = bim_reg.m.site.getNewsletters(); 
	for (var i in newsletters) {
		if(bim_reg.m.user.getNewsletters()[i] == true){
			if(newsletters[i]=="WVEC_BREAK"){
				nogo=true;
			}
		}
	}
	if(nogo){
		$("#newsLetterWidget form").css("display","none");
    	$(".newsletter-description").html("You are currently subscribed to this newsletter.");
	}else{	
 		$('#widget-email').val(bim_reg.m.user.getEmail());
 		$('#widget-email').css("border","none");
    	$('#widget-email').attr("disabled", true);
	}
}
$(document).ready(function(){
	//bimreg.listener.addListener("userLogin",loadprofile);
	bim_reg.subscribeToChannel("user_data_ready", loadprofile);
});
</script>
								

        

					        								        																																																																											                								        				        
