	belo_tracking_sitepage = window.location.host + window.location.pathname+"/";
	if(belo_tracking_sitepage.charAt(belo_tracking_sitepage.length-1) == "/") {
	    belo_tracking_sitepage = belo_tracking_sitepage.substring(0,belo_tracking_sitepage.length-1);
	}
}
