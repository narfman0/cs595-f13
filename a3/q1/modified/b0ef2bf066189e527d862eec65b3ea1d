<script>
	jQuery(document).ready(function(){
	
					//var masonryDivs = jQuery('.post-open-masonry').length;
					//console.log( 'Masonry divs = ' + masonryDivs );

					jQuery('.post-open-masonry').imagesLoaded(function() {
						jQuery('#dvLoading').fadeOut();
						jQuery( '.post-open-masonry' ).masonry({
								itemSelector: '.post-image-wrap',
								columnWidth: function( containerWidth ) {
								return (containerWidth / 2) - 12;
									},
								gutterWidth: 24
								});
								
							console.log('3 - modalFinishMasonry');
							jQuery('#dvLoading').fadeOut();
							
						});	
					
					jQuery(window).scroll(function () {
						console.log( 'Scrolling' );
					});

					
					jQuery('.yarpp-related').imagesLoaded(function() {
							jQuery( '.yarpp-related' ).masonry({
								itemSelector : '.yarpp-masonry',
								columnWidth: 186,
								gutterWidth: 12,
								isFitWidth: true
								});
							console.log('4 - MasonryFinishRelated');
							jQuery('#dvLoading').fadeOut();
						});
	
						jQuery('#modal-content-destroy .image-wrap').hover(function() {
							jQuery('.magnifier', this).stop().animate({"opacity": 1});
								},function() {
									jQuery('.magnifier', this).stop().animate({"opacity": 0.5});

						});
							
						jQuery('#permalink').click(function(e) {
								$(this).popover('show');
								console.log('Popover Clicked');
								
							});	
						jQuery(document).click(function (e)
      {
          // check the parents to see if we are inside of a tool tip.  If the click came
          // from outside the tooltip, then hide our tooltip
          if (($(e.target).parents('.popover').length == 0) && ($(e.target).parents('#permalink-list').length == 0) ) $('#permalink').popover('hide');
          console.log('Document click');
      });					
						
					
						
						var pagetrack = '/2013/09/pasquale-taraffo-harp-guitar/';
						var pageTitle = 'Pasquale Taraffo and the Harp Guitar';
						var pageURL = 'http://www.retronaut.com/2013/09/pasquale-taraffo-harp-guitar/';
						
							_gaq.push(['_trackPageview', pagetrack]);
	
						if (Modernizr.history) {
							// history management works!
							jQuery(document).attr('title', 'Retronaut - ' + pageTitle);
							window.history.replaceState("", pageTitle , pageURL );
							console.log('History');
							} else {
								// no history support :(
								console.log('No History');
								}
							
							console.log('Page Title - ' + pageTitle );

						try {
							FB.XFBML.parse(document.getElementById('facetweet-li'));
							
						} catch(err) { }
						
						twttr.widgets.load();
	});
</script>
