<script type="text/javascript">
(function() {
    function loadHorizon() { 
        var s = document.createElement('script'); 
        s.type = 'text/javascript'; 
        s.async = true;
        s.src = ('https:' === location.protocol?'https://d1gp8joe0evc8s.cloudfront.net':'http://cdn.sail-horizon.com')+'/horizon/v1.js';
        var x = document.getElementsByTagName('script')[0]; 
        x.parentNode.insertBefore(s, x);
    }
    loadHorizon();
    var oldOnLoad = window.onload;
    window.onload = function() {
        if (typeof oldOnLoad === 'function') {
            oldOnLoad();
        }
        Sailthru.setup({
            domain: 'horizon.bigcatcountry.com'
        });
    };
})();
</script>
    </div>
    
<script type="text/javascript">
  jQuery(window).load(function($){
    function key(suffix){
      var key = "sbn_entries_controller.show.article." + suffix;
      key += "." + Util.UserAgentProfiler.profile();
      return key;
    }

    var perf = window.performance || window.webkitPerformance
    var timing;
    var metrics = []

    if (perf) {
      
      if (timing = perf && perf.timing) {
        
        metrics.push({
          key: key('serverRequestTime'),
          value: timing.responseStart - timing.navigationStart
        })
        
          metrics.push({
            key: key('serverResponseTime'),
            value: timing.responseEnd - timing.navigationStart
        })
        
          metrics.push({
          key: key('domLoadTime'),
          value: timing.domContentLoadedEventStart - timing.navigationStart
        })
        
          metrics.push({
          key: key('domCompleteTime'),
          value: timing.domComplete - timing.navigationStart
        })
      }
    }

    Util.Instrumentation.report(metrics);
    Util.Instrumentation.activate();

  });
</script>


  </body>
</html>
