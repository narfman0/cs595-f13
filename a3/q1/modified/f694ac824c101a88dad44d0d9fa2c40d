                function (response) {
                    WpsTracking.omnitureCustomTrack(WpsTracking.properties["socialShare"], "Facebook Like");
                }
            );
        };

        runFacebook = function () {
            var js, id = 'facebook-jssdk', ref = document.getElementsByTagName('script')[0];
            if (document.getElementById(id)) { return; }
            js = document.createElement('script'); js.id = id; js.async = true;
            js.src = "//connect.facebook.net/en_US/all.js";
            ref.parentNode.insertBefore(js, ref);
        };
        CheckWindowLoadEventAndRunFunction(runFacebook);
    </script>
    <script type="text/javascript" charset="utf-8">
        window.twttr = (function (d, s, id) {
            function loadTwitterScript() {
                var t, js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) return;
                js = d.createElement(s);
                js.id = id;
                js.src = "//platform.twitter.com/widgets.js";
                fjs.parentNode.insertBefore(js, fjs);
            }
            CheckWindowLoadEventAndRunFunction(loadTwitterScript);
            return window.twttr || (t = { _e: [], ready: function (f) { t._e.push(f); } });
        }(document, "script", "twitter-wjs"));

        twttr.ready(function (twttr) {
            twttr.events.bind('tweet', function (event) {
                WpsTracking.omnitureCustomTrack(WpsTracking.properties["socialShare"], "Twitter Tweet");
            });

            twttr.events.bind('retweet', function (event) {
                WpsTracking.omnitureCustomTrack(WpsTracking.properties["socialShare"], "Twitter ReTweet");
            });

            twttr.events.bind('click', function (event) {
                WpsTracking.omnitureCustomTrack(WpsTracking.properties["navigation"], "Twitter Click");
            });

            twttr.events.bind('favorite', function (event) {
                WpsTracking.omnitureCustomTrack(WpsTracking.properties["navigation"], "Twitter Favorite");
            });
        });
    </script>            

    
