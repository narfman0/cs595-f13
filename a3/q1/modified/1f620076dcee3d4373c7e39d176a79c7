    <script type="text/javascript">
        function evaluateFreeMessageStatus(){
            var isObit = false;
            if(!isObit){
                var isContentFree = true;
                if(isContentFree) {
                    // the other use of the free message is in review.jsp
                    if ( wwd.user.showFreeMessage() )  {
                        //Target Article First Paragraph - if there is one
                        var contentBody   = jQuery(".article .article-copy");
                        var firstPara     = jQuery(".article .article-copy p:first");
                        //required to determine width of text column
                        var freeMessage   = "<span id='free-message'>. . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .</span>";
                        if ( (firstPara.find("br").length > 0) || (contentBody.find("p").length < 1) ) {
                            //if a BR tag exists within the first P tag or if there is no p tag at all, add the text after the first BR tag
                            jQuery(".article .article-copy br:first").after(freeMessage);
                        } else {
                            //otherwise, add the text after the first P tag
                            jQuery(".article .article-copy p:first").after(freeMessage);
                        }
                        //after adding free message box to content, the column width is evaluated to display the version that fits
                        var colWidth      = jQuery('#free-message').width();
                        var messageNarrow = "<a href='https://subscribe.wwd.com/wwd/subnew_onepage_v2.aspx?PC=WO&PK=N26FREE'><img src='/images/processed/wwd3/free-preview-narrow.png' title='Free Preview' alt='Free Preview'></a>";
                        var messageWide   = "<a href='https://subscribe.wwd.com/wwd/subnew_onepage_v2.aspx?PC=WO&PK=N26FREE'><img src='/images/processed/wwd3/free-preview-wide.png' title='Free Preview' alt='Free Preview'></a>";
                        if (colWidth > 620) {
                            jQuery("#free-message").html(messageWide);
                        } else {
                            jQuery("#free-message").html(messageNarrow);
                        }

                        jQuery("#free-message").addClass('show');

                    } else {
                        jQuery("#free-message").hide();
                    }
                }
            }
        }

        jQuery(document).ready(function() {
            //jQuery('body').addClass('7180477');
            evaluateFreeMessageStatus();
            wwd.events.addListener("loggedIn", function(){evaluateFreeMessageStatus();});
        });
        </script>

