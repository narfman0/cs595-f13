$j(document).ready(function() {
	var open = false;
	// Subscription panel
	$j('#subscribe_adv_2 a[href=#subscribe_adv_1]').click(function() {
		if (!open) {
			$j('#subscribe_adv_1').slideDown(500);
			$j('#peelback').animate({ top: '+=121px' }, 500);
			open = true;
		} else {
			$j('#subscribe_adv_1').slideUp(500);
			$j('#peelback').animate({ top: '-=121px' }, 500);
			open = false;
		}
	});
});
</script>

