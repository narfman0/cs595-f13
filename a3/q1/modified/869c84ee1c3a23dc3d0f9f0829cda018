        <script type="text/javascript">
            jQuery('#footer-nav > div > ul').each(function(){
                jQuery(this).children('li').removeClass('first');
                jQuery(this).children('li:first').addClass('first');
            });
            
            jQuery('#footer-nav li').each(function(){
                var link = jQuery(this).children('a:first');
                var href = link.attr('href');
                if(!link.attr('href') || href == '#') {
                    link.removeAttr('href').addClass('underlineNone');
                }
            });
            
        </script>
