				var url = 'http://ricmais.com.br/sc/wp-content/plugins/ric_sentimentos/ajax-salva-sentimento.php?sentimento='+id+'&post_id='+post_id; 
				$("#container-sentimento").html("Enviando seu sentimento... "); 
				$("#container-sentimento").load(url);
				return false; 
			}); 
				
		}); 
	</script>
		

    </footer><!-- .entry-meta -->
</article><!-- #post-80342 -->
