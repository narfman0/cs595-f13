$(document).ready(function() {
  updateDateRange(currTerm);
});

function changeLoc(href) {
  if(href != 'select') {
    document.location.href = href;
  }
}

function updateDateRange(position) {
  if(position == 'pick') return;
  var options = dateRanges[position];
  $('#statssel').html('<option value="select">Pick Year</option>');
  for(var i in options) {
    $('#statssel').append('<option value="' + i + '">' + options[i] + '</option>');
  }
}
</script>
<select id="possel" onChange="updateDateRange(this.options[this.selectedIndex].value)"/>
