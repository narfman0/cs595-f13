<script type="text/javascript">
function adSpaceSizer() { 

  	var adSpaceObj=document.getElementById('adSpace_bottom');   
	var adImgObj=document.getElementById('adTab').getElementsByTagName('img')[0]; 
	
	var windowWidth=window.innerWidth;
	var windowHeight=window.innerHeight;
	
	if ( adSpaceObj !== undefined )
	{
		adSpaceObj.style.maxHeight="50px"; 
		adSpaceObj.style.minHeight="20px"; 
		adSpaceObj.style.height=Math.floor(windowWidth*0.15625)+'px';
	}

	if ( adImgObj !== undefined )
	{
		adImgObj.style.maxHeight="50px"; 
		adImgObj.style.minHeight="20px"; 
		adImgObj.style.height=Math.floor(windowWidth*0.15625)+'px';
	}

}
</script>



