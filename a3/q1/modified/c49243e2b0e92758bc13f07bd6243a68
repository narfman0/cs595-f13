<script>
$(document).ready(function()
{
	/* -- .js to stick the top mobile add to the to */
	
	var adPos = $('#adSpace_bottom').offset();
	var adTop = adPos.top;
	$(document).scroll(function() {
		var scrollTop = $(this).scrollTop();
		if ( scrollTop >= adTop ) {
			$('#adSpace_bottom').css({top:scrollTop+'px'});
		} else {
			$('#adSpace_bottom').css({top:adTop+'px',position:'absolute'});
		}
	});
	
/*	*/
});
</script>

 

