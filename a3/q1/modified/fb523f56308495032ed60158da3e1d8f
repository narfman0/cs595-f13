	if ( typeof ord == 'undefined' ) {
		ord = Math.random() * 100000000000000000;
		//Forcing first Ad to load next after load
		adIframeDiv = jQuery( '.mmcAd_Iframe_div' ).eq( 0 );
		adIframeDiv.attr( 'data-adrender', 0 );
