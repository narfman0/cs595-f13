		<script language="JavaScript" type="text/javascript">
			jQuery(document).ready(function() {
				//Related Posts AJAX code
				related_page = 1;
				jQuery('.more-related-article > strong > a').click(function(){
					var current_element = jQuery(this);
					if ( current_element.text() == 'More Stories' ) {
						current_element.html('<img src="http://s1.wp.com/wp-content/themes/vip/pmc-hollywoodlife/images/loading.gif?m=1349392494g" />')
							.addClass('ajax-link')
							.removeClass('action-link');

						jQuery.ajax({
							url: "http://hollywoodlife.com/wp-admin/admin-ajax.php",
							type: 'GET',
														data: "action=fetch_more_related_posts&page=" + related_page + "&postid=1197388",
							context: current_element,
							success: function( dropdown ) {
								if ( dropdown !=0 ) {
									jQuery('.related-articles').append(dropdown);
								}

								if ( dropdown == "" ) {
									jQuery('.more-related-article').hide();
								}

								related_page += 1;

								jQuery(this).text('More Stories')
									.removeClass('ajax-link')
									.addClass('action-link');
							}
						});
					}

					return false;
				});
			});

		</script>

		
