				var Cookies = {
					init:function () {
						var allCookies = document.cookie.split("; ");
						for (var i = 0; i < allCookies.length; i++) {
							var cookiePair = allCookies[i].split("=");
							this[cookiePair[0]] = cookiePair[1];
						}
					},
					create:function (name, value, hours) {
						var expires;
						var date = new Date();
						if (hours) {
							date.setTime(date.getTime() + (hours * 60 * 60 * 1000));
						}
						else{
							date.setTime(date.getTime() + ( 60 * 1000));
						}
						expires = "; expires=" + date.toGMTString();
						document.cookie = name + "=" + value + expires + "; path=/";
						this[name] = value;
					},
					erase:function (name) {
						this.create(name, "", -1);
						this[name] = undefined;
					}
				};

				function are_cookies_enabled() {
					document.cookie = "testcookie";
					var cookieEnabled = (document.cookie.indexOf("testcookie") != -1) ? true : false;
					return (cookieEnabled);
				}

				function pmc_check_hash_tag(){

					if( window.location.hash ){
						var hash = window.location.hash;

						var hash_tags = {};

						hash.replace(new RegExp("([^#=&]+)(=([^&]*))?", "g"), function($0, $1, $2, $3) { hash_tags[$1] = $3; });

						var pid = hash_tags['pid']

						if( pid != null && pid != "" && pid.toLowerCase() == 'ob' ) {
							return false;
						}
					}

					return true;
				}

				if( are_cookies_enabled() && pmc_check_hash_tag() ){
					Cookies.init();
					var sInterstitial = Cookies["cki"];
					if (sInterstitial == "undefined" || sInterstitial == null) {
						Cookies.create("cki", "laititsretni",5);

						if( ! Cookies["pmc_referrer"] ) {
							Cookies.create("pmc_referrer", encodeURIComponent(document.referrer), 1);
						}

						pmc_do_analytics_pagecount = false;		//don't want this pageview to count, set flag to false

						window.location.href = "http://hollywoodlife.com/interstitial/?ref=" + encodeURIComponent(window.location.href);
					}
				}
