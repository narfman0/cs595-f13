  function isMobile(userAgent)
  {
    var mobileDevices = new Array("240x320","acs","alcatel","amoi","audiovox","benq","blackberry","cdm","compal","configuration/cldc","dbtel","docomo","epoc","ezos","ericsson","fly","grundig","htc","http:#www.voxtel.ru","iphone","ipod","kddi","kwc","lenovo","lg-","lg/","lge","medion","mda","midp","mitsu","mmp/","mobile","mot-","motorola","nec","netfront","nokia","o2","opera mini","opwv","palmos","palmsource","panasonic","philips","phone","pocket pc","portable","portalmmm","qtek","sagem","samsung","sanyo","sch","sda","sec","sendo","sgh-","sharp","sie-","sonyericsson","spv","symbian","telit","tsm","up.browser","vk-","vodafone","vx","wap","web'n'walk","winwAP","windows ce","xda","xiino","yourwap","zte");
    var noMobileDevices = new Array("check_http","clixsmart","ia_archiver","ipad","kindle","libcurl","libwww","nsplayer","quicktime","sunrise","wget","windows-media-player","www-mechanize","windows nt");
    var agent = navigator.userAgent.toLowerCase();
    try {
        for(var i = 0; i < noMobileDevices.length; i++)
        {
          if (agent.indexOf(noMobileDevices[i]) != -1) {
            return false;
          }
        }
    }
    catch(e) { }
    try {
      for(var i = 0; i < mobileDevices.length; i++)
      {
        if (agent.indexOf(mobileDevices[i]) != -1) {
          return true;
        }
      }
    }
    catch(e) { }
    return false;
  }

