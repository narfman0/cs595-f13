			<div id="page-top-ad">
			<!-- bannerAdType: "1x1 rich media" --><!-- show rich media ad -->
			
			
			
		
<script type="text/javascript">
		(function(){
			var getCookie = function(name) {
				var nameEQ = name + "=";
				var ca = document.cookie.split(';');
				for(var i=0;i < ca.length;i++) {
					var c = ca[i];
					while (c.charAt(0)==' ') c = c.substring(1,c.length);
					if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
				}
				return '';
			}
			var subPathVars = function(_path,_subs){
				for(_key in _subs){
					if(_path.indexOf('{'+_key+'}') > -1){ _path = _path.replace('{'+_key+'}',_subs[_key]); }
				}
				return _path
			}
			var _path1 	= "http://ad.doubleclick.net/adj/nfl.home/landing/shade;s1=landing;pos=shade;url=landing;nfl=ad;!category=;kw=;tile={tile};test=;sz=1x1;{aamnfl};u={aam_did}{dcopt};ord={ord}?";
			var _path2  = "http://ad.doubleclick.net/adj/nfl.home/landing;s1=landing;slot=shade;url=landing;nfl=ad;!category=;kw=;tile={tile};test=;sz=728x90;{aamnfl};u={aam_did}{dcopt};ord={ord}?"; // we render TWO paths for 1x1's, this allows them to swap out as needed when 1x1's are enabled.
			var _subs = {
				aamnfl: getCookie('aamnfl'),
				aam_did: getCookie('aam_did'),
				tile: 1,
				dcopt: ';dcopt=ist'
			};
			for(_k in nfl.ads.instances){ _subs.tile = _subs.tile+1; _subs.dcopt = ';dcopt=';}
			_path2 = subPathVars(_path2,_subs);
			nfl.ads.instances["top"] = _path2;
			for(_k in nfl.ads.instances){ _subs.tile = _subs.tile+1; _subs.dcopt = ';dcopt=';}
			_path1 = subPathVars(_path1,_subs);
			nfl.ads.instances["shade"] = _path1;
			
			
			
				document.write('<scr' + 'ipt src="' + _path2.replace('{ord}',window.nfl.constants.AD_RANDOM) + '" type="text/javascript"></scr' +'ipt>');
				document.write('<scr' + 'ipt src="' + _path1.replace('{ord}',window.nfl.constants.AD_RANDOM) + '" type="text/javascript"></scr' +'ipt>');
			
			
		})();
		</script></div>
		
		
