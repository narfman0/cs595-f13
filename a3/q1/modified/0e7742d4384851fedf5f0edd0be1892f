<script>

if(navigator.userAgent.match(/iPhone/i) || navigator.userAgent.match(/iPad/i) || navigator.userAgent.match(/iPod/i)) { addEventListener("load", function(){ setTimeout(function(){ window.scrollTo(0,1); },0); }, false); }




nfl.use('mobile-panel','cookie', function (Y) {				
	if(Y.Cookie.get('carrier') == null || Y.Cookie.get('carrier') == 'other' || nfl.constants.COUNTRY_CODE != 'us') { return; }

	if (navigator.userAgent.match(/iPhone/i)) {
		var interstitialVerizonModalOverlay;

		function launchVerizonInterstitialModal(){
			var d = new Date();
			if(Y.Cookie.get('verizoninterstitial-tnf')) {
				return;
			}	else {
				Y.Cookie.set('verizoninterstitial-tnf','shown', {domain: '.nfl.com', expires: new Date(d.setTime(d.getTime() + (24 * 60 * 60 * 1000))), path: "/"});
			}

			var heading = "Watch TNF Live",
				body = '<a href="#" onclick="doNFLMobile()" style="display:block; width:100%; text-align:center;"><img src="http://s.nflcdn.com/static/content/public/static/img/mobile/NFLN_Interim_290x360_NFLN.jpg"/></a>';

			interstitialVerizonModalOverlay = new Y.MobilePanel({
				headerContent: heading,
				bodyContent: body,
				centered: true,
				render: true,
				visible: false
			});
			interstitialVerizonModalOverlay.show();
		}
		launchVerizonInterstitialModal();		
	}		
});
function doNFLMobile(){
	document.location = 'nflmobile://www.nfl.com/livestream/nflnetwork';
	setTimeout( function(){
		document.location = 'https://itunes.apple.com/us/app/nfl-mobile/id389781154?mt=8&at=11l7iA&ct=tnf';
	}, 300);
}

</script>

