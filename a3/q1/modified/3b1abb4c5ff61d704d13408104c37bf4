<script type="text/javascript">
    var pageShowing ="page1";
    function showPage(pageDivId){
        $("#"+pageShowing).fadeOut();
        $("#"+pageDivId).fadeIn();
        pageShowing = pageDivId;
    }

    function toggleThumbs(thumbAmt) {
        jQuery.noConflict();
        if (thumbAmt == "all") {
            jQuery(".slideshow-launch .scrollable-container").attr('class','scrollable-container-all');
            jQuery("#toggleimage").css("left","0px");
            //jQuery(".slideshow-launch div:first").addClass("size1 hero controls-index clearfix shadow-right")
        }
        else {
            jQuery(".slideshow-launch .scrollable-container-all").attr('class','scrollable-container size1 hero controls-index clearfix shadow-right shadow-right-ie');
        }
    }

</script>

    
    
    
    

