			var adConfig, adFactory;
			
			// instantiate the ad factory config object
			adConfig = new TiiAdConfig('3475.si2');
			adConfig.setCmSitename('cm.si2');		
			adConfig.setRevSciTracking(true);
			
			// instantiate the ad factory itself and set zone
			adFactory = new TiiAdFactory(adConfig, 'nfl/audibles');
						
			
			(function() {
				var _si_ads = function() {
					var slots = [];
		
					this.get_ad = function(width, height, container_id, position) {
						var ad = adFactory.getAd(width, height);
						
						if(position != undefined) {
							ad.setPosition(position);
						}
						
						if(container_id != undefined) {
							ad.write(container_id);
							this.establish_ad_reference(container_id);
						} else {
							ad.write();
						}
					}
		
					this.refresh_all = function() {
						try { adFactory.refreshAds(slots); } catch(e) {}
					}
			
					this.establish_ad_reference = function(container_id) {
						slots.push(container_id);	
					}
				}
	
				window.si_ads = new _si_ads();
			})();
		</script>
