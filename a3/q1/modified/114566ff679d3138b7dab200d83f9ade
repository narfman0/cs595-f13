	<!-- Default post #17253 -->
<div id="post-17253" class="post-wrapper default">
	<div class="inner inner-shadow">
		<div class="body">
			<div class="social">
				<a href="#comments"><span class="icon comments"></span></a>
				<a href="#"><span class="icon twitter post-share-twitter"></span></a>
				<a href="#"><span class="icon facebook post-share-facebook"></span></a>
