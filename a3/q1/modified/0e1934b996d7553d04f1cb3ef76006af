<script type="text/javascript">
	$(document).ready(function() {
		var postcode ='';
		var distance = '';
		$("#search-button").click(function() {
			postcode = 'postcode=' + $('#location-container #location').val().replace(/\ /g,'').toLowerCase() + ';';
			distance = 'distance=' + $('#distance-container #distance option:selected').text().trim().replace(/\ /g,'').toLowerCase() + ';';
			document.cookie=postcode;
			document.cookie=distance;
		});
	});
</script>
