<div id="ps-content" class="bucket">
  <h2>Book Description</h2>
  <div id="bookmetadata" class="buying" style="display: none">
        <input id="pubdate" type="hidden" value="2013-06-09T00:00:00" />
        <span id="pubdatelabel" class="byLinePipe"></span>
