function set_cookie(cookie_name, cookie_value,lifespan_in_days, valid_domain){
    var domain_string = valid_domain ?("; domain=" + valid_domain) : '' ;
    document.cookie = cookie_name+"="+encodeURIComponent(cookie_value)+"; max-age=" + 60 * 60 *24 * lifespan_in_days +"; path=/" + domain_string;
}
var comment_update=function(){
    //console.log("comment_update");
    idleTime=idleTime+1;
    if(idleTime<2){
        $.post("/article_reply.php",{parent_idx:'68776',mode:'update',click1:$('.comment_update').attr('data-click-num1'),click2:$('.comment_update').attr('data-click-num2')},function(update_data){
            if(update_data[0]>0){
                $.post("/common/show_comment.php",{reply_idxs:update_data[1],rereply_idxs:update_data[2]},function(data){//prepare new comment list
                    $('.update_result').html(data[0]);//updated content
                    $('.update_result').attr('data-nr',data[1]);//updated last reply idx
                    $('.update_result').attr('data-nrr',data[2]);//updated last rereply idx
                    $('.update_result').attr('data-tc',update_data[0]);//updated total new comment number
                },"json");
                //$(".update_screen").addClass('update_screen_css');
                $(".update_screen").html('Load '+Number(update_data[0])+' New Comment(s)').show('fast');
                if($(".update_screen").attr("data-status")==0){
                }
                $(".update_screen").attr("data-status","1");
            }else if(update_data[0]=="login"){
                alert(login_alert);return;
            }else if(update_data[0]==0){//no changed
            }else{alert(update_data);}//error
        },"json");
    }else{/*console.log("Move your mouse to activate.");*/}
}

if (typeof scroll_to_target_id=='function'){}else{
function scroll_to_target_id(id_name){
    if($('#'+id_name).attr('id')!=undefined){
        if (navigator.userAgent.search("Chrome") >= 0){
            parent.document.body.scrollTop=$('#'+id_name).offset().top;
        }else{
            document.getElementById(id_name).scrollIntoView();
        }
        if(id_name!="goto_box_comment"){
            if(id_name=="article-nav"){
                $('#cmt_screen').css({'opacity':'0'}).animate({opacity:1},2000);
            }else{
                $('#'+id_name).css('opacity','0').animate({opacity:1},2000);
            }
        }    
    }
}
}
if (typeof get_emoticon_set=='function'){}else{
function get_emoticon_set(data){
    $.post("/article_reply.php",{article_data:data,mode:'emoticon'},function(data){
       if(data[0]==1){
            $('.editer-content').html(data[1]);
            if(data[2]=="unlocked")$('.toolbar1 .btn-unlock').hide();
            else if(data[2]=="locked")$('.toolbar1 .btn-unlock').show('slow');
        }else{
            alert(data[0]);return;
        }
    },"json");   
}
}
if (typeof check_emoticon_use_cnt=='function'){}else{
function check_emoticon_use_cnt(){
    var temp=$('.redactor_akp_posts_reply').html();
    var emoticon_use_cnt=temp.match(/\/emoticon\//g);
    if(emoticon_use_cnt==null){
        return 1;
    }else{
        return emoticon_use_cnt.length+1;
    }
}
}
if (typeof pasteHtmlAtCaret=='function'){}else{
function pasteHtmlAtCaret(html) {
    //setEndOfContenteditable($('.redactor_akp_posts_reply').get(0));
    document.getElementById('redactor_akp_posts_reply').focus();
    var sel, range;
    if (window.getSelection) {
        // IE9 and non-IE
        sel = window.getSelection();
        //console.log(sel);
        //console.log(sel.baseNode);
        if (sel.getRangeAt && sel.rangeCount) {
            range = sel.getRangeAt(0);
            range.deleteContents();
            //range.selectNodeContents(document.getElementById('redactor_akp_posts_reply').firstChild);
            
            // Range.createContextualFragment() would be useful here but is
            // non-standard and not supported in all browsers (IE9, for one)
            var el = document.createElement("div");
            el.innerHTML = html;
            
            var frag = document.createDocumentFragment(), node, lastNode;
            
            while ( (node = el.firstChild) ) {
                lastNode = frag.appendChild(node);
            }
            range.insertNode(frag);

            if (lastNode) {// Preserve the selection
                range = range.cloneRange();
                range.setStartAfter(lastNode);
                range.collapse(true);
                sel.removeAllRanges();
                sel.addRange(range);
            }
        }
    } else if (document.selection && document.selection.type != "Control") {
        document.selection.createRange().pasteHTML(html);// IE < 9
    }
}
}
if (typeof setEndOfContenteditable=='function'){}else{
function setEndOfContenteditable(contentEditableElement){
    var range,selection;
    if(document.createRange){//Firefox, Chrome, Opera, Safari, IE 9+
        range = document.createRange();//Create a range (a range is a like the selection but invisible)
        range.selectNodeContents(contentEditableElement);//Select the entire contents of the element with the range
        range.collapse(false);//collapse the range to the end point. false means collapse to end rather than the start
        selection = window.getSelection();//get the selection object (allows you to change selection)
        selection.removeAllRanges();//remove any selections already made
        selection.addRange(range);//make the range you have just created the visible selection
    }else if(document.selection){//IE 8 and lower
        range = document.body.createTextRange();//Create a range (a range is a like the selection but invisible)
        range.moveToElementText(contentEditableElement);//Select the entire contents of the element with the range
        range.collapse(false);//collapse the range to the end point. false means collapse to end rather than the start
        range.select();//Select the range (make it the visible selection
    }
}
}

$(document).ready(function(){
    $('.btn-cmt').on('click',function(){window.parent.$('.login_btn').click();});
    $('.akp_category').on('click',function(){
        var slug = $(this).attr('data-value');
        var url="/category/"+slug+"/page/1";
        location.href=url;return; 
        if (/MSIE (10.\d+);/.test(navigator.userAgent)||/MSIE (9.\d+);/.test(navigator.userAgent)||/MSIE (8.\d+);/.test(navigator.userAgent)||/MSIE (7.\d+);/.test(navigator.userAgent)){ //test for MSIE x.x;
            updateSite(url,'ie9');
        }else{
            (function(window,undefined){
        		if(document.location.protocol==='file:'){alert('The HTML5 History API (and thus History.js) do not work on files, please upload it to a server.');}
        		var
        			History = window.History,// Note: We are using a capital H instead of a lower h
        			State = History.getState(),
        			$log = $('#log');
                History.Adapter.bind(window,'statechange',function(){ // Note: We are using statechange instead of popstate
                	var State = History.getState();//Note: We are using History.getState() instead of event.state
                    if(prev_updateSite!=State.title){
                        History.log('statechange:', State.data, State.title, State.url);
                        updateSite(State.title,'other');
                        prev_updateSite=State.title;        
                    }
                });
        	})(window);
            window.History.pushState({page:url},url,url);
        }
    });
    get_emoticon_set($('.toolbar1 .selected').attr('data-value'));
    $('.toolbar1 .btnsticker').on('click',function(e){
