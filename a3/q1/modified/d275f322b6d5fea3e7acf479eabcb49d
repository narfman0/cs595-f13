    $('a.reply').live('click',function(e){//rereply insert box open 1
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},500);
            if(user_id!=""){
                var send_comment_box=$(this).closest('.box_comment').find('.send_comment');
                var akp_posts_rebtn=send_comment_box.children('.akp_posts_rebtn');//send btn
                var akp_posts_reedit=send_comment_box.children('.akp_posts_reedit');//edit btn
                akp_posts_rebtn.attr('data-value',$(this).attr('data-value'));
                akp_posts_rebtn.show();//show send btn
                akp_posts_reedit.hide();//hide edit btn
            
                var send_comment_box=$(this).closest('.box_comment').find('.send_comment');
                if(send_comment_box.find('.akp_posts_rereply').val()==''){
                    if(send_comment_box.attr('data-flag')=="false"){
                        send_comment_box.slideDown('fast');
                        send_comment_box.attr('data-flag',"true");
                    }else{
                        send_comment_box.slideUp('fast');
                        send_comment_box.attr('data-flag',"false");
                    }
                }else{
                    send_comment_box.find('.akp_posts_rereply').val('');
                }
            }else{alert(login_alert);return;}
        }
    });
    $('a.reply_re').live('click',function(e){//rereply insert box open 1
    
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},500);
        
            if(user_id!=""){
                var to='@'+$(this).attr('data-to')+' ';
                var send_comment_box=$(this).closest('.box_comment').find('.send_comment');
                var akp_posts_rebtn=send_comment_box.children('.akp_posts_rebtn');//send btn
                var akp_posts_reedit=send_comment_box.children('.akp_posts_reedit');//edit btn
                akp_posts_rebtn.attr('data-value',$(this).attr('data-value'));
                akp_posts_rebtn.show();//show send btn
                akp_posts_reedit.hide();//hide edit btn
                
                if(send_comment_box.find('.akp_posts_rereply').val()==to){
                    if(send_comment_box.attr('data-flag')=="false"){
                        send_comment_box.slideDown('fast');
                        send_comment_box.find('.akp_posts_rereply').val(to);
                        send_comment_box.attr('data-flag',"true");
                    }else{
                        send_comment_box.slideUp('fast');
                        send_comment_box.find('.akp_posts_rereply').val('');
                        send_comment_box.attr('data-flag',"false");
                    }    
                }else{
                    if(send_comment_box.attr('data-flag')=="false"){
                        send_comment_box.slideDown('fast');
                        send_comment_box.find('.akp_posts_rereply').val(to);
                        send_comment_box.attr('data-flag',"true");
                    }else{
                        send_comment_box.find('.akp_posts_rereply').val(to);
                    }
                }
            }else{alert(login_alert);return;}
        }
    });
    
    $(".akp_posts_rereply").live('click',function(e){//rereply textarea box wider 2
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},500);
