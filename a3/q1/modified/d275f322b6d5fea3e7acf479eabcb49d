        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
        var report_content=$(this).closest('.report_screen').children('.modal-body').find('input[name="report_value"]:checked');
        var report_option=$(this).closest('.report_screen').children('.modal-body').find('.report_option');
        report_content.prop('checked',false);
        report_option.val('');
        $('#report_screen').modal('hide');
        }
    });
    $('a.rereport').live('click',function(e){//reply report
