    $('.akp_posts_rebtn').live('click',function(e){//rereply insert send 3
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);

	var akp_posts_rebtn=$(this);
        if(user_id!=""){
            var co = $.cookie("write_");
            if(co==null){
                var akp_posts_rereply=$(this).parent().children('.akp_posts_rereply');
                var send_comment_div=$(this).parent();
                if(akp_posts_rereply.attr('value')!=""){
                    $.post("/article_reply.php",{reply_data:$(this).attr('data-value'),mode:'rereply_write',akp_posts_rereply:akp_posts_rereply.attr('value')},function(data){
                        if(data[0]==1){
                            $('.comment_count').text(Number($('.comment_count').text())+1);
                            $('.akp_posts_rereply').val("");
                            //alert("Comment Inserted.");
                            $(data[1]).insertBefore(send_comment_div);
                            
                            $('.send_comment').slideUp('fast');
                            $('.send_comment').attr('data-flag',"false");
                            
                        }else if(data[0]=="200"){
                            $('.akp_posts_rereply').val("");
                            alert("You've reached the maximum of 200 comments and posts.");return;
                        }else if(data[0]=="ban"){
                            alert("You can't post another comment until "+data[1]+" EST.");
                            return;
                        }else if(data[0]=="ip_ban"){
                            alert(data[1]);
                            return;
                        }else if(data[0]=="login"){
                            alert(login_alert);return;
                        }else{/*alert(data);return;*/}
                    },"json"); 
                }    
            }else{alert(wait_alert);return;}
        }else{alert(login_alert);return;}
	}else{}
    });
    
    $('a.reedit_re').live('click',function(e){//rereplay edit
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
            reedit_re=$(this).closest('.box_friend_comment').find('.rereply_content');
            var send_comment_box=$(this).closest('.box_comment').find('.send_comment');
            var akp_posts_rebtn=send_comment_box.children('.akp_posts_rebtn');//send btn
            var akp_posts_reedit=send_comment_box.children('.akp_posts_reedit');//edit btn
            akp_posts_rebtn.hide();//hide send btn
            akp_posts_reedit.attr('data-value',$(this).attr('data-value')).show();//show edit btn
            
            if(send_comment_box.attr('data-flag')=="false"){
                send_comment_box.slideDown('fast');
                send_comment_box.find('.akp_posts_rereply').val(reedit_re.text());
                send_comment_box.attr('data-flag',"true");
            }else{
                send_comment_box.slideUp('fast');
                send_comment_box.find('.akp_posts_rereply').val('');
                send_comment_box.attr('data-flag',"false");
            }
        }
    });
    $('.akp_posts_reedit').live('click',function(e){//rereply edit send
        e.preventDefault();
	    if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
            var akp_posts_rereply=$(this).parent().children('.akp_posts_rereply');
            var send_comment_box=$(this).closest('.box_comment').find('.send_comment');
            var akp_posts_rebtn=send_comment_box.children('.akp_posts_rebtn');//send btn
            var akp_posts_reedit=$(this);//edit btn
            
            if(akp_posts_rereply.attr('value')!=""){
                $.post("/article_reply.php",{rereply_data:$(this).attr('data-value'),mode:'rereply_edit',akp_posts_rereply:akp_posts_rereply.attr('value')},function(data){
                    if(data[0]==1){
                        akp_posts_reedit.hide();//hide edit btn
                        akp_posts_rebtn.show();//show send btn
                        reedit_re.html(data[1]);
                        send_comment_box.slideUp('fast');
                        send_comment_box.attr('data-flag',"false");
                    }else if(data[0]=="login"){
                        alert(login_alert);return;
                    }else{/*alert(data);return;*/}
                },"json"); 
            }    
	    }else{}
    });
    $('a.del_status_re').live('click',function(e){//rereplay delete
        e.preventDefault();
        var del_what=$(this).closest('.box_comment');
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
            if(user_id!=""){
                var choose = confirm("Do you want to delete it?");
                if(choose){
                    $.post("/article_reply.php",{rereply_data:$(this).attr('data-value'),mode:'rereply_delete'},function(data){
                        if(data==1){
                            $('.comment_count').text(Number($('.comment_count').text())-1);
                            del_what.remove();
                            comment_loadData('',orderby,'n');
                            //alert("Comment Deleted.");return;
                        }else if(data=="login"){
                            alert(login_alert);return;
                        }else{/*alert(data);return;*/}
                    });
                }else{return;}
            }else{alert(login_alert);return;}
        } 
    });
