<script type="text/javascript">
    //PAGE TYPE
var ph_page_type='single';
var ph_post_id='396973';
    
    //AD PAGE
    var ph_ad_page = 'no';
    

    //BACK AD PAGE
    var ph_back_ad_page = 'no';

    //BACK AD COUNTRY
    var ph_back_ad_country = 'no';

    //IS IT A DEFINED COUNTRY?
