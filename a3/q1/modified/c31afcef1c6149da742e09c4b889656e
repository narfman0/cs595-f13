function belologinFromNav() {
	//var username = bimreg.getuserkey("username");
	//[bim_reg] update -Begin-
	var username = bim_reg.m.user.getUsername();
	//[bim_reg] update -End-
if($("#bimregStatusBox").length){
        var toolbarObject = $("#bimregStatusBox");
}else{
	var toolbarObject = $("#toolbar-status");
}
	$(toolbarObject).removeClass("loggingiIn").removeClass("hide");
	$(toolbarObject).find("ul").show();
	$(toolbarObject).find("#status-loggingin").hide();
	$(".status-out").hide();
	$("li#login-item.top-level-item").addClass("hide");
	$(toolbarObject).show();
	$("li.status-loggedin").show();
	//$('span.username').text(bimreg.getuserkey("username"));
	//[bim_reg] update -Begin-
	$('span.username').text(bim_reg.m.user.getUsername());
	//[bim_reg] update -End-
	$(toolbarObject).find('li#status-welcome').html('Welcome, <a href=\"/register?profile='+username+'"\'>'+username+'<\/a>');
	//if(typeof bimreg.getuseravatar() != "undefined"){
		//var avatarImageSrc="http://bim-storage.s3.amazonaws.com/BIM/"+bimreg.getuserkey('key')+".jpg?cb="+Math.floor(Math.random()*90000) + 10000;
		//$("div#avatar-image").html("<img src='"+avatarImageSrc+"' width='40' height='34'/>");
	//};
	//[bim_reg] update -Begin-
	$("div#avatar-image").html("<img src='"+bim_reg.m.user.getAvatarUrl()+"' width='40' height='34'/>");
	//[bim_reg] update -End-
	$(toolbarObject).find('li#status-profile').html("<a href='/register?profile="+username+"'>My Profile<\/a>");
	var surveyObject = $('#content-two table .cmP_survey') 
	$(surveyObject).find('.cmP_question, .cmP_response, .cmP_responses, #survey-submit, .cmP_input, .cmP_label, #survey-signup-newsletters').removeClass("hide");
	$("#survey-reg-prompt").addClass("hide");
	$("form[name=loginformnav] input[name=login-nav-password]").val("");
}
    
function belologoutFromNav() {
if($("#bimregStatusBox").length){
        var toolbarObject = $("#bimregStatusBox");
}else{
	var toolbarObject = $("#toolbar-status");
}
	$(".status-out").show();
	$("li#login-item.top-level-item, li#login-item.top-level-item #login-button").removeClass("hide");
	$(toolbarObject).hide();
	$("li.status-loggedin").hide();
	$("#status-loggingin").hide();
	}
    	

function onloadlogout(status) {
	$("li#login-item.top-level-item").removeClass("hide");
	$(".signin").removeClass("menu-open");
	BELOJS.reg.pagecount.reset();
	belologoutFromNav();
}

//bimreg.listener.addListener("userLogout",onloadlogout);
//[bim_reg] update -Begin-
bim_reg.subscribeToChannel("user_logout", function(){document.location = '/'; });
//[bim_reg] update -End-

var carouselRan = false;

function modalShow() {
	$("#regModal").removeClass("hide").dialog();
	BELOJS.reg.showLoginModal();
	//bimreg.listener.addListener("userLogin",BELOJS.reg.loginModalClose);
	//[bim_reg] Update -Begin-
	bim_reg.subscribeToChannel("user_login_success", BELOJS.reg.loginModalClose);
	//[bim_reg] Update -End-
	if(!carouselRan){
		setTimeout("loadCarousel()",500);
		carouselRan = true;
	}
	return false;
}
    
$(function() {
    if(document.URL.indexOf('form=int') > -1 || document.URL.indexOf('form=signup') > -1) {
        $(function() {
            window.onunload = function() { bim_reg.l.cookies.set("BELO_OptOut","true",30); }
        })
    }
    
    $("#status-signout a").click(function(e) {
        belologout(belologoutFromNav);
        return false;
    });

    $("#login-button a.signin").click(function(e) {
    	if($('#regModal').html().length<5){
    		jQuery.post('/templates/belo_registration_modal', function(data) {
  				$('#regModal').html(data);
			});
    	}
    	setTimeout("modalShow()",500);
    	return false;
	});
});

