cssdropdown.startchrome("chromemenu");
  }

  // Hide unfulfilled high impact ads

  var setMenu = 0;

  setInterval(function() {

     if (document.getElementById('chromemenu') && !(setMenu))
     {
        cssdropdown.startchrome("chromemenu");
        setMenu = 1;
     }

     var divID = 'div-gpt-ad-1360189087376-0';

     if (document.getElementById(divID))
     {
         $("#" + divID + " iframe").each(function (i) {

         var currentID = $(this).attr('id');

         if (!(currentID.indexOf('hidden') >= 0))
         {
             if (this.height)
                $("#" + divID).show();
             else
                $("#" + divID).hide();
         }

         });
     }

  }, 500);

