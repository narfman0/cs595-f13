  var pluckEntries = pluckEntries || [];

  if (typeof pluckCats != 'undefined' && pluckCats == 'MT Blog')
     {
       uuid = pluckID;
       var stylesheet = document.createElement("link");
       stylesheet.type = "text/css";
       stylesheet.href = "http://www.boston.com/css/bcom-comments.css?v=1"
       stylesheet.rel = "stylesheet";
       document.body.appendChild(stylesheet);
	$.getScript('http://www.boston.com/js/lib/tiny_mce/tiny_mce.js', function(data, textStatus, jqxhr) {
           var script   = document.createElement("script");
           script.type  = "text/javascript";
           script.src   = "http://www.boston.com/js/bcom-comments.js?v=1";
           document.body.appendChild(script);
        });   
}
  else if(pluckEntries.length>0){
    var script = document.createElement('script');
    script.type = "text/javascript";
    script.src = "http://www.boston.com/js/ugc-blog-index.js?v=3";
    document.body.appendChild(script);
  }


</script>

