    <script type="text/javascript">
		function moveSlider(direction){
		    var slideList = $("#gallery-nav-slides");
		    var currentSlide = $("#gallery-nav-slides > li.current");
		    
		    var nextSlide;
		    var multiplier = -1;

		    if(direction){
	            nextSlide = currentSlide.next('li.gallery-nav-img');
		    } else {
	            nextSlide = currentSlide.prev('li.gallery-nav-img');
	            multiplier = 1;
		    }
		    
		    if(nextSlide.children().length <= 0){
		    	//Can't go any more.
		        return;   
	        }
		    
		    if(nextSlide.next('li.gallery-nav-img').children().length <= 0){
		    	$("#gallery-img-nav-next").addClass('hidden');
		    } else if($("#gallery-img-nav-next").hasClass('hidden')){
		    	$("#gallery-img-nav-next").removeClass('hidden');
		    }
		    
		    if(nextSlide.prev('li.gallery-nav-img').children().length <= 0){
		    	$("#gallery-img-nav-prev").addClass('hidden');
		    } else if($("#gallery-img-nav-prev").hasClass('hidden')){
		    	$("#gallery-img-nav-prev").removeClass('hidden');
		    }
		    
		    currentSlide.removeClass("current");
		    var currentMargin = parseInt(slideList.css('margin-left').replace('px', ''));
		    var newLeftMargin = currentMargin +  multiplier * 90;
		    slideList.animate({ 'margin-left': newLeftMargin + 'px' }, { complete : function() { nextSlide.addClass("current"); } });
		}

	</script>
</div>
		
