var adsenseClickAlreadyTracked = false;

function trackAdsenseClick() {
	if (!adsenseClickAlreadyTracked) {
		adsenseClickAlreadyTracked = true;
		var theLink = this;
		
		require(["jquery"], function() {
			var postData = {};
			
			if (window.document && window.document.referrer) {
				postData.refererUri = window.document.referrer;
			}
			
			if (theLink.hasOwnProperty("href")) {
				postData.clickDest = theLink.href;
			}
			
			jQuery.ajax({
				url: "/ajax/trackAdsenseClick",
				type: "POST",
				data: postData,
				dataType: "json"
			});
		});
	}
}
</script>
