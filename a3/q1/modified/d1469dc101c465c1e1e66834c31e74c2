			require(["jquery"], function() {
				<!-- Endless Scroll -->
				$(window).scroll(function () { 
					if (!scrolling && $(window).scrollTop() >= $(document).height() - $(window).height() - 100) {
						getContent();
					}
				});
			});
			
			function getContent(){
				if (!scrolling) {
				   	scrolling = true;
				   	
				  //Loading GIF
				   	$("#endlessGrid").append('<li id="loading"><img src="http://staticcontent.chacha.com/b77c19801e60d8303b2ca6a3e39cd31f/images/loading.gif" alt="Loading more Galleries" style="display: block; margin-left: auto; margin-right: auto;"/></li>');
				   
					$.ajax({
						type: "GET",
						url: "/ajax/galleryGrid",
						data: {offset : scrollOffset, showAd : false},
						dataType: "html"
					}).done(function( html ) {
						$("#loading").remove();
						$("#endlessGrid").append(html);
						scrollOffset += 9;
						scrollPage++;
						scrolling = false;
					});
			   	}
			}
		</script>
    	
