OW" /><meta property="og:description" content=" Andrew Vigliotti &amp; Shane Miller host a comprehensive two-hour weekly sports show that covers the most pressing matters of the week. The Vig &amp; Miller Show is on The Sports Chronicle Radio Network. We will breakdown Week 3 and shed light on the National Football League. We&#39;ll also look into Week 4! The Manny Machado injury was gruesome Monday. We will update his condition and debate how it affects he and the team moving forward. Shaq is joining the Kings as a minority owner! We will debate this as well. All this and MORE on The Vig &amp; Miller Show at 7 PM EST " /><meta property="og:image" content="http://cdn.btrcdn.com/pics/showpics/large/2ed4d3b3-d540-4fbb-90a8-8641d4b2341b_thesportschroniclesradionetwork.jpg" /><meta property="og:site_name" content="BlogTalkRadio" /><meta property="og:title" content="The Vig &amp; Miller Show" /><meta property="og:type" content="blogtalk:episode" /><meta property="og:url" content="http://www.blogtalkradio.com/thesportschroniclesradionetwork/2013/09/24/the-vig-miller-show" /><meta property="og:video" content="https://player.cinchcast.com/assets/player/jwplayer-5.9.2156.swf?config=https://player.cinchcast.com/config?platformId%3d1%26assetId%3d5322601%26assetType%3dsingle%26version%3d2.0" /><meta property="og:video:secure_url" content="https://player.cinchcast.com/assets/player/jwplayer-5.9.2156.swf?config=https://player.cinchcast.com/config?platformId%3d1%26assetId%3d5322601%26assetType%3dsingle%26version%3d2.0" /><meta property="og:video:width" content="400" /><meta property="og:video:height" content="370" /><meta property="og:video:type" content="application/x-shockwave-flash" /><meta property="profile:username" content="TSCRN" /><meta property="profile:image" content="http://cdn.btrcdn.com/pics/hostpics/2ed4d3b3-d540-4fbb-90a8-8641d4b2341b_thesportschroniclesradionetwork.jpg" /><meta property="profile:url" content="http://www.blogtalkradio.com/thesportschroniclesradionetwork" /><meta property="profile:description" content="Join our hosts for all the REAL sports talk involving all of your favorite sports. From the NFL to the NBA, MLB, NASCAR and more we have you covered!" /><meta property="fb:app_id" content="72364733256" /><meta property="fb:admins" content="1497081862,634878728,1266030015,620692898,1497081862,634878728,1266030015,100000467586047,1335360029,305767,1180318833,1335360029,686929151,1832820561,1180318833,1335360029,686929151,100002444163843,100006414641203,100003196743945,603325027,508193487,100003773906712,100006414641203,100003196743945,561060199,603325027,508193487,100003773906712,100004000941064,1138243919,100004000941064,541734815,1075606712,541734815,100003976890888,1211355076,100003671419862,1180318833,100000174078357,100003671419862,1180318833,100000174078357" /><script>
var UserAccount = {
    cookie: '',
    userCookie: '',
    isAuthenticated: false,
    isHost: false,
    isPremium: false,
    hasAccount: false,
    isTrafficDriving: false,
    hasRememberMe: false,

    init: function () {
        this.parseCookie();

        this.setTrafficDrivingStatus();
        this.setAuthenticationStatus();
        this.setUserAccountStatus();
    },
    parseCookie: function () {
        this.cookie = document.cookie.toLowerCase();
        var userCookieIndex = this.cookie.indexOf('user=');
        var userCookieTerminatorIndex = this.cookie.substring(userCookieIndex).indexOf(';');
        this.userCookie = userCookieTerminatorIndex > -1 ? this.cookie.substr(userCookieIndex, userCookieTerminatorIndex) : this.cookie.substring(userCookieIndex);
    },
    setTrafficDrivingStatus: function () {
        var aidCookieValueKeys = ['jema', 'adon', 'adcom', 'adkno', 'tab', '65687978','7376','90277','4806'];
        for (var i = 0; i < aidCookieValueKeys.length; i++) {
            if (this.cookie.indexOf('aid=' + aidCookieValueKeys[i] + ';') > -1)
                this.isTrafficDriving = true;
        }
    },
    setAuthenticationStatus: function () {
        var authedCookieKey = 'authed=';
        var authedCookieIndex = this.cookie.indexOf(authedCookieKey);
        this.isAuthenticated = (authedCookieIndex > -1 && !isNaN(this.cookie.substr(authedCookieIndex + authedCookieKey.length, 1)));
    },
    setUserAccountStatus: function () {
        if (this.userCookie.length == 0) return;

        var hostIdCookieKey = 'hid=';
        var hostIdFirstCharacter = this.userCookie.substr(this.userCookie.indexOf(hostIdCookieKey) + hostIdCookieKey.length, 1);
        this.isHost = (!isNaN(hostIdFirstCharacter) && Number(hostIdFirstCharacter) > 0);

        var hasPremiumAccountKey = 'prem=';
        this.isPremium = this.userCookie.substr(this.userCookie.indexOf(hasPremiumAccountKey) + hasPremiumAccountKey.length, 4).toLowerCase() == 'true';
        
        this.hasAccount = this.cookie.indexOf('hasbtracct=true') > -1;
        
        this.hasRememberMe = this.cookie.indexOf('mybtracct') > -1;
    }
};

;UserAccount.init();
</script><script>
var EarlyLoadingCssManager = {
    init: function () {
        this.addHideCssClassesBasedOnUserStatus();
    },
    addHideCssClassesBasedOnUserStatus: function () {
        var hideCssClasses = [];

        if (UserAccount.isAuthenticated || UserAccount.hasRememberMe) hideCssClasses.push('IfAuthenticated');
        else                                    hideCssClasses.push('IfNotAuthenticated');
        if (UserAccount.isTrafficDriving)       hideCssClasses.push('IfTrafficDrivingUser');
        if (UserAccount.isHost)                 hideCssClasses.push('IfUserIsAHost');
        else                                    hideCssClasses.push('IfUserIsNotAHost');
        if (UserAccount.isPremium)              hideCssClasses.push('IfCurrentIsPremium');
        else                                    hideCssClasses.push('IfCurrentIsNotPremium');
        if (UserAccount.hasAccount)             hideCssClasses.push('IfUserHasAnAccount');

        if (UserAccount.isAuthenticated &&
           !UserAccount.isHost)                 hideCssClasses.push('IfAuthenticatedAndIsNotAHost');

        for (var i = 0; i < hideCssClasses.length; i++) {
            hideCssClasses[i] = '.hide' + hideCssClasses[i] + ' { display: none; }';
        }

        this.addCssToHead(hideCssClasses.join(''));
    },
    addCssToHead: function (cssString) {
        var styleElement = document.createElement('style');
        styleElement.type = 'text/css';

        if (styleElement.styleSheet)
            styleElement.styleSheet.cssText = cssString;
        else
            styleElement.appendChild(document.createTextNode(cssString));

        document.getElementsByTagName('head')[0].appendChild(styleElement);
    }
};
;EarlyLoadingCssManager.init();
</script>
