<script type="text/javascript">
    (function ($, undefined) {
        function doJail(selector) {
            if (!$.fn.jail)
                return;

            var validImages = [];

            $(selector).each(function () {
                var img = $(this);
                var dataHref = img.data("href");
                if (typeof (dataHref) !== "undefined") {
                    img.attr("data-src", dataHref).removeAttr("data-href");
                }
                
                if (typeof(img.data("src")) !== "undefined") {
                    validImages.push(img);
                }

            }).removeClass("lazyDocumentReady lazyWindowLoad deferredLoad");

            $(validImages).jail();
        }

        $(document).ready(function () {
            doJail("img.lazyDocumentReady, img.deferredLoad");
        });

        $(document).on("domUpdated", function() {
            doJail("img.deferredLoad, img.lazyDocumentReady, img.lazyWindowLoad");
        });

        $(window).load(function () {
            doJail("img.lazyWindowLoad, img.deferredLoad");
        });
    })(jQuery);
</script>
    

