    var url_to_load     = null;
    var name_to_load    = null;
    var html_to_load    = null;
    var time_to_load    = null;    
    var width_to_load   = 300;
    var height_to_load   = 300;
    var country_to_load = null;

   
        country_to_load = 'else'
    
        _dict_ads = dict_ads['else'] 
        
        for (_key in _dict_ads) {
            if ( jQuery.cookie("steponlayer_corner_"+_key)!=1 ){
                name_to_load   = _key
                time_to_load   = _dict_ads[_key][1]
                url_to_load    = _dict_ads[_key][2]
                height_to_load = _dict_ads[_key][3]
                width_to_load  = _dict_ads[_key][0]

                break;// outer_loop;
            }
        }
        if (name_to_load == null){
        } //if has all the cookies and none has expired cancel popup
        else if(screen.availWidth<650 || screen.availHeight<600){}//too small
        else if(jQuery.cookie("steponlayer_corner_"+"___capoday")!=1){

            $( "#dialog" ).dialog({

                autoOpen: false,
                            width: width_to_load,
                            height: height_to_load,
                position:['right','bottom'],
                    });
            $( "#dialog").html("<a href='"+url_to_load+ "' target='_blank'><iframe id='framex' style='overflow:hidden;padding:0px;margin:0px' width='"+width_to_load+"' height='"+height_to_load+"' frameborder='0' src='"+url_to_load+"'></iframe></a>") 
            $("#dialog").dialog("open");  
                  setcookie(name_to_load,time_to_load)
          setcookie("___capoday",60*24)

  var W=0, H=0, X=0, Y=0;

   W = $("#framex").width();
   H = $("#framex").height();
   X = $("#framex").position().left;
   Y = $("#framex").position().top;
   $("#framex").after('<div class="overlay" />');
   $("#framex").next('.overlay').css({
        width: W,
        height: H,
        left: X,
        top: Y        
    });
        }
   
});


function setcookie(name,time_min){
        var date = new Date();
        date.setTime(date.getTime() + (time_min * 60 * 1000));
        $.cookie("steponlayer_corner_"+name, 1, { expires : date , path: '/' });
} 

     
