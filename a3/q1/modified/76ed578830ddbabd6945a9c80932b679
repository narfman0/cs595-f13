            <div id="sn-marketing-article-right"></div>
<script type="text/javascript">
(function($) {
    function weightPromoItems(items, weights) {
        var totalWeight = 0;
        $.each(weights, function() {
            totalWeight += this;
        });
        var weightedItems = [];
        var currentItem = 0;
        while (currentItem < items.length){
            for (i = 0; i < weights[currentItem]; i++) {
                weightedItems.push(items[currentItem]);
            }
            currentItem++;
        }
        return {
            items: weightedItems,
            total: totalWeight
        };
    }

    var sport = 'ncaa-football';

    var products = [];
    var weights = [];

    switch (sport) {
        case 'mlb':
            products = ['fanduel', 'fanatics', 'covers'];
            weights = [100, 300, 1];
            break;
        case 'nhl':
            products = ['fanatics'];
            weights = [1];
            break;
        case 'nba':
            products = ['fanatics'];
            weights = [1];
            break;
        case 'nfl':
        case 'ncaa-football':
        case 'ncaa-basketball':
        case 'nascar':
        default:
            products = ['fanatics', 'covers'];
            weights = [300, 1];
    }

    weightedItems = weightPromoItems(products, weights);

    var pick = Math.floor(Math.random() * weightedItems.total);

    if (weightedItems.items[pick] == "fanatics") {
        var url = "http://www.sportingnews.com/ad-modules/sportingnews/fanatics/ncaa-football/300x250?sport=ncaa-football&ad_page=article";
    } else {
        var url = "http://www.sportingnews.com/ad-modules/sportingnews/"
            + weightedItems.items[pick]
            + ".php?sport=ncaa-football";
    }

    $.ajax({
        url: url,
        success: function(data) {
            $('#sn-marketing-article-right').html(data).show();
        }
    });
})(jQuery);
</script>

