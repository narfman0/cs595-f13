            <script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?sensor=false"></script>
<style>
#map_container {
    position:relative;
    width:668px;
    background-color:#e3e9f0;
}
#map_loading {
   position:absolute;
   top:210px;
   left:280px;
   visibility:hidden; 
}

.mapRendering {
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=50)";
    filter: alpha(opacity=30);
    -moz-opacity: 0.3;
    -khtml-opacity: 0.3;
    opacity: 0.3;
}
</style>

<script type="text/javascript">
function showHideMapLoading(val){
    var loadingDiv = document.getElementById('map_loading');
    var mapDiv = document.getElementById('map_canvas');
    loadingDiv.style.visibility = val;
    mapDiv.className = (val==="visible") ? "mapRendering" : "";
    if(val==="visible"){
        mapDiv.innerHTML = "";
    }
}

var map = false;
function initialize() {
    showHideMapLoading("visible");
    var latlng = new google.maps.LatLng(0,0);
    var myOptions = {
        zoom: 8,
        zoomControl:false,
        streetViewControl:false,
        panControl:false,
        center: latlng,
        mapTypeControl:false,
        mapTypeId: google.maps.MapTypeId.TERRAIN
    };
    map = new google.maps.Map(document.getElementById("map_canvas"), myOptions);

    //var host = 'share.gps.motionxlive.com';
    var host = "share.gps.motionxlive.com";
    var share = new google.maps.KmlLayer('http://' + host + '/shr/x/kmz/a1bab41c514f0443c46bac2a0b37c7fd');
    share.setMap(map);
    var t = setTimeout(function() { force(share);}, 4000);
}
function force(share) {
    var bounds = map.getCenter();
    //console.log("share status:" + share.getStatus());
    // console.log("share getDefaultViewport:" + share.getDefaultViewport());
    // console.log("share status:" + share.getStatus());
    // console.log("share getMap:" + share.getMap());
    if(bounds.lat()==0 && bounds.lng()==0){
        initialize();
    }
    else {
        showHideMapLoading("hidden");
        // if(bounds){
        //     console.log(bounds.lat());
        //     console.log(bounds.lng());
        // }
        share.getMap().setOptions(
            {
                center: new google.maps.LatLng(bounds.lat(),bounds.lng()),
                zoomControl:true,
                panControl:true,
                mapTypeControl:true,
                streetViewControl:true
            });
    }
}
</script>

