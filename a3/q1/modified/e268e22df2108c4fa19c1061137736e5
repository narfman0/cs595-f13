<script>  
function eraseCookie() 
{
    document.cookie = "Device=false" + "; expires=-1" + "; path=/" + "; domain=motionx.com";
}

function reload_page() { window.location="http://gps.motionx.com/maps/a1bab41c514f0443c46bac2a0b37c7fd"; }



function set_cookie ( name, value )
{
	
//force expire previous cookie
eraseCookie();
	
	//Prepare new cookie
	//Cookie strings
	var cookie_string = name + "=" + value;
	var expires_date = "; expires=";
	var path = "; path=/";
	var domain = "; domain=motionx.com";

	//set the cookie
	document.cookie = cookie_string + expires_date + path + domain;
	
	
}



</script> 


    
