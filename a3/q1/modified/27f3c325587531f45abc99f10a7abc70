jQuery(document).ready(function() {
if ( (document.getElementById('storyInline') != null) && (jQuery('blockquote').length > 0) ) {
embeddedMediaHeight = jQuery('#storyInline').height();
embeddedMediaTopOffset = jQuery('#storyInline').offset().top;
blockQuoteTopOffset = jQuery('blockquote').offset().top;
if (embeddedMediaTopOffset > blockQuoteTopOffset) {
offsetDifference = (embeddedMediaTopOffset - blockQuoteTopOffset);
} else {
offsetDifference = (blockQuoteTopOffset - embeddedMediaTopOffset);
}
if (embeddedMediaHeight > offsetDifference/2) {
jQuery('blockquote').css({'width':'275px'});
} else {
jQuery('blockquote').css({'width':'600px'});
}
}
