(function(){
var _el = document.getElementById('Article'+GEL.thepage.pageinfo.key); // first look for the current article
if (!_el) { // if not current article, find last article
  try {
    var _lis = document.getElementById('ody-rightrail-moreheadlines-scroll-list').getElementsByTagName('li');
    _el = _lis[_lis.length - 1];
  } catch (ex) {}
}
if (_el)  { _el.parentNode.removeChild(_el); } // remove article, if found
// apply class to remove dotted line from last child
try {
  var _lis = document.getElementById('ody-rightrail-moreheadlines-scroll-list').getElementsByTagName('li');
  _el = _lis[_lis.length - 1];
  _el.className+=" last";
} catch (ex) {}
})(); 
</script>
