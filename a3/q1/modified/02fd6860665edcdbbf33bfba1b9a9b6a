    $('a.reply_plus').live('click',function(e){//reply like
        e.preventDefault();
    	if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
    
            var icon_plus=$(this);
            if(user_id!=""){
                if(user_id==icon_plus.attr("data-usr")){
                    alert("You cannot vote on your own comment or post.");return;
                }else{
                    var total=icon_plus.parent().parent().children('.total');
                    var num_votes=icon_plus.parent().parent().children('.num_votes');
                    $.post("/article_reply.php",{reply_data:icon_plus.attr('data-value'),mode:'reply_like'},function(data){
                        if(data==1){
                            total.text(Number(total.text())+1);
                            num_votes.text(Number(num_votes.text())+1);
                        }else if(data=="login"){
                            alert(login_alert);return;
                        }else if(data=="vote_five"){
                            alert("Maximum 5 votes for same person per 24hours.");return;
                        }else if(data=="vote_one"){
                            alert("Maximum 1 vote for same content per 24hours.");return;
                        }else if(data=="vote_minus_cancel"){
                            total.text(Number(total.text())+1);    
                            num_votes.text(Number(num_votes.text())-1);
                        }else if(data=="ip_ban"){
                            alert("Banned for multiple trolling accounts.");return;
                        }else{/*alert(data);return;*/}
                    });    
                }
            }else{alert(login_alert);return;}   
    	}else{} 
    });
    $('a.reply_minus').live('click',function(e){//reply dislike
        e.preventDefault();
       if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
            var icon_minus=$(this);
            if(user_id!=""){
                if(user_id==icon_minus.attr("data-usr")){
                    alert("You cannot vote on your own comment or post.");return;
                }else{
                    var total=icon_minus.parent().parent().children('.total');
                    var num_votes=icon_minus.parent().parent().children('.num_votes');
                    $.post("/article_reply.php",{reply_data:icon_minus.attr('data-value'),mode:'reply_dislike'},function(data){
                        if(data==1){
                            total.text(Number(total.text())-1);    
                            num_votes.text(Number(num_votes.text())+1);
                        }else if(data=="login"){
                            alert(login_alert);return;
                        }else if(data=="vote_five"){
                            alert("Maximum 5 votes for same person per 24hours.");return;
                        }else if(data=="vote_one"){
                            alert("Maximum 1 vote for same content per 24hours.");return;
                        }else if(data=="vote_plus_cancel"){
                            total.text(Number(total.text())-1);    
                            num_votes.text(Number(num_votes.text())-1);
                        }else if(data=="ip_ban"){
                            alert("Banned for multiple trolling accounts.");return;
                        }else{/*alert(data);return;*/}
                    });    
                }
            }else{alert(login_alert);return;}
    	}else{}
    });
    //for rereply
    $('a.reply').live('click',function(e){//rereply insert box open 1
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},500);
            if(user_id!=""){
                var send_comment_box=$(this).closest('.box_comment').find('.send_comment');
                var akp_posts_rebtn=send_comment_box.children('.akp_posts_rebtn');//send btn
                var akp_posts_reedit=send_comment_box.children('.akp_posts_reedit');//edit btn
                akp_posts_rebtn.attr('data-value',$(this).attr('data-value'));
                akp_posts_rebtn.show();//show send btn
                akp_posts_reedit.hide();//hide edit btn
            
                var send_comment_box=$(this).closest('.box_comment').find('.send_comment');
                if(send_comment_box.find('.akp_posts_rereply').val()==''){
                    if(send_comment_box.attr('data-flag')=="false"){
                        send_comment_box.slideDown('fast');
                        send_comment_box.attr('data-flag',"true");
                    }else{
                        send_comment_box.slideUp('fast');
                        send_comment_box.attr('data-flag',"false");
                    }
                }else{
                    send_comment_box.find('.akp_posts_rereply').val('');
                }
            }else{alert(login_alert);return;}
        }
    });
    $('a.reply_re').live('click',function(e){//rereply insert box open 1
    
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},500);
        
            if(user_id!=""){
                var to='@'+$(this).attr('data-to')+' ';
                var send_comment_box=$(this).closest('.box_comment').find('.send_comment');
                var akp_posts_rebtn=send_comment_box.children('.akp_posts_rebtn');//send btn
                var akp_posts_reedit=send_comment_box.children('.akp_posts_reedit');//edit btn
                akp_posts_rebtn.attr('data-value',$(this).attr('data-value'));
                akp_posts_rebtn.show();//show send btn
                akp_posts_reedit.hide();//hide edit btn
                
                if(send_comment_box.find('.akp_posts_rereply').val()==to){
                    if(send_comment_box.attr('data-flag')=="false"){
                        send_comment_box.slideDown('fast');
                        send_comment_box.find('.akp_posts_rereply').val(to);
                        send_comment_box.attr('data-flag',"true");
                    }else{
                        send_comment_box.slideUp('fast');
                        send_comment_box.find('.akp_posts_rereply').val('');
                        send_comment_box.attr('data-flag',"false");
                    }    
                }else{
                    if(send_comment_box.attr('data-flag')=="false"){
                        send_comment_box.slideDown('fast');
                        send_comment_box.find('.akp_posts_rereply').val(to);
                        send_comment_box.attr('data-flag',"true");
                    }else{
                        send_comment_box.find('.akp_posts_rereply').val(to);
                    }
                }
            }else{alert(login_alert);return;}
        }
    });
    
    $(".akp_posts_rereply").live('click',function(e){//rereply textarea box wider 2
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},500);
            if($(this).height()==20){
                $(this).animate({ height: 100}, 'fast' );
            }
        }
    });
    $('.akp_posts_rebtn').live('click',function(e){//rereply insert send 3
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);

	var akp_posts_rebtn=$(this);
        if(user_id!=""){
            var co = $.cookie("write_");
            if(co==null){
                var akp_posts_rereply=$(this).parent().children('.akp_posts_rereply');
                var send_comment_div=$(this).parent();
                if(akp_posts_rereply.attr('value')!=""){
                    $.post("/article_reply.php",{reply_data:$(this).attr('data-value'),mode:'rereply_write',akp_posts_rereply:akp_posts_rereply.attr('value')},function(data){
                        if(data[0]==1){
                            $('.comment_count').text(Number($('.comment_count').text())+1);
                            $('.akp_posts_rereply').val("");
                            //alert("Comment Inserted.");
                            $(data[1]).insertBefore(send_comment_div);
                            
                            $('.send_comment').slideUp('fast');
                            $('.send_comment').attr('data-flag',"false");
                            
                        }else if(data[0]=="200"){
                            $('.akp_posts_rereply').val("");
                            alert("You've reached the maximum of 200 comments and posts.");return;
                        }else if(data[0]=="ban"){
                            alert("You can't post another comment until "+data[1]+" EST.");
                            return;
                        }else if(data[0]=="ip_ban"){
                            alert(data[1]);
                            return;
                        }else if(data[0]=="login"){
                            alert(login_alert);return;
                        }else{/*alert(data);return;*/}
                    },"json"); 
                }    
            }else{alert(wait_alert);return;}
        }else{alert(login_alert);return;}
	}else{}
    });
    
    $('a.reedit_re').live('click',function(e){//rereplay edit
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
            reedit_re=$(this).closest('.box_friend_comment').find('.rereply_content');
            var send_comment_box=$(this).closest('.box_comment').find('.send_comment');
            var akp_posts_rebtn=send_comment_box.children('.akp_posts_rebtn');//send btn
            var akp_posts_reedit=send_comment_box.children('.akp_posts_reedit');//edit btn
            akp_posts_rebtn.hide();//hide send btn
            akp_posts_reedit.attr('data-value',$(this).attr('data-value')).show();//show edit btn
            
            if(send_comment_box.attr('data-flag')=="false"){
                send_comment_box.slideDown('fast');
                send_comment_box.find('.akp_posts_rereply').val(reedit_re.text());
                send_comment_box.attr('data-flag',"true");
            }else{
                send_comment_box.slideUp('fast');
                send_comment_box.find('.akp_posts_rereply').val('');
                send_comment_box.attr('data-flag',"false");
            }
        }
    });
    $('.akp_posts_reedit').live('click',function(e){//rereply edit send
        e.preventDefault();
	    if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
            var akp_posts_rereply=$(this).parent().children('.akp_posts_rereply');
            var send_comment_box=$(this).closest('.box_comment').find('.send_comment');
            var akp_posts_rebtn=send_comment_box.children('.akp_posts_rebtn');//send btn
            var akp_posts_reedit=$(this);//edit btn
            
            if(akp_posts_rereply.attr('value')!=""){
                $.post("/article_reply.php",{rereply_data:$(this).attr('data-value'),mode:'rereply_edit',akp_posts_rereply:akp_posts_rereply.attr('value')},function(data){
                    if(data[0]==1){
                        akp_posts_reedit.hide();//hide edit btn
                        akp_posts_rebtn.show();//show send btn
                        reedit_re.html(data[1]);
                        send_comment_box.slideUp('fast');
                        send_comment_box.attr('data-flag',"false");
                    }else if(data[0]=="login"){
                        alert(login_alert);return;
                    }else{/*alert(data);return;*/}
                },"json"); 
            }    
	    }else{}
    });
    $('a.del_status_re').live('click',function(e){//rereplay delete
        e.preventDefault();
        var del_what=$(this).closest('.box_comment');
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
            if(user_id!=""){
                var choose = confirm("Do you want to delete it?");
                if(choose){
                    $.post("/article_reply.php",{rereply_data:$(this).attr('data-value'),mode:'rereply_delete'},function(data){
                        if(data==1){
                            $('.comment_count').text(Number($('.comment_count').text())-1);
                            del_what.remove();
                            comment_loadData('',orderby,'n');
                            //alert("Comment Deleted.");return;
                        }else if(data=="login"){
                            alert(login_alert);return;
                        }else{/*alert(data);return;*/}
                    });
                }else{return;}
            }else{alert(login_alert);return;}
        } 
    });
    $('a.report_cancel_re').live('click',function(e){
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
            var report_content_re=$(this).closest('.report_screen_re').children('.modal-body').find('input[name="report_value_re"]:checked');
            var report_option_re=$(this).closest('.report_screen').children('.modal-body').find('.report_option_re');
            report_content_re.prop('checked',false);
            report_option_re.val('');
            $('#report_screen_re').modal('hide');
        }
    });
    $('a.rereport_re').live('click',function(e){//rereplay report
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
            rereport_re=$(this);
            if(user_id!=""){
                if(user_id==$(this).attr("data-rto")){
                    alert(own_report);return;
                }else{
                    $('#report_screen_re').modal('show');
                }
            }else{alert(login_alert);return;}   
        }
    });
    $('a.report_send_re').live('click',function(e){
         e.preventDefault();
         if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
             var report_content_re=$(this).closest('.report_screen_re').children('.modal-body').find('input[name="report_value_re"]:checked');
             var report_option_re=$(this).closest('.report_screen_re').children('.modal-body').find('.report_option_re');
             if(report_content_re.val()==undefined){
                alert("choose one of your reason.");return;
             }else if(report_option_re.val()==""){
                alert(cmt_report);return;
             }else{
                 $.post("/article_reply.php",{rereply_data:rereport_re.attr('data-value'),report_content_re:report_content_re.val(),report_option_re:report_option_re.val(),mode:'rerepply_report'},function(data){
                    if(data==1){
                        if(Number(rereport_re.children('.rereport_screen_re').text())<4){
                            rereport_re.children('.rereport_screen_re').html(Number(rereport_re.children('.rereport_screen_re').text())+1).show();    
                        }else{
                            rereport_re.children('.rereport_screen_re').html(Number(rereport_re.children('.rereport_screen_re').text())+1).show();
                        }
                        report_content_re.prop('checked',false);
                        $('#report_screen_re').modal('hide');
                        //alert("Comment is reported.");return;   
                    }else if(data=="onetime"){
                        alert(one_report);return;
                    }else if(data=="login"){
                        alert(login_alert);return;
                    }else{/*alert(data);return;*/}
                });
            }
	    }else{}
    });
    $('a.community').live('click',function(e){//rereplay like.
    e.preventDefault();
    if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
        var community=$(this);
        if(user_id!=""){
            if(user_id==community.attr("data-usr")){
                alert("You cannot vote on your own comment or post.");return;
            }else{
                var like=community.parent().children('.like');
                var dislike=community.parent().children('.dislike');
                $.post("/article_reply.php",{rereply_data:community.attr('data-value'),mode:'rereply_like'},function(data){
                    if(data==1){
                        like.text(Number(like.text())+1);
                    }else if(data=="login"){
                        alert(login_alert);return;
                    }else if(data=="vote_five"){
                        alert("Maximum 5 votes for same person per 24hours.");return;
                    }else if(data=="vote_one"){
                        alert("Maximum 1 vote for same content per 24hours.");return;
                    }else if(data=="vote_minus_cancel"){
                        dislike.text(Number(dislike.text())-1);
                    }else if(data=="ip_ban"){
                        alert("Banned for multiple trolling accounts.");return;
                    }else{/*alert(data);return;*/}
                });    
            }
        }else{alert(login_alert);return;}    
        }
    });
    $('a.except').live('click',function(e){//rereplay dislike
    e.preventDefault();
    if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
        var except=$(this);
        if(user_id!=""){
            if(user_id==except.attr("data-usr")){
                alert("You cannot vote on your own comment or post.");return;
            }else{
                var like=except.parent().children('.like');
                var dislike=except.parent().children('.dislike');
                $.post("/article_reply.php",{rereply_data:except.attr('data-value'),mode:'rereply_dislike'},function(data){
                    if(data==1){
                        dislike.text(Number(dislike.text())+1);
                    }else if(data=="login"){
                        alert(login_alert);return;
                    }else if(data=="vote_five"){
                        alert("Maximum 5 votes for same person per 24hours.");return;
                    }else if(data=="vote_one"){
                        alert("Maximum 1 vote for same content per 24hours.");return;
                    }else if(data=="vote_plus_cancel"){
                        like.text(Number(like.text())-1);
                    }else if(data=="ip_ban"){
                        alert("Banned for multiple trolling accounts.");return;
                    }else{/*alert(data);return;*/}
                });
            }
        }else{alert(login_alert);return;}
        }
    });
    if (typeof comment_loadData=='function'){}else{
        function comment_loadData(lastmsg,orderby,anchor){
            $.post("/common/comment.php",{lastmsg:lastmsg,article_idx:'68761',orderby:orderby},function(data){
                $("#comment_container").html(data);
                if(anchor=="y"){
                    scroll_to_target_id('goto_box_comment');    
                }
            });
        }
    }
    $('.morebox_btn').live("click",function(){
        $('.more').click();
    });
    $('.more').on("click",function(){
        var ID = $(this).attr("id");
        var num=$(this).attr("data-value");
        if(ID){
            $("#more"+ID).html('<img src="/img/loading.gif" />');
            $.ajax({
                type: "POST",
                url: "/common/comment.php",
                data: "lastmsg="+ ID + "&article_idx=68761&num="+num+"&orderby="+orderby, 
                cache: false,
                success: function(html){
                    $("#comment_container").append(html);
                    $("#more"+ID).remove(); // removing old more button
                    /*if(user_id=="392712"){
                        //$('.akp_ros_300x250_d').append("<!-- akp_ros_300x250_c --><div id='div-gpt-ad-1368577201454-18' style='width:300px;'><scr"+"ipt type='text/javas"+"cript'>googletag.cmd.push(function() { googletag.display('div-gpt-ad-1368577201454-18'); });</scr"+"ipt></div>");
                    }*/
                }
            });
        }else{
            $(".morebox.end").html('The End');// no results
        }
        return false;
    });
    $('.content-show').hide();
    $('.show-icon').on('click',function (e) {
        e.preventDefault();
        if (status == 0) {
            $('.content-show').slideUp('fast');
			$('.show-icon img').attr("src","http://static1.allkpop.com/img/btn22.png");
            status = 1;
        }else{
            $('.content-show').slideDown('fast');
            $('.show-icon img').attr("src","http://static1.allkpop.com/img/btn22_active.png");
            status = 0;
        }
    });
    function get_social_count(){
        //if($(this).attr('data-chk')==0){
            var fb_share=$('.customFShare');
            var tw_share=$('.customTShare');
            var g_share=$('.customGShare');
            var url=$('.fb_article').attr('data-social');
            console.log(url);
            if(fb_share.text()=="share"){
                $.getJSON("http://graph.facebook.com/?id="+url, function(data){;
                    if(data.shares>=0){fb_share.text(data.shares);}else{fb_share.text('0');return;}
                    data='';
                 });
            }
            if(tw_share.text()=="share"){
                $.getJSON("http://urls.api.twitter.com/1/urls/count.json?url="+url+"&callback=?",function(data){
                    if(data.count>=0){tw_share.text(data.count);}else{tw_share.text('0');return;}
                    data='';
                });
            }
            //$(this).attr('data-chk','1');
        //}
    }
    get_social_count();
});
function edit_article(data){window.open("../../..//juniornl/articles/articles_newarticles.php?data="+data,'_blank');}
