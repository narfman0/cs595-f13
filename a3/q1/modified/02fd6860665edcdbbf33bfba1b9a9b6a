    $('.akp_posts_btn').on('click',function(e){//reply insert send 2
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
    $('.alert').hide();
	if(user_id!=""){
            var co = $.cookie("write_");
            if(co==null){
                var akp_posts_reply=$('.redactor_akp_posts_reply').html();
                if(akp_posts_reply!=""&&akp_posts_reply!="<p><br></p>"){
                    $.post("/article_reply.php",{parent_idx:'68761',mode:'write',akp_posts_reply:akp_posts_reply},function(data){
                        if(data[0]==1){
                            if (status==0){
                                $('.content-show').slideUp('fast');
                    			$('.show-icon img').attr("src","http://static1.allkpop.com/img/btn22.png");
                                icon_status = 1;
                            }
                            $('.comment_count').text(Number($('.comment_count').text())+1);
                            $('.akp_posts_reply').val("");
                            $('.redactor_akp_posts_reply').html("<p><br></p>");
                            $('.list-comments').prepend(data[1]);
                            //comment_loadData('',orderby,'y');
                            setEndOfContenteditable($('.redactor_akp_posts_reply').get(0));
                        }else if(data[0]=="200"){
                            if (status==0){
                                $('.content-show').slideUp('fast');
                    			$('.show-icon img').attr("src","http://static1.allkpop.com/img/btn22.png");
                                icon_status = 1;
                            }
                            $('.comment_count').text(Number($('.comment_count').text())+1);
                            $('.akp_posts_reply').val("");
                            $('.redactor_akp_posts_reply').html("<p><br></p>");
                            setEndOfContenteditable($('.redactor_akp_posts_reply').get(0));
                            alert("You've reached the maximum of 200 comments and posts.");return;
                        }else if(data[0]=="login"){
                            alert(login_alert);return;
                        }else if(data[0]=="ban"){
                            alert("You can't post another comment until "+data[1]+" EST.");
                            return;
                        }else if(data[0]=="ip_ban"){
                            alert(data[1]);
                            return;
                        }else if(data[0]=="warning"){
                            
                        }else{/*alert(data);return;*/}
                    },"json"); 
                }else{
                    $('.alert').html('<a class="close">x</a><strong>Error!</strong> '+empty_alert).show();
                    $('.close').click(function(){$('.alert').hide();});
                    return;
                }
            }else{
                $('.alert').html('<a class="close">x</a><strong>Error!</strong> '+wait_alert).show();
                $('.close').click(function(){$('.alert').hide();});
                return;
            }
        }else{
            $('.alert').html('<a class="close">x</a><strong>Error!</strong> '+login_alert).show();
            $('.close').click(function(){$('.alert').hide();});
            return;
        }
	}else{}
    });
    $('.akp_posts_cancel').on('click',function(e){
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
            if(user_id!=""){
                var choose=confirm("Do you want to cancel it?");
                if(choose){
                    if (status==0){
                        $('.content-show').slideUp('fast');
                		$('.show-icon img').attr("src","http://static1.allkpop.com/img/btn22.png");
                        status = 1;
                    }
                    $('.redactor_akp_posts_reply').html("<p><br></p>");
                    $('.akp_posts_reply').val('');
                    setEndOfContenteditable($('.redactor_akp_posts_reply').get(0));
                    if(ref!=""){
                        scroll_to_target_id('cmt_id_'+ref);
                        ref="";    
                    }
                    $('.akp_posts_edit').hide();
                    $('.akp_posts_cancel').hide();
                    $('.akp_posts_btn').show();
                }
            }
        }
    });
