    $(".update_screen").on('click',function(e){
        e.preventDefault();
        var nr=$('.update_result').attr('data-nr');
        var nrr=$('.update_result').attr('data-nrr');
        var tc=$('.update_result').attr('data-tc');
        
        $(".update_screen").fadeOut('fast',function(){
            $(".update_screen").attr("data-status","0");
            if(nr!=null && Number($('.comment_update').attr('data-click-num1'))<Number(nr)){
                $('.comment_update').attr('data-click-num1',nr);//change to last reply_idx    
            }
            if(nrr!=null && Number($('.comment_update').attr('data-click-num2'))<Number(nrr)){
                $('.comment_update').attr('data-click-num2',nrr);//change to last rereply_idx    
            }
            if(tc!=null){
                $('.comment_count').text((Number($('.comment_count').text().replace(",",""))+Number(tc)).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ","));//update final total comment number    
            }
            $('.new_screen').prepend($('.update_result').html());
            $('.new_screen_box').show();
        });
    });
