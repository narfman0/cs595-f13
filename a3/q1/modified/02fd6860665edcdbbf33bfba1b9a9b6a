    $('a.report_send').live('click',function(e){
         e.preventDefault();
    	 if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
             var report_content=$(this).closest('.report_screen').children('.modal-body').find('input[name="report_value"]:checked');
             var report_option=$(this).closest('.report_screen').children('.modal-body').find('.report_option');
             if(report_content.val()==undefined){
                alert("choose one of your reason.");return;
             }else if(report_option.val()==""){
                alert(cmt_report);return;
             }else{
                $.post("/article_reply.php",{reply_data:rereport.attr('data-value'),report_content:report_content.val(),report_option:report_option.val(),mode:'report'},function(data){
                    if(data==1){
                        if(Number(rereport.children('.rereport_screen').text())<4){
                            rereport.children('.rereport_screen').html(Number(rereport.children('.rereport_screen').text())+1).show();
                        }else{
                            rereport.children('.rereport_screen').html(Number(rereport.children('.rereport_screen').text())+1).show();
                        }
                        report_content.prop('checked',false);
                        $('#report_screen').modal('hide');
                        //alert("Comment is reported.");return;
                    }else if(data=="onetime"){
                        alert(one_report);return;
                    }else if(data=="login"){
                        alert(login_alert);return;
                    }else{/*alert(data);return;*/}
                });
             }
        }else{}
    });
