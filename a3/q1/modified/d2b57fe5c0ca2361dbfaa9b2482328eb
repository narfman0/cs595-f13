<!-- Twitter -->
<script src="http://static.tumblr.com/6nxvfxq/xQxmjucwc/twitter_helpers.js"></script>
<script>
	var twitter_feed       = $('.twitter-feed'),
		tweet_list         = twitter_feed.find('.tweet-list'),
		twitter_count      = twitter_feed.data('count') || 5;

	var parse_date = function( str ) {
		var v = str.split(' ');
		return new Date(Date.parse(v[1]+" "+v[2]+", "+v[5]+" "+v[3]+" UTC"));
	};

	var recent_tweets = function( data ) {
		var count = (twitter_count) ? Math.min(parseInt(twitter_count, 10), data.length) : data.length,
			tweets_markup = '';

		for (var i = 0; i < count; i++) {
			tweets_markup += '<li class="' + ((0 === i) ? 'tweet tweet_first' : 'tweet') + '">';
			tweets_markup += '<span class="tweet-time"><a href="http://twitter.com/ogjimrock/status/' + (data[i].id_str ? data[i].id_str : data[i].id) + '" title="view tweet on Twitter">about ' + $.timeago(parse_date(data[i].created_at)) + '</a></span> ';
			tweets_markup += '<span class="tweet-text">' + twttr.txt.autoLink( data[i].text ) + '</span>';
			tweets_markup += '</li>';
		}

		tweets_markup = String(tweets_markup).replace('about about', 'about');

		// Insert tweets markup
		tweet_list.html( tweets_markup );
	};

	var ticker = $('.tweet-list');
	ticker.children('li:first').show().siblings().hide();
	
	setInterval(function() {
		ticker.find('li:visible').fadeOut(500,function() {
			$(this).appendTo(ticker);
			ticker.children('li:first').fadeIn(500);
		});
	},3500);

</script>
<script type="text/javascript" src="/tweets.js"></script>

<!-- bxSlider JS -->

<script>
	$(document).ready(function() {
		
		// photoset grid
		$('.photoset-grid').photosetGrid({
			highresLinks: true,
			rel: $('.photoset-grid').attr('data-id'),
			gutter: '10px'
		});
		
		// FitVids
		$('.video-player').fitVids({ customSelector: "iframe[src^='//www.tumblr.com']"});
		
		// removing modules
		function removeModules() {
			$('.typo-slider,.icon-boxes-bg,.text-button-bg,.product-box-bg,.projects-bg,.news-bg,.team-bg,.testimonials-bg,.clients-bg').remove();
		}		
		if(location.pathname != '/') {
			removeModules();
		}
		
		
		
		
		// vimeo color
		if($('iframe[src*="player.vimeo.com"]')) {					 
			var vimeocolor = '#e6736f',
			newvimeocolor = vimeocolor.replace("#","");
			$('iframe[src*="player.vimeo.com"]').attr('src', function() {
			  return this.src + '?title=1&byline=0&portrait=0&color=' + newvimeocolor;
			});
		} 
		// spotify fix
		$('iframe[src*="embed.spotify.com"]').each( function() {
			$(this).css('width',$(this).parent(1).css('width'));
			$(this).attr('src',$(this).attr('src'));
		});
	});
	$(window).load(function() {	
		// creating spans
		$(".typo-slider .slide img").each(function() {
			var slideImgTitle = $(this).attr('title');
			var pipe_pos = slideImgTitle.indexOf('|');
			var end_pos = slideImgTitle.length;
			var text_big = slideImgTitle.substring(0,pipe_pos);
			var text_small = slideImgTitle.substring(pipe_pos+1,end_pos);
			$(this).next().find("span").remove();
			$(this).next().append('<span class="big-text">' + text_big + '</span><span class="small-text">' + text_small + '</span>').hide().fadeIn(1000);
		});
		
		// dynamic top margin
		function captionTop() {
			var slideImgHeight = $(".typo-slider .slide > img").height();
			var changedTop = (slideImgHeight/2)-40;
			$(".typo-slider .slide .bx-caption").css('top', changedTop + 'px');
		}		
		$(window).resize(function() {
			captionTop();
			// spotify fix
			$('iframe[src*="embed.spotify.com"]').each( function() {
				$(this).css('width',$(this).parent(1).css('width'));
				$(this).attr('src',$(this).attr('src'));
			});
		});
		captionTop();
	
		// search
		$("input#search").focus(function(){
			$(this).addClass("focus");
			$("label.icon-search").addClass("focus");
		});
		$("input#search").blur(function(){
			$(this).removeClass("focus");
			$("label.icon-search").removeClass("focus");
		});
		
		//  mobile menu display
		$(".menu-button").click(function(){
			$("ul.menu-links").toggleClass("menu-show");
		})
	})
</script>

