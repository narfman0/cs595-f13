        <script>
        $(function() {
        
	//create colorbox link. enable colorbox for featured upon onclick
	var counter = 0;
         $(".gallery_colorbox .view-content .views-row .views-field a").each(function(i){
		
		var id = "views-row-"+(i+1);
                var caption = $(".gallery_colorbox .view-content ."+id+" .views-field-field-story-caption-value .field-content").html();
                var caption_plain = $(".gallery_colorbox .view-content ."+id+" .views-field-field-story-caption-value .field-content").text();
                
		$(".colorbox-"+(i+1)+"").attr("title",caption);
		
		
		counter++;
        });	
	
	if(counter > 1)
	{
		//show left and right button when mouseover
		$('#slideshow, .imageOverLeft, .imageOverRight').css('cursor', 'pointer').mouseover(function() {
		$(".custom-story-gallery-container .imageOverLeft").show();
		$(".custom-story-gallery-container .imageOverRight").show();
		}).mouseout(function() {
		$(".custom-story-gallery-container .imageOverLeft").hide();
		$(".custom-story-gallery-container .imageOverRight").hide();
		});
		
		$(".imageOverRight").click(function() {
		$(".nav-controls .next").trigger("click");
		});
		
		//featured image mouseover arrow - left
		$(".imageOverLeft").click(function() {
		$(".nav-controls .prev").trigger("click");
		});    
		
		$("#thumbs, #galleriffic").show();                
	}
	else
	{
		$(".imageOverLeft, .imageOverRight").hide();
		$("#galleriffic").show();
	}
	
        $(".custom_colorbox").click(function() {
                $('a.custom_colorbox').colorbox({initialWidth:"100%", initialHeight:"100%",maxWidth:"100%", maxHeight:"100%"});
        });
	
	if(counter < 13)
	{
		//document.styleSheets[0].insertRule(".custom-story-gallery-container .pagination{ display:none; }", 0); //will make error in firefox
		//$('.custom-story-gallery-container .pagination').attr('style','display:none');s
		//$(".custom-story-gallery-container .pagination").css({"display":"none"});
		if ($.browser.msie) 
		document.styleSheets[0].addRule('.custom-story-gallery-container .pagination', 'display:none', 0);
		else
		document.styleSheets[0].insertRule(".custom-story-gallery-container .pagination{ display:none; }", 0); //will make error in firefox
		
		 
	}
	
	/* //not in use
	jQuery(".pagination").live("hide");
	if($(".custom-story-gallery-container .pagination").html() == "")
	{
		$(".custom-story-gallery-container .pagination").hide();
	}
	*/
        
	});
        </script>
