<script type='text/javascript'>
$(function() { 
	$('#st_blocks_story_ajax_sidebar').html('');
	$.ajax({
		type: 'GET',
		url: 'http://www.straitstimes.com/st/ajax_story_sidebar/5?parent=Breaking News&type=sidebar_next&name=Lifestyle&asiareport=false',
		dataType: 'html',
		async: true,
		cache: true,
		beforeSend:function(){},
		success: function(data){
		$('#st_blocks_story_ajax_sidebar').empty();
		$('#st_blocks_story_ajax_sidebar').html(data);
	
		$('#st_blocks_story_ajax_sidebar .border-container').append("");
	
		var current_sidebar_position = data.indexOf("current-1549701");
		
		//_custom - sidebar
		var current = "div-1549701";
				
		if (current_sidebar_position != -1) {
		
			var current_class = $("."+current+"").attr("class");
		 
			var prev_id = $("."+current+"").prevAll().attr("class");
			var next_id = $("."+current+"").nextAll().attr("class");
	
			var first_position = current_class.indexOf("first");
			var last_position = current_class.indexOf("last");

			//prev
			if (first_position == -1) {
				prev_id = prev_id.split(" ");
				var prev_link_a = $("."+prev_id[3]+" a").attr("href");
				if (typeof prev_link_a != "undefined") {
					$('#prevstory').attr('href',prev_link_a);
					}
				}
			else {
				$('#prevstory').hide();
				}	
	
			//next
			if (last_position == -1) {
				next_id = next_id.split(" ");
				var next_link_a = $("."+next_id[3]+" a").attr("href");
				if (typeof next_link_a  != "undefined") {
					$('#nextstory').attr('href',next_link_a);
					}
				}
			else {
				$('#nextstory').hide();
				}		
			
			}
		else {
			$('.panelTopOtherStories').hide();
			}
		//end sidebar
		
		},
		error: function() {}
	});
	
	//_custom - footer
	$('#story_footer_next').html('');
	$.ajax({
		type: 'GET',
		url: 'http://www.straitstimes.com/st/ajax_story_footer/5?parent=Breaking News&&name=Lifestyle&current_nid=1549701&current_tid=5&firstKey=5',
		dataType: 'html',
		async: true,
		cache: true,
		beforeSend:function(){},
		success: function(data){
		$('#story_footer_next').empty();
		$('#story_footer_next').html(data);
		},
		error: function() {}
	});
	//end footer
	
});
</script>

