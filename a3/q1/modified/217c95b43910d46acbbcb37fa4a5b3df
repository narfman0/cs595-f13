	decorator.contentTitle = Power Rankings
	decorator.contentLink = 
	decorator.sponsorTitle = 
	decorator.sponsorLink = 
	decorator.trackingPixel = 
	decorator.imageUrl = /article-images/power-rankings/power_ranking.png
	decorator.adServed = false
	THEMETYPE sponsored-article<br />
