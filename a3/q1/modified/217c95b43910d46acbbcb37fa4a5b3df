<script type="text/javascript">
	//create new slideshow nonesense
	function removeVideosOnZero(){
		if(!document.loaded){
			document.observe('dom:loaded',removeVideosOnZero);
			return
		}
		if($('rr-videos')){
			$('rr-videos').remove();
			console.log('removeVideosOnZero: removed videos node');
		}else{
			try{setTimout(removeVideosOnZero,1000)}catch(e){}
		}
	}
	if(4 > 0){
		document.observe('videos:carousel:start',function(event){
			var elapse	= 10;
			var indicatorWidth	= ($('rr-videos-indicator').getWidth());
			try{rrVideosCarousel.timer.stop();}catch(e){};
			try{rrVideosCarousel.indicatorMorph.cancel();}catch(e){};
			
			rrVideosCarousel.indicatorMorph	= new Effect.Morph('rr-videos-indicator', {
				style: 'left: 0px;',
				duration: (elapse - 1),
				beforeStart: function(){$('rr-videos-indicator').setStyle({'left':'-'+(indicatorWidth)+'px'});}
			});
			rrVideosCarousel.timer	= new PeriodicalExecuter(function(){
				document.fire('videos:carousel:scroll',{direction:'right',id:'rr-videos-carousel'});
				try{ rrVideosCarousel.indicatorMorph.cancel(); }catch(e){};
				$('rr-videos-indicator').setStyle({'left':'-'+(indicatorWidth)+'px'});
				rrVideosCarousel.indicatorMorph	= new Effect.Morph('rr-videos-indicator', {
					style: 'left: 0px;',
					duration: (elapse - 1),
					beforeStart: function(){$('rr-videos-indicator').setStyle({'left':'-'+(indicatorWidth)+'px'});}
				});
			},elapse);
			if($('rr-videos-controls')){
				$('rr-videos-controls').addClassName('playing');
			}
		});
		document.observe('videos:carousel:stop',function(event){
			try{rrVideosCarousel.timer.stop();}catch(e){}
			rrVideosCarousel.timer	= null;
			try{ rrVideosCarousel.indicatorMorph.cancel(); }catch(e){};
			var indicatorWidth	= ($('rr-videos-indicator').getWidth());
			
			if($('rr-videos-controls')){
				$('rr-videos-controls').removeClassName('playing');
			}
			$('rr-videos-indicator').setStyle({'left':'-'+(indicatorWidth)+'px'});
		});
		var videosWidgetDestroy	= function(){
			try{ rrVideosCarousel.timer.stop();}catch(e){};
			try{ rrVideosCarousel.timer	= null;}catch(e){};
			try{ rrVideosCarousel.indicatorMorph.cancel(); }catch(e){};
			try{ rrVideosCarousel.indicatorMorph = null;}catch(e){};
			try{ rrVideosCarousel.destroy();}catch(e){};
			console.info('destroyed videos widget');
			document.stopObserving('tab:content:destroy',videosWidgetDestroy);
		}
		document.observe('tab:content:destroy',videosWidgetDestroy);
		
		var opts	= {'rows':1,'debug':true,'evtprefix':'videos'};
		var rrVideosCarousel	= new nfl.widgets.Carousel('rr-videos',opts);
		rrVideosCarousel.timer			= null;
		rrVideosCarousel.indicatorMorph	= null;
		
		
	}else{
		/* hide videos widget all together */
		removeVideosOnZero();
	}
</script>
  				
  			
