<script type="text/javascript">
(function(){

    var i, len, url, match,
        allScripts = YUI.presentation.lazyScriptList,
        comboRoot = 'http://l.yimg.com/zz/combo?',
        isCombo =  allScripts[0].indexOf(comboRoot) === 0,
        yuiMods = [],
        lazyScripts = [];

    if (isCombo) {
        for (i = 0, len = allScripts.length; i < len; ++i) {
            url = allScripts[i].replace(comboRoot, '');
            allScripts = allScripts.concat(url.split('&'));
        }
        allScripts = allScripts.slice(len);
    }

    for (i = 0, len = allScripts.length; i < len; ++i) {
        url = allScripts[i];
        match = url.match(/\/yui\/([^._]*)/);
        if (match && match.length > 1) {
            yuiMods.push(match[1]);
        } else {
            lazyScripts.push(url);
        }
    }

    if (isCombo) {
        lazyScripts = [comboRoot + lazyScripts.join('&')];
    }

    if (YMedia.ModulePlatform) {
        YMedia.ModulePlatform.reset();
    }

    YMedia.use(yuiMods, function (Y) {
        Y.dali.config.debug = false;
        Y.Get.script(lazyScripts, { onSuccess: function(){
            Y.use("*", function (Y) {
                Y.mix(Y.dali.config.props, {
                    libRoot: "",
                    user : "",
                    ultSpaceId : "2023538075",
                    ultBeaconHost : "\/p.gif",
                    modChromeHtml : "<div id=\"error-{type_name}\" class=\"mod view_error\">  <div id=\"error-{type_name}-bd\" class=\"bd  _error\">{html}    <\/div>\n<\/div>",
                    modErrorHtml : "<div class=\"oops-msg\" role=\"alertdialog\">\n                <span class='icon' style='background-image:url({img});_filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled=true, sizingMethod=crop src=\"{img}\"); _background-image:none;'><\/span><span class='shadow'><\/span>\n                <h3 class=\"oops\">\"{name}\" encountered a problem while trying to load<\/h3>\n                <p class=\"oops\">{message}<\/p>\n                                <p class=\"oops oops-link\">Or visit <a href=\"{url}\">{url}<\/a><\/p>\n            <\/div>",
                    crumbErrorHtml : "<div class=\"oops-msg\" role=\"alertdialog\">\n                <span class=\"icon icon-error y-glbl-universal\"><\/span>\n                <h3 class=\"oops\">The module encountered a problem while trying to load<\/h3>\n                <p class=\"oops\">2048<\/p>\n                \n            <\/div>",
                    cookieErrorHtml : "<div class=\"oops-msg\" role=\"alertdialog\">\n                <span class=\"icon icon-error y-glbl-universal\"><\/span>\n                <h3 class=\"oops\">The module encountered a problem while trying to load<\/h3>\n                <p class=\"oops\">16384<\/p>\n                \n            <\/div>",
                    passthru : "m_mode=fragment&site=news&region=US&lang=en-US&pagetype=contentb",
                    proxyTimeout : 15000,
                    requestUrlLite: '/hjsal',
                    arcadeLiteEnabled : "1",
                    intl : "us",
                    "ywa.rapid.enabled": 1,
                    "ywa.rapid.richview.enabled": 1,
                    requestId: 'xsZEUg_NiWJnLAEA6rP9JQ..',
