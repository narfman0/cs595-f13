                        <script>
                        (function(r) {
                            if(! r) { return; }
                            
                            if(typeof YMEDIA_REQ_ATTR === 'object' && typeof YMEDIA_REQ_ATTR.instr === 'object') {
                                r.rapidConfig.keys        = r.rapidConfig.keys || {};
                                r.rapidConfig.keys.authfb = YMEDIA_REQ_ATTR.instr.authfb;
                                r.rapidConfig.yrid    = YMEDIA_REQ_ATTR.instr.request_id;
                            }
                            
                            if(typeof r.initRapid === 'function') {
                                r.initRapid();
                            } else if(YAHOO && YAHOO.i13n && YAHOO.i13n.Rapid) {
