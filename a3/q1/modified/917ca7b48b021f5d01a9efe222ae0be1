<section id="mediacontentstory" class="yom-mod yom-card-main yom-article" data-uuid="49ad9db3-e93c-35cc-8b99-6403751edbde" data-type="story" data-ylk="t1:a3;t2:ct-embed;itc:0;rspns:nav;">
<div class="book clearfix">
    <header class="header">
            <h1 class="headline">'Get a boat!' Venezuela flights booked full for months</h1>
    </header>

<div class="interest-bar">
    <div class="action-bar">
        <div class="action-bar-wrapper">
