                                        DARLA_CONFIG.onSuccess = function (eventId, positions) {        
                        var pos_list = ['LREC', 'LREC2', 'LREC3'];
                        for(var k in pos_list){ 
                            var pos_id = pos_list[k],
                                adModule = Y.one('.yog-rp #yom-ad-' + pos_id);
                            if(adModule) {
                                adContainer = adModule.one('div');
                                if('LREC' === pos_id){
                                    adModule.addClass('yom-app');
                                } 
                                else if(!adContainer){
                                    adModule.setStyle('display', 'none');
                                    Y.log(pos_id + ' is hidden because it has no container', 'info', 'darla');
                                }
                                else if(adContainer && (adContainer.getComputedStyle('width') === '1px') && (adContainer.getComputedStyle('height') === '1px')){                          
                                    adModule.setStyle('display', 'none');
                                    Y.log(pos_id + ' is hidden because it is 1x1 blank ads', 'info', 'darla');
                                }
                                else if(adContainer && ((adContainer.getComputedStyle('width') === '0px') || (adContainer.getComputedStyle('height') === '0px'))){
                                    adModule.setStyle('display', 'none');
                                    Y.log(pos_id + ' is hidden because it is not rendered', 'info', 'darla');
                                }
                                else{
                                    adModule.addClass('yom-app');
                                }
                            }
                        }
        
                        Y.Global.fire('darla:success', {
                            positions: positions
                        });
                        Y.log('fetch ad success: ' + positions.join(', '), 'info', 'darla');
                    };

                    DARLA_CONFIG.onFailure = function (eventId) {
                        var positions = null;
                        try {
                            positions = DARLA_CONFIG.events[eventId].ps.split(',');
                        } catch (e) {}

                        Y.Global.fire('darla:failure', {
                            positions: positions
                        });
                        Y.log('fetch ad failure' + (positions ? (': ' + positions.join(', ')) : ''), 'warn', 'darla');
                    };        
                    /* preparing the Nodes with iframes. Note this would be hidden iframe and not visible in the DOM */
                    var _prepareNodes = function(){
                        for(var cleanId in nodes){
                            var center = Y.one("#"+cleanId+" > center");
                            if(center != null && center && !Y.one("#" + nodes[cleanId].id)){
                                center.set("innerHTML","");
                                center.insert(nodes[cleanId].node);
                            }
                        }
                    };

                    /* setting up DARLA events */
                    var w = window,
                        D = w.DARLA,
                        C = w.DARLA_CONFIG,
                        DM = w.DOC_DOMAIN_SET || 0;
                    if (D) {
                        if (D && C) {
                            C.dm = DM;
                            /*D.config(C);*/
                        }
                        
                        _prepareNodes();
                        
                        /* setting DARLA configuration */
                        DARLA.config(C);

                        /* rendering prefetched Ad */
                        DARLA.render();  
                    }

                    return {
                        event: function (eventId, spaceId, adsSa) {
                            if (window.DARLA && eventId) {
                                _prepareNodes();
                                var eventConfig = {};
                                if (!isNaN(spaceId)) {
                                    eventConfig.sp = spaceId;
                                }
                                /* Site attributes */
                                adsSa = (typeof adsSa !== "undefined" && adsSa !== null) ? adsSa : "";
                                eventConfig.sa = DARLA_CONFIG.events[eventId].sa_orig.replace ? DARLA_CONFIG.events[eventId].sa_orig.replace("ADSSA", adsSa) : "";
                                DARLA.event(eventId, eventConfig);
                            }
                        }
                    };

                })(); /* End of YUI.AdsDarla */

                    YUI.AdsDarla.mediaadsdarla = { fetch: (Y.bind(YUI.AdsDarla.event, YUI.AdsDarla, 'mediaadsdarla')) };                    YUI.AdsDarla.fetch = YUI.AdsDarla.mediaadsdarla.fetch;   
                    Y.Global.fire('darla:ready'); 
             }); /* End of YMedia */      if (console && console.log) { if (_callOnTimerFn=='_gsadtimer_') {console.log('Ads loaded by _gsadtimer_:'+1999);} else {console.log('Ads loaded by PLT');}}; clearTimeout(_gsPltTimer);
  }; /* End of GrandSlam Onload */
  if(window.addEventListener)window.addEventListener('load',loadAdsAfterOnLoad,0);
  else if(window.attachEvent)window.attachEvent('onload',loadAdsAfterOnLoad);
