            if (event) {
              var targetUrl;
              if (event.target && event.target.nodeName == 'IFRAME') {
                targetUrl = extractParamFromUri(event.target.src, param);
              }
              _gaq.push(['_trackSocial', 'twitter', tag, targetUrl]);
            }
          });
        };

        bind_event('tweet', "tweet", "url");
        bind_event('follow', "follow", "screen_name");
      });
    }
  });
</script>

