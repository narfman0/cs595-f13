      return window.location.href.match(new RegExp("^" + url)); 
    });

    if (matching_urls.length > 0) {
      $("#match-detail .inner-list li a").filter(function(index, link) { 
        return link.href.match(/(estadistiques|estadisticas|stats)$/);
      }).hide();
    }
  });
