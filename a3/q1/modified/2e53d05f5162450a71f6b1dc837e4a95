<script src="http://static.tumblr.com/aichqcs/GLOmslz7m/infinitescroll.js"></script>
<script language="JavaScript">

var sidebarnumber = 1;
var sidebarazar = Math.random();
var rand2 = Math.round( (sidebarnumber-1) * sidebarazar);
 
 
sidebars = new Array;
sidebars[0] = "http://static.tumblr.com/50c066c6dac91c6fa4573597f30a6698/cxqqh0s/n4Jmtlbz1/tumblr_static_20120717_001.jpg";

 
var sidebar = sidebars[rand2];


window.onload = terribledemegusta;
 
function terribledemegusta() {
document.body.insertAdjacentHTML( 'beforeEnd', '<iframe id="my-like-frame" style="display:none;"></iframe>' );
document.addEventListener( 'click', function ( event ) {
    var myLikeLink = event.target;
    if( myLikeLink.className.indexOf( 'boton_megusta' ) > -1 ) {
        var myLikeFrame = document.getElementById( 'my-like-frame' ),
            liked = ( myLikeLink.className == 'boton_megusta_activado'),
            command = liked ? 'unlike' : 'like',
            reblog = myLikeLink.getAttribute( 'data-reblog' ),
            postId = myLikeLink.getAttribute( 'data-id' ),
            oauth = reblog.slice( -8 ),
            likeUrl = 'http://www.tumblr.com/' + command + '/' + oauth + '?id=' + postId;
        myLikeFrame.src = likeUrl;
        liked ? myLikeLink.className = 'boton_megusta'
            : myLikeLink.className = 'boton_megusta_activado';
    };
    if( myLikeLink.className.indexOf( 'entrada_megusta' ) > -1 ) {
        var myLikeFrame = document.getElementById( 'my-like-frame' ),
            liked = ( myLikeLink.className == 'entrada_megusta_activado'),
            command = liked ? 'unlike' : 'like',
            reblog = myLikeLink.getAttribute( 'data-reblog' ),
            postId = myLikeLink.getAttribute( 'data-id' ),
            oauth = reblog.slice( -8 ),
            likeUrl = 'http://www.tumblr.com/' + command + '/' + oauth + '?id=' + postId;
        myLikeFrame.src = likeUrl;
        liked ? myLikeLink.className = 'entrada_megusta'
            : myLikeLink.className = 'entrada_megusta_activado';
    };
}, false );
};
 
</script>
 
 
<script>
 
$(document).on("ready",function(){
        $("#boton_plus").on("click",function(){
                        $("#navegacion").css({
                'opacity' : '1',
                'width' : '150px',
                'height' : 'auto',
                'max-height': '400px',
                'top' : '+=25',
                'visibility' : 'visible',
                });
                        $("#boton_plus").addClass("boton_plus");
                        $("#boton_plus").on("click",function(){
                                $("#navegacion").animate({
                'opacity': '0',
                'width' : '0',
                'height' : '0',
                'top' : '+=-50',
                'visibility' : 'hidden',
                }, {
                'duration': 600,
                });
                                $("#boton_plus").removeClass("boton_plus");
                        });
                });
        });
   
</script>
 
 
 

