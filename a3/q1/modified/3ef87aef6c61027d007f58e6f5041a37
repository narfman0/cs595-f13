 <script>
jQuery(function($) {
    $(document).ready(function() {

        /*$("#datepicker").datepicker({ showOn: 'button', buttonImageOnly: false, buttonImage: 'http://go.aviatur.com/images/flujos/ico-calendar.jpg', numberOfMonths:2, minDate: 0,
         onSelect: function(dateText, inst) {
         var lockDate = new Date($('#datepicker').datepicker('getDate'));
         //lockDate.setDate(lockDate.getDate() + 1);
         $('input#datepicker2').datepicker('option', 'minDate', lockDate);  }  	
         });
         
         $("#datepicker2").datepicker({ showOn: 'button', buttonImageOnly: false, buttonImage: 'http://go.aviatur.com/images/flujos/ico-calendar.jpg', numberOfMonths:2 });*/

        $("input#datepicker").datepicker({numberOfMonths: 2, minDate: 0,
            onSelect: function(dateText, inst) {
                var lockDate = new Date($('#datepicker').datepicker('getDate'));
//lockDate.setDate(lockDate.getDate() + 1);
                $('input#datepicker2').datepicker('option', 'minDate', lockDate);
            }
        });

        $("input#datepicker2").datepicker({numberOfMonths: 2});

    });
});
</script>	
