                    <script type="text/javascript">
                    function delete_comment(Id) {
                        if( ! confirm("このコメントを削除しますか？") ) return;
                        $.getJSON("http://www.recipe-blog.jp/api/comment/delete", {token:"AUTO", id:Id}, function(json){
                            if( json.is_success ) {
                                alert("コメントを削除しました");
                                
                                location.href="http://www.recipe-blog.jp/profile/5863/blog/13670512?t=comment&show_comment="
                                
                            } else {
                                alert( json.error || "更新に失敗しました" );
                            }
                        });
                    }
                    
                    function delete_note(Id) {
                        if( ! confirm("このごちそうさまノートを削除しますか？") ) return;
                        $.getJSON("http://www.recipe-blog.jp/api/note/delete", {token:"AUTO", id:Id}, function(json){
                            if( json.is_success ) {
                                alert("ごちそうさまノートを削除しました");
                                
                                location.href="http://www.recipe-blog.jp/profile/5863/blog/13670512?t=note&show_note="
                                
                            } else {
                                alert( json.error || "更新に失敗しました" );
                            }
                        });
                    }
                    </script>
                    

                    
                    
                    
