				document.missionViewForm.page.value = page;
				document.missionViewForm.submit();
			}
			else return;
		} else{
			document.missionViewForm.page.value = page;
			document.missionViewForm.submit();
		}
	}
	function setView(view) {
		document.missionViewForm.viewCode.value = view;
		document.missionViewForm.page.value = 1;
		document.missionViewForm.submit()
	}
	function setGrid(grid) {
		document.missionViewForm.grid.value = grid;
		document.missionViewForm.submit()
	}
