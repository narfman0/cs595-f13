	var bOdaiModified = false;
	var iEditFlag = {
		bFlag : false,
		sService : ""
	};
	function goPage(page) {
		if(iEditFlag.bFlag == true ){
			alert("編集中の"+iEditFlag.sService+"があるため、ページングはできません。");
			return;
		} else if (bOdaiModified){
