    jQuery('#aboutPage, #profilePage, #actorsTab, #usersTab').mouseenter(function () {
        $(this).find('.arrow img').attr('src', '/static/VpLtQDxB61ElPTflNhDIXFMUjzzqRJlwHiPcbdOxxRH.png');
        $(this).find('.arrow').css('background-color', '#ffbb00');
        $(this).css('background-color', '#f3e9cd');
        $(this).find('.item_menu').css('color', '#000');
        $(this).find('.top_yellow_line').show();
    })

    jQuery('#aboutPage, #profilePage, #actorsTab, #usersTab').mouseleave(function () {
        $(this).find('.arrow img').attr('src', '/static/kmcwkCFlzRcWNsnvkhAHEcDRmZt1B2zspSSh4hul8Ek.png');
        if ($(this).hasClass('active')) {
            $(this).find(' .arrow').css('background-color', '#f3e9cd');
        } else {
           $(this).find('.item_menu').css('color', '#999');
           $(this).css('background-color', '#f1f1f1');
           $(this).find(' .arrow').css('background-color', '#f1f1f1');
        }
         $(this).find('.top_yellow_line').hide();
    })

    var nav = $(".user_menu");
    nav.find("li").each(function() {
        if ($(this).find("ul").length > 0) {
            $(this).mouseenter(function() {
                $(this).find("ul").stop(true, true).show();
             });

            $(this).mouseleave(function() {
                $(this).find("ul").stop(true, true).hide();
            });
        }
    });
