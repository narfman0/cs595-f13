   function addPosts($newItems) {
      $('#viewPosts').append( $newItems ).isotope( 'insert', $newItems );

      if ($newItems.get(0).className == 'bounce_words') {
          jQuery('#bounce').append($newItems.get(0));
      } else {
          if ($newItems.size() == 2 &&  $newItems.get(0).className == 'no_posts' && $newItems.get(2).className == 'bounce_words') {
               jQuery('#bounce').append($newItems.get(0));
               jQuery('#bounce').append($newItems.get(2));
          }
          if ($newItems.get(0).className == 'fan_actor_statistic') {
              jQuery('#fanStatistic').append($newItems.get(0));
          }
      }

      // ensure that images load before adding to masonry layout
       $newItems.imagesLoaded(function() {
         $('#viewPosts').isotope('reLayout');
      })
   }

  function filterPostBy() {

      var filter = jQuery("#selectOrderBy").val();
      var displayActivity = jQuery('#selectDisplayActivity').val();

      var hashTagFilter = jQuery('.hash_tag_is_selected').attr('id');
      var includedTypes = "";
      jQuery(".post_type :input[type=checkbox]:checked").each(function() {
          includedTypes += jQuery(this).attr('name') + ",";
      });
      includedTypes = includedTypes.substring(0, includedTypes.length - 1);
      var actorId;
      var allActivities = false;
      if (true) {
          actorId = '32';
      } else {
         var selectedVal = jQuery('#selectActor').val();
         if (selectedVal == 'allActivities') {
             allActivities = true;
         } else {
            actorId = jQuery('#selectActor').val();
          }
      }

      var pageBeforUpdate =  page;

      jQuery.ajax({
        type:'POST',
        url:"/actor/filterBy",
        data: {displayActivity: displayActivity, allActivities: allActivities,fanId: '' ,filter: filter, actorId: actorId, myActivitiesInProfile: false, page: page, firstTimeActivity : false, showMyFeeds: false,  myBounces: false, includedTypes: includedTypes, filterByType: true},
        success:function(data, textStatus){
             var $newElems = $(data);
             jQuery('#loadingPost').fadeOut();
             if (pageBeforUpdate == 0) {
                isotopeReload();
                jQuery('#viewPosts').html('');
                jQuery('#fanStatistic').fadeOut();
                jQuery('#fanStatistic').html('');
                 jQuery('#fanStatistic').fadeIn();

                if (data != '') {
                     addPosts($newElems);
                } else {
                     allPostLoaded = true;
                }
             } else {

                 if (data =='') {
                    allPostLoaded = true;
                 } else {
                     addPosts($newElems);
                 }
            }

            jQuery('#' + filter + 'Spinner').hide();
            jQuery('#selectActorSpinner').hide();
            jQuery("#orderBySpinner").hide();
            jQuery('#selectActivitySpinner').hide();

            if (allPostLoaded) {
               jQuery('#mostContent').hide();
            } else  {
                if (isVisibleMoreContentByDefault) {
                    jQuery('#moreContent').show();
                }
            }
        },

        error:function(XMLHttpRequest,textStatus,errorThrown){}
      });
      page +=20;
  }

   jQuery('#moreContent').click(function() {
       jQuery('#loadingPost').show() ;
       jQuery('#moreContent').hide();
       filterPostBy();
    })

   jQuery('.post_type input[type=checkbox]').click(function() {
        page = 0;
        allPostLoaded = false;
        jQuery('#selectActorSpinner').show();
        jQuery('#selectActivitySpinner').show();
        filterPostBy();
    });

  filterPostBy();

  function isotopeReload() {
      $('#viewPosts').html('');
      $('#viewPosts').isotope( 'destroy' );

       $('#viewPosts').imagesLoaded( function(){
          $('#viewPosts').isotope({
              itemSelector : '.post'
          });
      });
  }

   jQuery("#selectDisplayActivity").change(function() {
       page = 0;
       allPostLoaded = false;
       jQuery('#selectActivitySpinner').show();
       filterPostBy();
   });

  jQuery("#selectOrderBy").change(function() {
       page = 0;
       allPostLoaded = false;
       jQuery('#orderBySpinner').show();
       filterPostBy();
   });

  if (false) {
     jQuery('#selectActor').change(function () {
         allPostLoaded = false;
         page = 0;
         jQuery('#selectActorSpinner').show();
         checkUserBounced();
         filterPostBy();
      });

     function checkUserBounced() {
          jQuery.ajax({
               type:'POST',
               url:"/actor/ifUserBounced",
               data:{fanId: '', actorId: jQuery('#selectActor').val()},
               success:function(data){
                  if (data.ifUserBounced) {
                      jQuery('#myBounces').show();
                  } else {
                      jQuery('#myBounces').hide();
                  }
               }
          });
     }
  }


    

