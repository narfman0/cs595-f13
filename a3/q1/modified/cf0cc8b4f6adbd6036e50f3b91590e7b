    jQuery('.click_more_less_words').live('click', function() {
        var text = jQuery(this).children('span').text().trim();
        if (text == "more") {
            jQuery(this).children('span').text("less");
            jQuery(this).children('img').attr('src', '/static/kmcwkCFlzRcWNsnvkhAHEcDRmZt1B2zspSSh4hul8Ek.png');
            jQuery(this).siblings(".all_keywords").find("span.tree_dot").hide();
            jQuery(this).siblings(".all_keywords").find("span.message_end").css("display", "inline")
        } else {
           jQuery(this).children('span').text('more');
           jQuery(this).children('img').attr('src','/static/kmcwkCFlzRcWNsnvkhAHEcDRmZt1B2zspSSh4hul8Ek.png');
           jQuery(this).siblings('.all_keywords').find("span.tree_dot").show();
           jQuery(this).siblings(".all_keywords").find("span.message_end").css("display", "none")
        }
    });

