    $(function () {
      //Resolving problem with double click for iOS and links with images and hover state
      var isScrolling = false
      $('a').on('touchstart', function () {
        isScrolling = false;
      })
          .on('touchmove', function (e) {
            isScrolling = true;
          })
          .on('touchend', function (e) {
            if (!isScrolling) {
              window.location = $(this).attr('href');
            }
          });
    });
   jQuery('a.badge_to_click').live('click', function() {
       var fanBadgeId = jQuery(this).attr('id');
        jQuery.ajax({type:'POST',data:'fanBadgeId=' + fanBadgeId, url:'/actor/updateBadgeInfo',success:function(data,textStatus){jQuery('#overlayContainer').html(data);},error:function(XMLHttpRequest,textStatus,errorThrown){}});
    });

    //Prevent doubleclick
    $("a, button, input[type=submit]").click(function(e) {
        if (!$(this).data('isClicked')) {
            var link = $(this);

            // Your code on successful click

            // Set the isClicked value and set a timer to reset in 3s
            link.data('isClicked', true);
            setTimeout(function() {
            link.removeData('isClicked')
            }, 3000);
        } else {
            // Anything you want to say 'Bad user!'
            e.preventDefault();
        }
    });
  
  
