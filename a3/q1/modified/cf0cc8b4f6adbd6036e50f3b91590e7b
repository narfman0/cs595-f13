  var page = 0;
  var allPostLoaded = false;
  var isVisibleMoreContentByDefault = false;

  $(window).scroll(function() {
     if($(window).scrollTop() + $(window).height() == $(document).height()) {
          if (!allPostLoaded) {
            jQuery('#loadingPost').show() ;
            filterPostBy();
          }
     }
  });

