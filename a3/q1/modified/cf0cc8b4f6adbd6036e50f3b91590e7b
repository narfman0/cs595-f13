<script src="/static/ZNI029XkYl1yHwkNkeOyiYJ3Xkh0sWZGdOQplkogGhP.js" type="text/javascript" ></script>
<script type="text/javascript">
var selectedPostIndex = 0;
var  postId=0;


jQuery('.show_badge').live('click', function() {
var parent = jQuery(this).attr('topFan');
var action =  jQuery(this).find('span').text();
if (action == 'less') {
jQuery("#" + parent).removeClass('pink_block');
jQuery(this).find('span').text('more');
jQuery(this).children('img').attr('src', '/static/kmcwkCFlzRcWNsnvkhAHEcDRmZt1B2zspSSh4hul8Ek.png');
          jQuery(this).siblings('.badge_container').find('.hidden_badge').hide();
      } else {
          jQuery("#" + parent).addClass('pink_block');
          jQuery(this).find('span').text('less');
          jQuery(this).children('img').attr('src', '/static/GQp5XDddDRKinsksu6DkC79I8SyRWAUGZ4M5xwFxEaR.png');
          jQuery(this).siblings('.badge_container').find('.hidden_badge').show();
      }
  });

  jQuery('.top_share .icon_like').live("click", function () {
    var parent = jQuery(this).parents('.top_share');
    postId = parent.attr('postId');
    if (postId) {
      if (true) {
jQuery.ajax({type:'POST',data:{'actorId': '32'}, url:'/actor/showCalculateHeartbeatsOverlay',success:function(data,textStatus){jQuery('#overlayContainer').html(data);},error:function(XMLHttpRequest,textStatus,errorThrown){}});
} else {
jQuery.ajax({
url:"/actor/likePost",
              type:"POST",
              data:{postId:postId},
              success:function (responseJSON) {
                var postBeat = responseJSON.postBeat;
                jQuery(document.getElementById('post_' + postId)).find('.post_beat span.post_beat_count').text(postBeat);
                jQuery(document.getElementById('topPost_' + postId)).find('span.post_beat_count').text(postBeat);
                jQuery(document.getElementById('post_' + postId)).find('.post_share .icon_like').toggleClass('selected');
                jQuery(document.getElementById('topPost_' + postId)).find('.icon_like').toggleClass('selected');
              }
        });
      }
    } else {
      $('#post_ .accounts').slideToggle('medium');
    }
  });

jQuery(".top_share_bounce_container .close a, .actor_info .prev, .actor_info .next").click(function() {
  jQuery('.top_share_bounce_container .accounts').slideUp('medium') ;
});

jQuery(".top_share_bounce_container .accounts .bounced").click(function () {
    var postId = jQuery(".top_share").attr('postId');
    var link = location.href;
    var fanId;
    if (!postId) {
       link = 'https://www.buddybounce.com/OneDirection/leaderBoard'
       fanId =  jQuery(".top_share").attr('fanId');
    }

    var providerId = $(this).attr('provider');

    window.open(link, "displayWindow", "width=520,height=300,left=350,top=170,status=no,toolbar=no,menubar=no");

    $.ajax({
      type: "POST",
      url:"/actor/shareContent",
      data: {actorName: 'One Direction',  type: 'shareTopContent', fanId: fanId,  postId: postId,  providerId: providerId, link: link},
      success: function(url) {
         window.open(url, "displayWindow", "width=520,height=300,left=350,top=170,status=no,toolbar=no,menubar=no");
      }
    });
});

  jQuery(".post_share .accounts.bounce .bounced").live("click", function () {
    sharePost($('.post:eq(' + selectedPostIndex + ')').attr('postId'), $(this).attr('provider'))
  });

  jQuery(".post_share .icon_like").live("click", function () {
     selectedPostIndex = $(".post_share  .icon_like").index(this);
     postId = $('.post:eq(' + selectedPostIndex + ')').attr('postId');
     iconLikeContainer = jQuery(this);
    jQuery.ajax({
      url:"/actor/likePost",
      type:"POST",
      data:{postId:postId},
      success:function (responseJSON) {
        if (!responseJSON.isUserFanOfActor) {
jQuery.ajax({type:'POST',data:'postId='+postId+'&actorId=32', url:'/actor/showCalculateHeartbeatsOverlay',success:function(data,textStatus){jQuery('#overlayContainer').html(data);},error:function(XMLHttpRequest,textStatus,errorThrown){}});
} else {
        var postBeat = responseJSON.postBeat;
        var like = responseJSON.like;
        var likeImageSrc;

      if (like){
        iconLikeContainer.addClass('selected');
      } else {
         iconLikeContainer.removeClass('selected');
      }
      jQuery(document.getElementById('topPost_' + postId)).find('.icon_like').toggleClass('selected');
      $('.post_share .icon_like:eq(' + selectedPostIndex + ')').find('img').attr('src', likeImageSrc);
      $('.post:eq(' + selectedPostIndex + ')').find('.post_beat_count').text(postBeat);
      jQuery(document.getElementById('topPost_' + postId)).find("span.post_beat_count").text(postBeat);
    }
  }
});

jQuery(".post_share .close a").live("click", function() {
 jQuery('.post_share .accounts.bounce:eq(' + selectedPostIndex + ')').slideUp('medium') ;
});
});

function sharePost(postId, providerId){
window.open("/actor/sharePost?postId="+postId+"&providerId="+providerId, "displayWindow",
        "width=520,height=400,left=350,top=170,status=no,toolbar=no,menubar=no");
  }

  jQuery(".photo, .message_photo, .video, .audio").live('click', function () {
      var postId = jQuery(this).attr('postId');
jQuery.ajax({type:'POST',data:'postId='+postId, url:'/actor/updatePostOverlay',success:function(data,textStatus){jQuery('#postContainer').html(data);},error:function(XMLHttpRequest,textStatus,errorThrown){}});
;
});


jQuery('.icon_skip_post').live('click', function() {
 var postId = jQuery(this).attr('postId');
 jQuery(document.getElementById('confirmSkip' + postId)).slideToggle('medium');
});

jQuery('.close_skip_cofirm a, .skip_confirm_buttons a.cancel').live('click', function() {
var postId = jQuery(this).attr('postId');
jQuery(document.getElementById('confirmSkip' + postId)).slideUp('medium');
});

jQuery(".report_content").live("click", function() {
jQuery("#reportPostId").val(postId);
  var top = f_scrollTop() + 50 + 'px';
  jQuery('#reportContentDialog').css('top', top);
jQuery("#reportContentDialog").jqmShow();
jQuery(this).parents('.accounts').slideUp('medium');
});

jQuery('.click_more_less').live('click', function() {
var text = jQuery(this).children('span').text().trim();
var postId = jQuery(this).attr('postId');
if (text == "more") {
jQuery(document.getElementById('post_' + postId)).css('z-index','5');
jQuery(this).children('span').text("less");
jQuery(this).children('img').attr('src', '/static/kmcwkCFlzRcWNsnvkhAHEcDRmZt1B2zspSSh4hul8Ek.png');
     jQuery(this).siblings("span.tree_dot").hide();
     jQuery(this).siblings("span.message_end").css("display", "inline")
    } else {
     jQuery(document.getElementById('post_' + postId)).css('z-index','1');
     jQuery(this).children('span').text('more');
     jQuery(this).children('img').attr('src','/static/kmcwkCFlzRcWNsnvkhAHEcDRmZt1B2zspSSh4hul8Ek.png');
     jQuery(this).siblings("span.tree_dot").show();
     jQuery(this).siblings("span.message_end").css("display", "none")
    }
  });

   jQuery(".post_rebounce .icon_rebounce").live("mouseover", function () {
      var rebouncedOverlay =  jQuery(this).parent().children('.accounts');
      rebouncedOverlay.slideDown('medium');
   });

   jQuery(".top_share .icon_rebounce").live("mouseover", function () {
      var id = jQuery(this).attr('postId');
      jQuery('.top_share_bounce_container .accounts .bounce_icon_overlay').attr('postId', id);
      if (jQuery(this).hasClass('selected')) {
          jQuery('.top_share_bounce_container .accounts .bounce_icon_overlay').hide();
      } else {
          jQuery('.top_share_bounce_container .accounts .bounce_icon_overlay').show();
      }
      jQuery('.top_share_bounce_container .accounts').slideDown('medium');
   });

   jQuery(".top_share_bounce_container .accounts,.carousel_holder").live("mouseleave", function () {
      jQuery('.top_share_bounce_container .accounts').slideUp('medium');
   });

   $('.post_rebounce').live("mouseleave", function () {
      var rebouncedOverlay =  jQuery(this).children('.accounts');
      rebouncedOverlay.slideUp('medium');
   });

   jQuery(".post_rebounce .close a").live("click", function() {
      var accountsOverlay =  jQuery(this).parent().parent();
      accountsOverlay.slideUp('medium') ;
   });

   jQuery(".post_rebounce .icon_rebounce.grey").live("click", function () {
      postId = jQuery(this).attr('postId');
      var rebouncedOverlay =  jQuery(this).parent().children('.accounts');
      rebounce(rebouncedOverlay, postId, jQuery(this));
   });

   jQuery(".top_share .icon_rebounce.grey").live("click", function() {
     postId = jQuery(this).attr('postId');
     var rebouncedOverlay = jQuery('.top_share_bounce_container .accounts');
     rebounce(rebouncedOverlay, postId, jQuery(this));
     jQuery('.top_share_bounce_container .accounts').slideUp('medium');
   });

   jQuery(".bounce_icon_overlay").live("click", function () {
     postId = jQuery(this).attr('postId');
     var rebouncedOverlay = jQuery(this).parents('.accounts');
     rebounce(rebouncedOverlay, postId, jQuery(this));
   });

   function rebounce(rebouncedOverlay, postId, selectedAction) {
      jQuery.ajax({
          url:"/actor/twitterExpired",
          type:"POST",
          data:{showAlert: false},
          success: function(responseJSON) {
              if (responseJSON.twitterExpired) {
jQuery.ajax({type:'POST',data:{'showAlert': 'true'}, url:'/actor/twitterExpired',success:function(data,textStatus){jQuery('#overlayContainer').html(data);},error:function(XMLHttpRequest,textStatus,errorThrown){}});
} else {
      jQuery('.post_overlay_header .bounce_icon_overlay').hide();

      if (selectedAction.hasClass('bounce_icon_overlay')) {
          if (!selectedAction.parents('.post_rebounce').children('.icon_rebounce.big').hasClass('selected')) {
             selectedAction.parents('.post_rebounce').children('.icon_rebounce.big').toggleClass("selected grey");
          }

          selectedAction.hide();
      }  else {
         selectedAction.toggleClass('selected grey');
      }

     if (!jQuery(document.getElementById('post_' + postId)).find('.icon_rebounce').hasClass('selected')) {
       jQuery(document.getElementById('post_' + postId)).find('.icon_rebounce').toggleClass('selected grey');
     }
     if (!jQuery(document.getElementById('topPost_' + postId)).find('.icon_rebounce').hasClass('selected')) {
       jQuery(document.getElementById('topPost_' + postId)).find('.icon_rebounce').toggleClass('selected grey');
     }
     rebouncedOverlay.find('.items .rebounced.selected').each(function(){
       rebouncePost($(this).attr('provider'));
     });
     rebouncedOverlay.slideUp('medium');
     jQuery(document.getElementById('post_' + postId)).find('.bounce_icon_overlay').hide();
     jQuery(document.getElementById('topPost_' + postId)).find('.bounce_icon_overlay').hide();

  }
}
})

}

function rebouncePost(providerId) {
jQuery.ajax({
url:"/actor/rebouncePost",
            type:"POST",
            data:{postId:postId, providerId: providerId},
            success:function (responseJSON) {
                 if (!responseJSON.isCurrentUserPost) {
                   $(document.getElementById('post_' + postId)).find('.post_like_count').text(responseJSON.rebounceCount);
                   $('.post_overlay_header span.post_like_count').text(responseJSON.rebounceCount);
                   $(document.getElementById('topPost_' + postId)).find('span.post_like_count').text(responseJSON.rebounceCount);
                   if (jQuery('#bouncedBy').is(':hidden')) {
                     jQuery('#bouncedBy').show();
                   }
                   jQuery.ajax({
                     url:"/actor/addBouncedPostFan",
                     type:"POST",
                     data:{postId: postId, mode: 'bounce'},
                     success:function (html) {
                       jQuery("#bouncedByContainer").append(html);
                     }
                   });
                 }
            }
          })
   }

