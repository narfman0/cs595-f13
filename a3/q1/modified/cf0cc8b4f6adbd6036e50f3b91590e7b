        $.ajax({
          type: "POST",
          url:"/actor/shareContent",
              data: {active: 'overview', actorName: 'One Direction', type: 'sharePage', providerId: providerId, link: url},
              success: function(url) {
                window.open(url, "displayWindow", "width=520,height=300,left=350,top=170,status=no,toolbar=no,menubar=no");
              }
        });
    });

