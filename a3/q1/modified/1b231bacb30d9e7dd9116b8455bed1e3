						<script type="text/javascript">
						function updateComments() {
							//alert('Error: ' + Comment.error);
							var refreshURL = "http://www.philly.com/philly/blogs/big_tent/225110872.html?c=0.07872710835698216&posted=y&viewAll=y#comments";
							if(Comment.comment != "") {
							//alert('comment flagged ' + Comment.flagged); 
								if(Comment.flagged) {
									refreshURL = "http://www.philly.com/philly/blogs/big_tent/225110872.html?c=0.8393250374897482&posted=n#comments"
								}
