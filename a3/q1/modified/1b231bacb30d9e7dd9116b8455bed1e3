										document.write('<div class=\'np-link\'> <b>Member Login:</b></div><div class=\'np-link\'> <a rel="nofollow" href="/s?action=login&requested=y&rurl=http%3A%2F%2Fwww.philly.com%2Fphilly%2Fblogs%2Fbig_tent%2F225110872.html" id="signinlink" onClick="var s=s_gi(\'phillycom\');s.tl(this,\'o\',\' TopNav: Right: Sign In\');"> Sign In</a> |</div><div class=\'np-link\'> <a rel="nofollow" href="/s?action=reg&requested=y&rurl=http%3A%2F%2Fwww.philly.com%2Fphilly%2Fblogs%2Fbig_tent%2F225110872.html" id="registerlink" onClick="var s=s_gi(\'phillycom\');s.tl(this,\'o\',\' TopNav: Right: Register\');">Register</a></div>');
